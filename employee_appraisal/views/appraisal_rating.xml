<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <!-- ************** BSSCL appraisal rating model action ************** -->
        <record id="appraisal_rating_action_id" model="ir.actions.act_window">
            <field name="name">Appraisal Rating / मूल्यांकन रेटिंग</field>
            <field name="res_model">appraisal.rating</field>
            <field name="view_mode">tree</field>
        </record>

        <!-- ************** BSSCL appraisal rating model Tree View ************** -->
        <record id="appraisal_rating_view_tree" model="ir.ui.view">
            <field name="name">Appraisal Rating View Tree</field>
            <field name="model">appraisal.rating</field>
            <field name="arch" type="xml">
                <tree string="Appraisal Rating Tree View" editable="bottom">
                    <field name="from_int"/>
                    <field name="to_int"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
    
        
    
    </data>
    

</odoo>
