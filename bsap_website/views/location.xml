<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="form_website_visitor_inherit" model="ir.ui.view">
            <field name="name">website.visitor.form.inherit</field>
            <field name="model">website.visitor</field>
            <field name="inherit_id" ref="website.website_visitor_view_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='last_connection_datetime']" position="after">
                        <field name="lat"/>
                        <field name="lng"/>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>