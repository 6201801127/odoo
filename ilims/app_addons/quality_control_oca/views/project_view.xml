<odoo>

    <record id="project_project_inherit_form_views_quality" model="ir.ui.view">
        <field name="name">project.project.form.view.quality.inherit</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="gts_acceptance_management.project_project_inherit_form_views_acceptance"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='open_acceptance']" position="after">
                <button class="oe_stat_button" name="open_quality_control" type="object" icon="fa-list-alt"
                        groups="quality_control_oca.group_quality_control_user,quality_control_oca.group_quality_control_manager,gts_groups.group_quality_management_can_create,gts_groups.group_quality_management_can_view">
                    <field string="Quality Control" name="quality_control_count" widget="statinfo"/>
                </button>
            </xpath>
        </field>
    </record>

</odoo>
