<odoo>
    <data noupdate="1">
        <record id="scheduled_pwa_notifications_cron_action" model="ir.cron">
            <field name="name">Scheduled PWA Push Notifications</field>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="model_id" ref="model_pwa_push_notifications"/>
            <field name="code">model.send_pwa_scheduled_notifications()</field>
        </record>
    </data>
</odoo>
