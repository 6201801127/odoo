<odoo>
    <data noupdate="1">
        <record id="website.default_website" model="website">
            <field name="pwa_name">Demo PWA Website</field>
            <field name="pwa_short_name">Demo PWA</field>
            <field name="pwa_start_url">/</field>
            <field name="pwa_bk_color">#FFFFFF</field>
            <field name="pwa_theme_color">#808000</field>
            <field name="pwa_icon" type="base64" file="odoo_ecommerce_pwa/static/src/img/launcher_512.png"/>
        </record>

        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_server_key', 'dummy')"/>
        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_api_key', 'dummy')"/>
        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_sender_id', 'dummy')"/>
        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_auth_domain', 'dummy')"/>
        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_database_url', 'dummy')"/>
        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_project_id', 'dummy')"/>
        <function model="ir.default" name="set" eval="('res.config.settings', 'pwa_firebase_app_id', 'dummy')"/>
    </data>
</odoo>
