<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">

		<!-- Sequences for risk type -->
		<record id="seq_risk_type" model="ir.sequence">
			<field name="name">code</field>
			<field name="code">seq.sh_risk_type</field>
			<field name="prefix">RT</field>
			<field name="padding">3</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="project_category_sequence" model="ir.sequence">
            <field name="name">project Sequence</field>
            <field name="code">project.category.sequence</field>
            <field name="prefix">PMS1</field>
            <field name="padding">4</field>
			<field name="company_id" eval="False"/>
        </record>
	</data>
</odoo>