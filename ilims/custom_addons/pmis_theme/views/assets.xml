<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
		<template id="assets_backend" name="PMIS Backend Theme" inherit_id="web.assets_backend">
		<xpath expr=".">
                <link rel="stylesheet" href="/pmis_theme/static/src/scss/colors.scss"/>
                <link rel="stylesheet" href="/pmis_theme/static/src/scss/style.scss"/>
				<link rel="stylesheet" href="/pmis_theme/static/src/scss/sidebar.scss"/>
				<script type="application/javascript" src="/pmis_theme/static/src/js/sidebar.js"/>
				<script type="application/javascript" src="/pmis_theme/static/src/js/sidebar-toggle.js"/>
			</xpath>
		</template>

		<template id="assets_frontend" name="PMIS Frontend Theme" inherit_id="web.assets_frontend">
			<xpath expr=".">
                <link rel="stylesheet" href="/pmis_theme/static/src/css/pmis_style.css"/>
			</xpath>
		</template>
    </data>
</odoo>
