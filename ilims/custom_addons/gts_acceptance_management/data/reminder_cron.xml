<odoo>

    <record id="online_sync_cron_acceptance_approve_reminder" model="ir.cron">
        <field name="name">Acceptance Approval Reminder</field>
        <field name="model_id" ref="gts_acceptance_management.model_acceptance_inspection"/>
        <field name="state">code</field>
        <field name="code">model.acceptance_activity_reminder()</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
    </record>

</odoo>
