<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_categorys_tree" model="ir.ui.view">
            <field name="name">category.tree</field>
            <field name="model">category</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="category">
                    <field name="category_name"/>
                </tree>
            </field>
        </record>

        <record id="view_categorys_form" model="ir.ui.view">
            <field name="name">category.form</field>
            <field name="model">category</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Test">
                    <sheet>
                        <group>
                            <field name="category_name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_categorys_view">
            <field name="name">Category</field>
            <field name="res_model">category</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_empty_folder">
                    No category(s) found.
                </p>
            </field>
        </record>
    </data>
</odoo>