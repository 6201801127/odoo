<odoo>

    <record id="view_project_project_inherit_cost_analysis" model="ir.ui.view">
        <field name="name">project.project.form.views.inherit.cost</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="om_account_budget.view_project_project_form_inherit_budget"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='open_budgets']" position="after">
                <button class="oe_stat_button" name="open_analytics" type="object" icon="fa-dollar"
                        groups="gts_groups.group_project_costing_can_view">
                    <field string="Cost Analysis" name="cost_analysis" widget="statinfo"/>
                </button>
            </xpath>
        </field>
    </record>

</odoo>
