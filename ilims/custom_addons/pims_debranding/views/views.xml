<odoo>
	<template id="debrand_assets_backend" name="DPS De-branding" inherit_id="web.assets_backend">
		<xpath expr="." position="inside">
			<script src="/pims_debranding/static/src/js/title.js"/>
		</xpath>
	</template>
	<template id="brand_promo_extend" inherit_id="web.brand_promotion">
		<xpath expr="//div[hasclass('o_brand_promotion')]" position="replace">
		</xpath>
	</template>
	<template id="web_layout_extend" inherit_id="web.layout">
        <xpath position="replace" expr="//title">
			<title t-esc="title or 'PMIS'"/>
		</xpath>
		<xpath expr="//head" position="replace">
            <head>
                <meta charset="utf-8"/>
                <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
                <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"/>

                <title t-esc="title or 'PMIS'"/>
                <link type="image/x-icon" rel="shortcut icon" t-att-href="x_icon or '/pims_debranding/static/src/img/logo.png'"/>

                <script type="text/javascript">
                    var odoo = {
                    csrf_token: "<t t-esc="request.csrf_token(None)"/>",
                    debug: "<t t-esc="debug"/>",
                    };
                </script>

                <t t-raw="head or ''"/>
            </head>
        </xpath>
	</template>
	<!-- Debrand Assets -->
	<!--Modify brand promotion details-->
	
</odoo>
