<?xml version="1.0"?>
<odoo>
    <template id="assets_backend" name="support assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/gts_ticket_management/static/src/scss/support_ticket.scss"/>
            <link rel="stylesheet" href="/gts_ticket_management/static/src/css/portal_support.css"/>
            <script type="text/javascript" src="/gts_ticket_management/static/src/js/support_ticket_dashboard.js"/>
        </xpath>
    </template>

    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Website Support">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/gts_ticket_management/static/src/js/website_support.menu.js"/>
        </xpath>
    </template>
</odoo>
