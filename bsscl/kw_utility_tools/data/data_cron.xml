<odoo>
    <data noupdate="1">

        <record id="cron_kw_whatsapp_message_notification" model="ir.cron">
            <field name="name">Send WhatsApp Notification </field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <!-- <field name="doal">0</field> -->
            <field name="numbercall">-1</field>
            <!-- <field name="nextcall" >2019-11-26 12:59:59</field> -->

            <field name="model_id" ref="model_kw_whatsapp_message_log"/>
            <field name="code">model.call_whatsapp_send_api()</field>
            <field name="state">code</field>

          
        </record>


    </data>
</odoo>