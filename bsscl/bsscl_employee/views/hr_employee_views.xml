        <record model="ir.ui.view" id="hr_employee_private_inherited">
            <field name="name">hr.employee.form</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <header>
                    <button name="action_update_administrative_services" string="Update Administrative Services /   प्रशासनिक सेवाओं को अपडेट करें" type="object" attrs="{'invisible':[('employee_type','!=','regular')]}" class="btn-primary"/>

                </header>
                <xpath expr="//page[@name='personal_information']" position="replace">
                    <field name="validate_details" invisible='1'/>
                    <page name="personal_information" attrs="{'invisible':[('validate_details','=',False)]}" string="Personal Information / व्यक्तिगत जानकारी
                    ">
                        <field name="employee_type" invisible="1"/>
                        <field name="recruitment_type" invisible="1"/>
                        <group>
                            <group string="Citizenship &amp; Other Information / नागरिकता और अन्य जानकारी">
                                <field name="country_id" string="Nationality (Country) / राष्ट्रीयता (देश)" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/>
                                <field name="bank_account_id" string="Bank Account Number / बैंक खाता संख्या"/>
                               
                                <field name="citizen_file_name" invisible="1"/>
                                <field name="show_citizen_field" invisible="1"/>
                                <field name="bank_account_id" string="Bank Account Number / बैंक खाता संख्या" required='1'/>
                                <field name="partner_id" invisible="1"/>
                                <field name="bank_name" invisible="1"/>
                                <field name="bank_account_number" invisible="1"/>
                                <field name="ifsc_code" invisible="1"/>
                            </group>
                            <group string="Status / दर्जा">
                                <field name="gender" string='Gender' invisible="1"/>
                                <field name="gende" string='Gender / लिंग' required="1"/>
                                <field name="marital" invisible="1"/>
                                <field name="spouse_complete_name" invisible = "True"/>
                                <field name="spouse_birthdate" invisible = "True"/>
                                <field name="children" invisible="True"/>
                            </group>
                            <group string="Contact Information / संपर्क जानकारी">
                                <field name="address_home_id" invisible="1" context="{'show_address': 1,'default_type': 'private','form_view_ref': 'base.res_partner_view_form_private'}" options="{&quot;always_reload&quot;: True, &quot;highlight_first_line&quot;: True}"/>
                                <field name="is_address_home_a_company" invisible="1"/>
                               
                                <field name="emergency_contact" string="Emergency Contact / आपातकालीन संपर्क" placeholder="Address / पता"/>                             <field name="emergency_phone" string="Emergency Phone / आपातकालीन फ़ोन"/>
                                <field name="personal_email" string='Personal Email / व्यक्तिगत ईमेल'/>
                                <field name="my_home_phone" string="Phone(Home) / घर का फोन"/>
                            </group>
                            <group name="identify_id" string="Identification Documents / पहचान दस्तावेज">
                                <field name="identification_id" groups="hr.group_hr_user" invisible="True"/>
                                <field name="uan_no" string="UAN No. / यूएएन नंबर"/>
                                <field name="pan_no" string="PAN Card No. / पैन कार्ड संख्या" required="1"/>
                                <field name="pan_upload" filename="pan_upload_filename" string="Upload(PAN) / अपलोड (पैन)" attrs="{'required': [('pan_no','!=',False)]}"/>
                                <field name="pan_upload_filename" invisible="1"/>
                                <field name="aadhar_no" required="False" string="Aadhaar Card No. / आधार कार्ड संख्या"/>
                                <field name="aadhar_upload" filename="aadhar_upload_filename" string="Upload(Aadhaar) / अपलोड (आधार)" attrs="{'required': [('aadhar_no','!=',False)]}"/>
                                <field name="aadhar_upload_filename" invisible="1"/>
                                <field name="passport_id" string = "Passport No. / पासपोर्ट संख्या" groups="hr.group_hr_user"/>
                                <field name="passport_upload" filename="passport_upload_filename" string="Upload(Passport) / अपलोड (पासपोर्ट)" attrs="{'required': [('passport_id','!=',False)]}"/>
                                <field name="passport_upload_filename" invisible="1"/>
                            </group>
                            <group string="Birth / जन्म">
                                <field name="birthday" required="1" string="Date of Birth / जन्म की तारीख"/>
                                <field name="place_of_birth" string="Place of Birth / जन्म स्थान"/>
                                <field name="country_of_birth" string="Country of Birth / जन्म का देश"/>
                            </group>
                            <group string="">
                                <field name="religion" required="True" string="Religion / धर्म" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/>
                                <field name="category" required="True" string="Category / वर्ग" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/>
                                 <field name="minority"  string="Minority / अल्पसंख्यक" attrs="{'invisible':[('minority_bool', '=', True)]}"/>
                                <field name="minority_bool" invisible="1"/>
                            </group>
                            <group string="Physical / शारीरिक">
                                <field name="height" string="Height / लंबाई" />
                                <field name="weight" string="Weight / वज़न"/>
                                <field name="blood_group" required="False" string="Blood Group / ब्लड ग्रुप"/>
                              
                            </group>
                            <group string="Work Permit" name="work_permit" invisible = "True">
                                <field name="visa_no" invisible = "True"/>
                                <field name="permit_no" invisible = "True"/>
                                <field name="visa_expire" invisible = "True"/>
                            </group>
                            <group string="Education / शिक्षा">
                                <field name="certificate" string="Certificate Level / प्रमाणपत्र स्तर"/>
                                <field name="study_field" string="Field of Study / अध्ययन क्षेत्र"/>
                                <field name="study_school" string="School / विद्यालय"/>
                            </group>
                        </group>
                       
                    </page>
                </xpath>

                <!--***************** Administrative Services History details ************************ -->
                <xpath expr="//page[@name='hr_settings']" position="after">
                    <page name="administrative_history" string="Administrative Services History / प्रशासनिक सेवा इतिहास">
                        <field name="administrative_task_ids" widget='one2many'>
                            <tree create="false">
                                <field name="task_type_id" />
                                <field name="current_work_location"/>
                                <field name="order_no" />
                                <field name="order_date" />
                                <field name="remarks" />
                            </tree>                       
                        </field>
                    </page> 
                    <field name="show_handbook" invisible="1"/>
					<page string="Organisational Policies / संगठनात्मक नीतियाँ" attrs="{'invisible': [('show_handbook', '=', False)]}">
						<group>
							<field name="handbook_info_details_ids" readonly="1">
								<tree>
									<field name="title"/>
									<field name="handbook_type_id"/>
									<field name="understood"/>
									<field name="date"/>
								</tree>
							</field>
						</group>
					</page>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="hr_employee_hr_setting_view">
            <field name="name">hr.employee.hr.setting.view</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">            
                      
                <xpath expr="//notebook/page[@name='hr_settings']/group/group/field[@name='user_id']" position="after">                   
                    <field name="current_office_id" />
                    <field name="state"/>
                    <field name="mid_year_factor" invisible='1'/>
                    <field name="work_shifts" string="Working Shifts / काम की शिफ्ट" />
                </xpath>       

            </field>
        </record>

        <record model="ir.ui.view" id="hr_employee_form_branch_inherited">
            <field name="name">hr.employee.form</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/header/button" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <field name="image_1920" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <xpath expr="//div/h1/field[@name='name']" position="attributes">
                    <attribute name="attrs">{'required': 0}</attribute>
                </xpath>
                <xpath expr="//div/h1/field[@name='name']" position="replace">
                    
                    <div style="margin-right: 15px;">
                        <span>

                            <field name="image_1920" widget="image" class="oe_avatar" options="{&quot;zoom&quot;: true, &quot;preview_image&quot;:&quot;image_128&quot;}"/>
                        </span>

                    </div>
                    <div>
                        <label for="name" string='Title / उपाधि'  class="oe_edit_only"/><br/>
                        <field name="salutation" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/><br/>
                        <field name="name" readonly='1' required='0'/>

                    </div>

                </xpath>


                <field name="category_ids" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="job_title" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="mobile_phone" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="work_phone" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="work_email" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="department_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="company_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="parent_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="coach_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <xpath expr="//notebook" position="before">
                    <group>
                        <group>
                            <field name="first_name" string="First Name / पहला नाम" required='1'/>

                            <field name="middle_name"/>
                            <field name="last_name"/>
                            
                        </group>
                        <group>
                            <field name="employee_type" string="Employment type / रोजगार के प्रकार"/>
                            <field name="identify_id" string="Identification No. / पहचान क्रमांक"/>
                            <field name="recruitment_type" string="Recruitment Type / भर्ती प्रकार"/>
                        </group>
                    </group>
                </xpath>
                <xpath expr="//field[@name='department_id']" position="attributes">
                    <attribute name="options">{'no_create': True, 'no_open': True, 'no_create_edit': True}</attribute>
                </xpath>
                <xpath expr="//field[@name='job_id']" position="attributes">
                    <attribute name="options">{'no_create': True, 'no_open': True, 'no_create_edit': True}</attribute>
                </xpath>
                <xpath expr="//field[@name='parent_id']" position="attributes">
                    <attribute name="options">{'no_create': True, 'no_open': True, 'no_create_edit': True}</attribute>
                </xpath>

                <xpath expr="//field[@name='address_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//page[@name='hr_settings']/group/group[2]" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//page[@name='hr_settings']/group/group[1]" position="attributes">
                    <attribute name="string">status / दर्जा</attribute>
                </xpath>
                <xpath expr="//page[@name='public']" position="attributes">
                    <attribute name="string">Work Information / कार्य की जानकारी</attribute>
                </xpath>
                <xpath expr="//page[@name='public']/div/div/group[1]" position="attributes">
                    <attribute name="string">Contact Information / संपर्क जानकारी
                   </attribute>
                </xpath>
                <xpath expr="//page[@name='hr_settings']/group/group[@name='active_group']/field[@name='first_contract_date']" position="attributes">
                    <attribute name="string">First Contract Date / पहले अनुबंध की तारीख</attribute>
                </xpath>
                <xpath expr="//field[@name='user_id']" position="attributes">
                    <attribute name="string">Related User / संबंधित उपयोगकर्ता</attribute>
                </xpath>
                <xpath expr="//field[@name='job_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//page[@name='hr_settings']" position="attributes">
                    <attribute name="string">HR Settings / एचआर सेटिंग</attribute>
                </xpath>
                <xpath expr="//page[@name='public']/div/div/group[2]" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//page[@name='public']/div/div/group[4]" position="attributes">
                    <attribute name="string">Position / पद</attribute>
                </xpath>
                <xpath expr="//page[@name='public']/div/div/group[1]" position="inside">
                    <field name="work_email" string="Work Email / कार्य ईमेल"/>
                    <field name="mobile_phone" string="Phone Number / फोन नंबर"/>
                    <field name="work_phone" string="Telephone Number / टेलीफोन नंबर"/>
                    <field name="ex_serviceman" string="Whether Ex Service Man / क्या आप एक्स सर्विस मैन हैं" required="True"/>
                    <field name="transfer_date" string="Joining Date / कार्यारंभ तारीख" />
                </xpath>
                <xpath expr="//field[@name='work_location']" position="attributes">
                    <attribute name="string">Work Location / कार्य स्थल</attribute>
                </xpath>
                <xpath expr="//field[@name='tz']" position="attributes">
                    <attribute name="string">Timezone / समय क्षेत्र</attribute>
                </xpath>
                <xpath expr="//field[@name='resource_calendar_id']" position="attributes">
                    <attribute name="resource_calendar_id">Working Hours / कार्य-समय</attribute>
                    <attribute name="options">{'no_create': True, 'no_open': True, 'no_create_edit': True}</attribute>

                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[1]/field[@name='bank_account_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                
                
                <xpath expr="//page[@name='public']/div/div/group[4]" position="inside">
                    <field name="department_id" string="Department / विभाग" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}" required='1'/>
                    <field name="parent_id" string="Manager / प्रबंधक" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/>
                    <field name="job_id" string="Post / पद" required='1'/>

                </xpath>
                

                <xpath expr="//page[@name='personal_information']" position="after">
                    <page string="Address / पता" attrs="{'invisible':[('validate_details','=',False)]}">
                        <field name="address_ids">
                            <tree editable="bottom" string="Address">
                                <field name="address_type" string="Address Type / पता मुद्रलेख"/>
                                <field name="street" string="Address Line1 / पता पंक्ति 1"/>
                                <field name="street2" string="Address Line2 / पता पंक्ति 2"/>
                                <field name="country_id" string="country / देश"/>
                                <field name="state_id" string="state / राज्य"/>
                                <field name="city_id" string="city/District / शहर/ज़िला"/>
                                <field name="zip" string="Pin / पिन"/>
                                <field name="is_correspondence_address" invisible='1'/>
                            </tree>
                            <form string="Address">
                                <group>
                                    <group>
                                        <field name="street"/>
                                        <field name="street2"/>
                                        <field name="city_id"/>
                                    </group>
                                    <group>
                                        <field name="state_id" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/>
                                        <field name="country_id" options="{'no_create': True, 'no_open': True, 'no_create_edit': True}"/>
                                        <field name="zip"/>
                                    </group>
                                </group>
                            </form>
                        </field>
                    </page>
                </xpath>
                <xpath expr="//page[@name='hr_settings']" position="after">
                    <page name="rewards" string="Award &amp; Punishment/ पुरस्कार और सजा">
                        <label for="rewards_details_ids" string="Reward Details / पुरस्कार विवरण" />
                        <field name="rewards_details_ids" string="Rewards / पुरस्कार"/>
                        <label for="punishment_ids" string="Punishment Details / सजा विवरण" />
                        <field name="punishment_ids" string="Punishment / सजा">
                            <tree editable="top" string="Punishment" >
                                <field name="given_by" width='250px' required='1'/>
                                <field name="given_date" width='250px'/>
                                <field name="approve_date" width='250px'/>
                                <field name="reason" width='250px'/>
                            </tree>
                        </field>
                    </page>
                </xpath>

                <xpath expr="//page[@name='rewards']" position="after">
                    <page name="Pension" string="Pension / पेंशन" >
                        <field name="pension_ids">
                            <tree editable="top" string="Pension">
                                <field name="start_date" width='350px'/>
                                <field name="end_date" width='250px'/>
                                <field name="approved_by" width='250px' required='1'/>
                                <field name="amount" width='350px'/>
                            </tree>
                            
                        </field>
                    </page>
                </xpath>

                

                <div class="oe_button_box" position="inside">
                    <field name="validate_details" invisible='1'/>
                    <button attrs="{'invisible':[('validate_details','=',False)]}" class="oe_stat_button" name="document_view" type="object" icon="fa-list-ol">
                        <field string="Documents / दस्तावेज़" name="document_count" widget="statinfo"/>
                    </button>

                    <button  name="%(action_emp_relatives_list)d" type="action" class="oe_inline oe_stat_button" icon="fa-users" help="Add/Edit/Review Relative details of employee" attrs="{'invisible':[('validate_details','=',False)]}" context = "{'default_employee_id': id}">
                        <field name="no_of_relative" widget="statinfo" string="Family/Dependents / परिवार/आश्रित
                        " />
                    </button>
                    <button  name="%(action_emp_vrs_action)d" type="action" class="oe_inline oe_stat_button" icon="fa-users" help="Add/Edit/Review Relative details of employee" attrs="{'invisible':[('validate_details','=',False)]}" context = "{'default_employee_id': id}">
                        <field name="vrs_retirement" widget="statinfo" string="VRS Retirement / वीआरएस सेवानिवृत्ति
                        " />
                    </button>
                    <button  name="apply_change_request" type="object" class="oe_inline oe_stat_button" icon="fa-users" help="Add/Edit/Review Relative details of employee" attrs="{'invisible':[('validate_details','=',False)]}" context = "{'default_employee_id': id}">
                        <field name="change_request" widget="statinfo" string="Change Request / अनुरोध बदलो

                        " />
                    </button>

                     <button  name="%(action_legal_issue_action)d" type="action" class="oe_inline oe_stat_button" icon="fa-legal"
                              context = "{'default_employee_id': id}"
                              >
                        <field name="legal_request" widget="statinfo" string="Legal Issues / कानूनी मुद्दों

                        " />
                    </button>


                    <button attrs="{'invisible':[('validate_details','=',False)]}" name="%(action_emp_education_list)d" type="action" style="width:197px !important" class="oe_inline oe_stat_button" icon="fa-book" help="Add/Edit/Review Education details of employee">
                        <field name="no_of_education" widget="statinfo" string="Education Details / शिक्षा विवरण
                        " />
                    </button>

                    <button attrs="{'invisible':[('validate_details','=',False)]}" name="%(action_emp_languages_list)d" type="action" class="oe_inline oe_stat_button" icon="fa-font" help="Add/Edit/Review Languages of employee">
                        <field name="no_of_lang" widget="statinfo" string="Languages / भाषा" />
                    </button>
                </div>
                <xpath expr="//form/sheet/div/button[@id='hr_presence_button']" position="replace">
                    <button invisible="1"
                            id="hr_presence_button"
                            class="oe_stat_button"
                            disabled="1"
                            attrs="{'invisible': ['|', ('last_activity', '=', False), ('user_id', '=', False)]}">
                        <div role="img" class="fa fa-fw fa-circle text-success o_button_icon" attrs="{'invisible': [('hr_presence_state', '!=', 'present')]}" aria-label="Available" title="Available"/>
                        <div role="img" class="fa fa-fw fa-circle text-warning o_button_icon" attrs="{'invisible': [('hr_presence_state', '!=', 'to_define')]}" aria-label="Away" title="Away"/>
                        <div role="img" class="fa fa-fw fa-circle text-danger o_button_icon" attrs="{'invisible': [('hr_presence_state', '!=', 'absent')]}" aria-label="Not available" title="Not available"/>

                        <div class="o_stat_info" attrs="{'invisible': [('hr_presence_state', '=', 'present')]}">
                            <span class="o_stat_text">
                                Not Connected
                            </span>
                        </div>
                        <div class="o_stat_info" attrs="{'invisible': [('hr_presence_state', '!=', 'present')]}">
                            <span class="o_stat_value" attrs="{'invisible': [('last_activity_time', '=', False)]}">
                                <field name="last_activity_time"/>
                            </span>
                            <span class="o_stat_value" attrs="{'invisible': [('last_activity_time', '!=', False)]}">
                                <field name="last_activity"/>
                            </span>
                            <span class="o_stat_text">Present Since</span>
                        </div>
                    </button>

                </xpath>

            </field>
        </record>
        <!-- <record model="ir.ui.view" id="view_employee_form_leave_inherit_new">
            <field name="name">hr.employee.form</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr_holidays.view_employee_form_leave_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='%(hr_holidays.act_hr_employee_holiday_request)d']" position="replace">
                    <button name="%(hr_holidays.act_hr_employee_holiday_request)d"
                        type="action"
                            invisible="1"
                        class="oe_stat_button"
