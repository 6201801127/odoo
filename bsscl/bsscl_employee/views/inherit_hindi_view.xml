<odoo>
    <data>

        <record model="ir.ui.view" id="ir_attachment_report_hindi_view">
            <field name="name">ir.inherit.view.hindi</field>
            <field name="model">ir.attachment</field>
            <field name="inherit_id" ref="base.view_attachment_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/label[@for='name']" position="attributes">
                    <attribute name="string">Name / नाम
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[1]/field[@name='type']" position="attributes">
                    <attribute name="string">Type / प्रकार

                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[1]/field[@name='datas']" position="attributes">
                    <attribute name="string">File Contenet / फ़ाइल सामग्री
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[1]/field[@name='url']" position="attributes">
                    <attribute name="string">URL / यूआरएल
                        
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[1]/field[@name='mimetype']" position="attributes">
                    <attribute name="string">Mime Type / माइम प्रकार
                    </attribute>
                </xpath>

                <xpath expr="//form/sheet/group/group[2]" position="attributes">
                    <attribute name="string">Attached To / से जुड़ा
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[2]/field[@name='res_model']" position="attributes">
                    <attribute name="string">Resource Model / संसाधन मॉडल
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[2]/field[@name='res_field']" position="attributes">
                    <attribute name="string">Resource Field / संसाधन क्षेत्र
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[2]/field[@name='res_id']" position="attributes">
                    <attribute name="string">Resource Id / संसाधन आईडी
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[2]/field[@name='res_name']" position="attributes">
                    <attribute name="string">Resource Name / संसाधन का नाम
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[2]/field[@name='company_id']" position="attributes">
                    <attribute name="string">Company / कंपनी 
                    </attribute>
                    <attribute name="options">{'no_create': True, 'no_open': True, 'no_create_edit': True}</attribute>

                </xpath>
                <xpath expr="//form/sheet/group/group[2]/field[@name='public']" position="attributes">
                    <attribute name="string">IS Public Document / सार्वजनिक दस्तावेज़ है

                    </attribute>
                </xpath>

                <xpath expr="//form/sheet/group/group[3]" position="attributes">
                    <attribute name="string">History / इतिहास
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[3]/label[@for='create_uid']" position="attributes">
                    <attribute name="string">Creation / निर्माण
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[3]/div/field[@name='create_date']" position="attributes">
                    <attribute name="string">Creation Date / निर्माण तिथि

                    </attribute>
                </xpath>

                <xpath expr="//form/sheet/group/group[4]" position="attributes">
                    <attribute name="string">Discription / विवरण
                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[4]/field[@name='description']" position="attributes">
                    <attribute name="string">Description / विवरण
                    </attribute>
                </xpath>

                <xpath expr="//form/sheet/group/group[5]" position="attributes">
                    <attribute name="string">Indexed Content / सूचकांक सामग्री

                    </attribute>
                </xpath>
                <xpath expr="//form/sheet/group/group[5]/field[@name='index_content']" position="attributes">
                    <attribute name="string">Indexed Content / सूचकांक सामग्री

                    </attribute>
                </xpath>
                
               
               
             </field>
        </record>

        <record model="ir.ui.view" id="hr_rec_hindi_view">
            <field name="name">hr.rec.view.hindi</field>
            <field name="model">hr.recruitment.source</field>
            <field name="inherit_id" ref="hr_recruitment.hr_recruitment_source_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='source_id']" position="attributes">
                    <attribute name="string">Source / स्रोत
                    </attribute>
                </xpath>
                <xpath expr="//tree/field[@name='job_id']" position="attributes">
                    <attribute name="string">Job / कार्य
                    </attribute>
                </xpath>

                <xpath expr="//tree/field[@name='email']" position="attributes">
                    <attribute name="string">Email / ईमेल
                    </attribute>
                </xpath>
                <xpath expr="//tree/button[@name='create_alias']" position="attributes">
                    <attribute name="string">Generate Emai / ईमेल जनरेट करें

                    </attribute>
                </xpath>
                
               
               
             </field>
        </record>


        <record model="ir.ui.view" id="hr_job_hindi_view">
            <field name="name">hr.jon.view.hindi</field>
            <field name="model">hr.job</field>
            <field name="inherit_id" ref="hr_recruitment.hr_job_survey"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']/button/field[@name='all_application_count']" position="attributes">
                    <attribute name="string">Applications / उपयोग</attribute>
                </xpath>
                <xpath expr="//div[@name='button_box']/button/field[@name='documents_count']" position="attributes">
                    <attribute name="string">Documents / दस्तावेज़</attribute>
                </xpath>
                <xpath expr="//div[@name='button_box']/button[@name='%(hr_recruitment.action_hr_job_sources)d']/div/span[@class='o_stat_text']" position="replace">
                    <span class="o_stat_text">Trackers / ट्रैकर्स</span>
                    
                </xpath>
                <!-- <attribute name="string">Trackers / ट्रैकर्स</attribute> -->

                
                <!-- o_stat_text -->
               
             </field>
        </record>


        <record model="ir.ui.view" id="hr_recruitment_hindi_tree_view">
            <field name="name">hr.recruitment.tree.view.hindi</field>
            <field name="model">hr.leave.report</field>
            <field name="inherit_id" ref="hr_holidays.hr_leave_report_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='employee_id']" position="attributes">
                    <attribute name="string">Employee / कर्मचारी</attribute>
                </xpath>
                <xpath expr="//tree/field[@name='number_of_days']" position="attributes">
                    <attribute name="string">Number of Days / दिनों की संख्या
                    </attribute>
                </xpath>
                <xpath expr="//tree/field[@name='leave_type']" position="attributes">
                    <attribute name="string">Request Type / अनुरोध का प्रकार
                    </attribute>
                </xpath>
                <xpath expr="//tree/field[@name='date_from']" position="attributes">
                    <attribute name="string">Start Date / आरंभ करने की तिथि
                    </attribute>
                </xpath>
                <xpath expr="//tree/field[@name='date_to']" position="attributes">
                    <attribute name="string">End Date / अंतिम तिथि
                    </attribute>
                </xpath>
                <xpath expr="//tree/field[@name='state']" position="attributes">
                    <attribute name="string">Status / स्थति</attribute>
                </xpath>
                <xpath expr="//tree/field[@name='name']" position="attributes">
                    <attribute name="string">Description / विवरण</attribute>
                </xpath>
             </field>
        </record>

        <record id="hr_org_chart" model="ir.ui.view">
            <field name="name">hr.employee.view.form.hindi</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div/div[@id='o_employee_right']/h4" position="replace">
                    <h4 class="o_org_chart_title mb16 mt0">Organization Chart / उपयोग</h4>
                </xpath>
            </field>
        </record>


