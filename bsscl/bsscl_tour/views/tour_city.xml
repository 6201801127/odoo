<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="bsscl_tour_city_view_form_id" model="ir.ui.view">
            <field name="name">BSSCL: Tour city view form</field>
            <field name="model">bsscl.tour.city</field>
            <field name="arch" type="xml">
                <form string="Tour City Form">
                    <sheet>
                        <group>
                            <field name="country_id" />
                            <field name="name" />
                            <field name="ha_eligible" />
                            <field name="eligibility_percent" />

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="bsscl_tour_city_view_tree_id" model="ir.ui.view">
            <field name="name">BSSCL: Tour city view tree</field>
            <field name="model">bsscl.tour.city</field>
            <field name="arch" type="xml">
                <tree string="Tour City Tree">
                    <field name="country_id" />
                    <field name="name" />
                    <field name="ha_eligible" />
                    <field name="eligibility_percent" />

                </tree>
            </field>
        </record>

        <record id="bsscl_tour_city_action_id" model="ir.actions.act_window">
            <field name="name">Tour City / यात्रा शहर</field>
            <field name="res_model">bsscl.tour.city</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
    

</odoo>
