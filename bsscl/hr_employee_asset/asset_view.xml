        <!-- Asset Transfer -->
        <record id="asset_transfer_form_view" model="ir.ui.view">
            <field name="name">asset.transfer.form</field>
            <field name="model">asset.transfer</field>
            <field name="arch" type="xml">
                <form string="Asset Transfer" version="7.0">
                     <header>
                        <button string="Confirm Transfer" 
                                name="button_confirm" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'draft')]}"
                                class="oe_highlight"
                                groups="hr.group_hr_user"/>
                        <button string="Receive Asset" 
                                name="button_transfer" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'awaiting')]}"
                                class="oe_highlight"
                                groups="hr.group_hr_user"/>
                        <button string="Cancel" 
                                name="button_cancel" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'awaiting')]}"
                                class="oe_highlight"
                                groups="hr.group_hr_user"/>
                        <button string="Reset To Draft" 
                                name="button_draft" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'cancelled')]}"
                                class="oe_highlight"
                                groups="hr.group_hr_user"/>
                        <field name="state" 
                           widget="statusbar" 
                           statusbar_visible="draft,awaiting,transferred" class="oe_highlight"
                           statusbar_colors='{"awaiting":"upload"}'/>
                    </header>
                    <sheet>
                        <div class="oe_title">                           
                            <h1>   
                                <field name="asset_number" class="oe_inline" readonly="1"/>
                            </h1>
                        </div>
                    <group>
                        <group>
                            <field name="creation_date" invisible="1"/>
                            <!--field name="service_area_id" 
                                on_change="onchange_service(service_area_id)"
                                required="1"
                                widget="selection"
                                options="{'no_create': True}"
                                attrs="{'readonly':['|',('state','!=','draft')]}"/-->
                            <field name="location_id"
                                attrs="{'readonly':[('state','!=','draft')]}"/>
                            <field name="business_partner" required="1"
                                options="{'no_create': True}"
                                attrs="{'readonly':[('state','!=','draft')]}"/>
                            <field name="remarks"
                                attrs="{'readonly':[('state','!=','draft')]}"/>
                            <!--field name="check_hrbp" invisible="1"/-->
                        </group>
                        <group>
                            <field name="transferred_by"
                                options="{'no_create': True}" readonly="1"/>
                            <field name="transferred_date" readonly="1"/>
                            <field name="received_by"
                                options="{'no_create': True}" readonly="1"/>
                            <field name="received_date" readonly="1"/> 
                            <!--field name="check_admin" invisible="1"/-->
                            <field name="asset_ids" invisible="1"/>                            
                        </group>
                    </group>
                    <group>
                        <notebook>
                            <page string="Asset">
                                <field name="asset_transfer_ids"
                                    attrs="{'readonly':[('state','=','transferred')]}"
                                    groups="hr_employee_asset.group_mro_manager">
                                    <tree string="Asset" editable="bottom">
                                        <field name="asset_id" domain="[('state','=', 'ready')]"/>
                                        <field name="receiving_status"/>
                                        <field name="remarks"/>
                                    </tree>
                                </field>
                            </page> 
                        </notebook> 
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
        </record>

