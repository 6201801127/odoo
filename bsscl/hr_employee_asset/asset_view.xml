        <!-- Asset Tree View  -->
        <record id="assets_tree_view" model="ir.ui.view">
            <field name="name">asset.asset.tree</field>
            <field name="model">asset.asset</field>
            <field name="arch" type="xml">
                <tree string="Assets">
                    <field name="asset_number" string='Reference #'/>
                    <field name="asset_type"/>
                    <field name="brands"/>
                    <field name="model_no"/>
                    <field name="serial"/>
<!--                     <field name="service_area_id"/> -->
                    <field name="employee_id"/>
                    <field name="department_id"/>
                    <field name="state"/>
                    <field name="creation_date" invisible="1"/>
                </tree>
            </field>
        </record>

        <!-- Asset Form View  -->
        <record id="assets_form_view" model="ir.ui.view">
            <field name="name">asset.asset.form</field>
            <field name="model">asset.asset</field>
            <field name="arch" type="xml">
                <form string="Assets" version="7.0">
                    <header>

                        <button string="Ready" 
                                name="button_ready" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'draft')]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <button string="Scrapped" 
                                name="button_scrapped" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'hold')]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <button string="Lost" 
                                name="button_lost" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'hold')]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <button string="Sold" 
                                name="button_sold" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'hold')]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <button string="Repair" 
                                name="button_repair" 
                                type="object"
                                attrs="{'invisible':[('state', '!=', 'hold')]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <button string="Hold" 
                                name="button_hold" 
                                type="object"
                                attrs="{'invisible':[('state', 'not in', ('sold','lost','repair','scrapped', 'available'))]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <button string="Available" 
                                name="button_available" 
                                type="object"
                                attrs="{'invisible':[('state', 'not in', ('hold','transfer','ready'))]}"
                                class="oe_highlight"
                                groups="hr_employee_asset.group_mro_manager"/>

                        <field name="state" 
                           widget="statusbar" 
                           statusbar_visible="draft,ready,available,hold,allocated,lost,sold,repair,scrapped" class="oe_highlight"
                           statusbar_colors='{"awaiting":"blue","allocated":"blue","lost":"red"}'/>
                    </header>
                    <sheet>
                        <div>
                            <!-- <label string="Asset #"/>                          -->
                            <field name="asset_number" class="oe_inline" readonly="1"/>
                        </div>
                        <group>
                            <group>
                                <field name="product_id" invisible="1"/>
                                <field name="asset_type" required="1"
                                    on_change="onchange_asset_type(asset_type)"
                                    attrs="{'readonly':[('state','!=','draft')]}"
                                    widget="selection"
                                    options="{'no_create': True}"/>
                                <field name="brand" invisible="1"/>
                                <field name="model" invisible="1"/>
                                <field name="mobile" invisible="1"/>
                                <field name="brands"
                                    options="{'no_create': True}"
                                    attrs="{'required':[('brand','=',True)],
                                            'readonly':[('state','!=','draft')]}"/>
                                <field name="model_no"
                                    options="{'no_create': True}"
                                    attrs="{'required':[('model','=',True)],
                                            'readonly':[('state','!=','draft')]}"/>
                                <field name="serial" required="1"
                                    attrs="{'readonly':[('state','!=','draft')]}"/>
                                <field name="mobile_no" 
                                    attrs="{'required':[('mobile','=',True)],
                                    'readonly':[('state','!=','draft')]}"/>
                                <field name='description' readonly="1"/>
                            </group>
                            <group>
<!--                                 <field name="service_area_id" readonly="1"/> -->
                                <field name="employee_id" readonly="1"/>
                                <field name="department_id" readonly="1"/>
                                <field name="active" readonly="1"/>
                                <field name="property_stock_asset"
                                    readonly="1"
                                    options="{'no_create': True}"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Asset Movement">
                                <field name="asset_move_id" readonly="1">
                                    <tree name="Asset Movement" editable="bottom"> 
                                        <field name="date"/>
                                        <field name="employee_id"/>
                                        <field name="department_id"/>
                                        <field name="action"/>
                                        <field name="executed_by"/>   
                                    </tree> 
                                </field> 
                            </page>
                            <page string="Other Information">
                                <group>
                                    <group>
                                        <field name="partner_id"
                                            readonly="1"
                                            options="{'no_create': True}"
                                            attrs="{'readonly':[('state','!=','draft')]}"/>
                                        <field name="purchase_date"
                                            readonly="1"
                                            attrs="{'readonly':[('state','!=','draft')]}"/>
                                    </group>
                                    <group>
                                         <field name="warranty_start_date"
                                            attrs="{'readonly':[('state','!=','draft')]}"/>
                                        <field name="warranty_end_date"
                                            attrs="{'readonly':[('state','!=','draft')]}"/>
                                        <field name="creation_date" readonly = "1"/>
                                    </group>
                                </group>
                                <group>
                                    <field name="specifications" attrs="{'readonly':[('state','!=','draft')]}"/>
                                </group>
                            </page>
                        </notebook>
                     </sheet>
                     <div class="oe_chatter">
                            <field name="message_follower_ids" widget="mail_followers"/>
                            <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

        
