<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <record id="asset_tracking_sequence" model="ir.sequence">
            <field name="name">Asset Tracking Sequence</field>
            <field name="code">asset.tracking</field>
            <field name="prefix">Asset Allocation # AAL</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    
        <record id="asset_tracking_change_sequence" model="ir.sequence">
            <field name="name">Asset Change Sequence</field>
            <field name="code">asset.tracking.change</field>
            <field name="prefix">Asset Change # ACL</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
 
