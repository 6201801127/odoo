<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="kw_parsing_log_view_tree" model="ir.ui.view">
        <field name="name">Document Log Tree</field>
        <field name="model">handbook_parsing_log</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="job_role_id"/>
                <!-- <field name="designations"/> -->
            </tree>
        </field>
    </record>
    <record model="ir.ui.view" id="kw_parsing_log_view_form">
        <field name="name">Document Log Form view</field>
        <field name="model">handbook_parsing_log</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="job_role_id"/>
                        <!-- <field name="designations"/> -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="kw_parsing_log_action" model="ir.actions.act_window">
        <field name="name">Document data</field>
        <field name="res_model">handbook_parsing_log</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="kw_parsing_document_log_menu"
              name="Handbook Parsing Log"
              action="kw_parsing_log_action"
              parent="kw_onboarding_handbook_configuration_menu"
              sequence="8"/>

</odoo>