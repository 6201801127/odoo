<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!-- kw handbook category actions views Opening-->
        <record model="ir.actions.act_window" id="kw_handbook_category_actions_window">
            <field name="name">Handbook Category</field>
            <field name="res_model">kw_onboarding_handbook_category</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'delete':False}</field>
		<field name="domain">[]</field>
        </record>
        <!-- kw handbook category actions views Ending-->

        <!-- explicit tree view kw handbook category Opening -->
        <record model="ir.ui.view" id="kw_handbook_category_tree_view">
            <field name="name">Kwantify employee handbook list view</field>
            <field name="model">kw_onboarding_handbook_category</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <!-- explicit tree view kw handbook category Ending -->
    </data>
</odoo>
