<odoo>
    <record id="advance_claim_settlement_paper_format" model="report.paperformat">
		<field name="name">Advance Claim Settlement paper Formate</field>
		<field name="default" eval="True"/>
		<field name="format">A4</field>
		<field name="page_height">0</field>
		<field name="page_width">0</field>
		<field name="orientation">Portrait</field>
		<field name="margin_top">4</field>
		<field name="margin_bottom">4</field>
		<field name="margin_left">4</field>
		<field name="margin_right">4</field>
		<field name="header_line" eval="False"/>
		<field name="header_spacing">0</field>
		<field name="dpi">80</field>
	</record>
	
	

    <report 
		string="Advance Claim Settlement Report"
		id="action_advance_claim_settlement_report" 
		model="kw_advance_claim_settlement" 
		report_type="qweb-pdf" 
		attachment_use="True" 
		name="kw_advance_claim.report_grievance_view"
		file="kw_advance_claim.report_grievance_view"
		paperformat="kw_advance_claim.advance_claim_settlement_paper_format"	
	/>

</odoo>