<odoo>
	<data>
        <record model="ir.ui.view" id="account_payment_method_tree_view">
			<field name="model">account.payment.method</field>
			<field name="arch" type="xml">
				<tree editable="bottom">
					<field name="name"/>
					<field name="code"/>
					<field name="payment_type"/>
				</tree>
			</field>
		</record>
        <record model="ir.actions.act_window" id="account_payment_method_action_window">
			<field name="name">Account Payment Method</field>
			<field name="res_model">account.payment.method</field>
			<field name="view_mode">tree</field>
<!--			<field name="view_type">form</field>-->
		</record>

        
	</data>
</odoo>