<odoo>
    <data>
        <template id="kwonboard_identification_details" name="Identification Details">
            <t t-call="website.layout">

                <div class="container mt-2 mb-2">
                    <div class="card">
                        <div class="card-body">
                            <div class="menu-bar">
                                <ol class="progresss progresss--medium">
                                    <li class="is-complete" data-step="1">Personal Details</li>
                                    <li class="is-complete" data-step="2">Educational Details</li>
                                    <li class="is-complete" data-step="3">Work Experience Details</li>
                                    <li data-step="4" class="is-active">Identification Details</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="container ">
                    <div class="card bg-white">
                        <div class="card-body pl-5 pr-5">

                            <!--Start : Message Template -->
                                <t t-call="kw_onboarding_process.kwonboard_response_message_template"></t>
                            <!--END : Message Template -->


                            <form name="frm_identification" id="frm_identification" class="form-horizontal indicator-form"  method="post" action="/identificationdata" autocomplete="off"  enctype="multipart/form-data">
                            <!-- Mandatory field -->
                            

                            <h4 class="text-dark">
                                <b>Identification Details</b>
                                <span class="col-sm-4 p-0 text-danger font-weight-bold font-italic pull-right text-right small"> <span class="text-danger "> * </span> indicate mandatory fields </span>
                            </h4>

                            <table id="idtable" class="table table-bordered table-responsive">
                                <thead>
                                    <tr>
                                        <th scope="col" class="wd-10">Identification Type</th>
                                        <th scope="col" class="wd-20">Document Number <span style="color:red;">*</span></th>
                                       
                                        <th scope="col" class="wd-25">
                                            Document Upload
                                            <i class="fa fa-info-circle" data-rel="tooltip" title="(Only .jpeg, .jpg, .png, .pdf format and maximum file size 4 MB is allowed.)"/>
                                            <i class="mandatory"> * </i>
                                        </th>
                                        <th scope="col" class="wd-12">Date Of Issue</th>
                                        <th scope="col" class="wd-12">Date Of Expiry</th>
                                        <th scope="col" class="wd-10">Renewal Applied</th>
                                        <th scope="col" class="wd-10"/>
                                    </tr>
                                </thead>

                                <tbody>
                                    <tr class="likert">
                                        <th scope="row">AADHAAR<i class="mandatory">*</i></th>
                                        <td class="md-form">
                                            <input type="text" placeholder="Aadhaar No." maxlength="12"
                                                   class="form-control likert-field" t-att-value="txtaadhaar"
                                                   id="txtaadhaar" name="txtaadhaar"/>
                                        </td>
                                       
                                        <td class="md-form">
                                            <div class="file-upload mt-1">
                                                <div class="file-select">
                                                    <div class="file-select-button" id="fileName">Choose file</div>
                                                    <div class="file-select-name" id="noFile">No file chosen... </div>
                                                    <input type="file" placeholder="Aadhar File"
                                                            accept="image/jpg,image/jpeg,image/png,application/pdf"
                                                            class="form-control-file clsfrmfile" id="flaadhaardocument"
                                                            name="flaadhaardocument"/>
                                                </div>
                                            </div>
                                            <span name="AadhaarFIleName" id="AadhaarFIleName"
                                                  class="col-sm-3 txt-filename">
                                                <t t-esc="aadhaarfilename"/>
                                            </span>
                                            <input title="Please upload document" type="hidden" class="form-control-file likert-field"
                                                   id="hiddenaadhaarname" name="hiddenaadhaarname"
                                                   t-att-value="aadhaarfilename"/>
                                        </td>

                                         <td>
                                            <input type="hidden" class="form-control identification_datepicker"
                                                   t-att-value="adtdoi" id="adtdoi" name="adtdoi" disabled="True"/>
                                        </td>
                                        <td>
                                            <input type="hidden" class="form-control identification_datepicker"
                                                   t-att-value="adtdoe" id="adtdoe" name="adtdoe" disabled="True"/>
                                        </td>
                                        <td>
                                            <select class="form-control" id="addlrenewal" name="addlrenewal"
                                                    disabled="True" style="display:none;">
                                                <option selected="selected" value="0">Select</option>
                                                <option value="True" t-att-selected="'True'==addlrenewal">Yes</option>
                                                <option value="False" t-att-selected="'False'==addlrenewal">No</option>
                                            </select>
                                        </td>

                                        <td>
                                            <center>
                                                <button type="button" class="btn btn-danger clear anchor  btn-sm pr-3 pl-3"><i class="fa fa-trash"/></button>
                                            </center>
                                        </td>
                                    </tr>


                                    <tr class="likert">
                                        <th scope="row">PAN<i class="mandatory">*</i></th>
                                        <td class="md-form">
                                            <input  title="Please enter PAN No." type="text" placeholder="PAN" maxlength="10"
                                                   class="form-control likert-field" t-att-value="txtpan" id="txtpan"
                                                   name="txtpan"/>
                                        </td>
                                       
                                        <td class="md-form">
                                            <div class="file-upload mt-1">
                                                <div class="file-select">
                                                    <div class="file-select-button" id="fileName">Choose file</div>
                                                    <div class="file-select-name" id="noFile">No file chosen... </div>
                                                    <input  type="file" placeholder="Pan File"
                                                        accept="image/jpg,image/jpeg,image/png,application/pdf"
                                                        class="form-control-file clsfrmfile" id="flpandocument" name="flpandocument"/>
                                                </div>
                                            </div>
                                            <span name="PanFIleName" id="PanFIleName" class="col-sm-3 txt-filename">
                                                <t t-esc="panfilename"/>
                                            </span>
                                            <input title="Please upload document" type="hidden" class="form-control-file likert-field"
                                                   id="hiddenpanname" name="hiddenpanname" t-att-value="panfilename"/>
                                        </td>

                                         <td>
                                            <input type="hidden" class="form-control identification_datepicker"
                                                   t-att-value="pandtdoi" id="pandtdoi" name="pandtdoi"
                                                   disabled="True"/>
                                        </td>
                                        <td>
                                            <input type="hidden" class="form-control identification_datepicker"
                                                   t-att-value="pandtdoe" id="pandtdoe" name="pandtdoe"
                                                   disabled="True"/>
                                        </td>
                                        <td>
                                            <select class="form-control  " id="panddlrenewal" name="panddlrenewal"
                                                    disabled="True" style="display:none;">
                                                <option value="0">Select</option>
                                                <option value="True" t-att-selected="'True'==str(panddlrenewal)">Yes</option>
                                                <option value="False" t-att-selected="'False'==str(panddlrenewal)">No</option>
                                            </select>
                                        </td>
                                        <td>
                                            <center>
                                                <button type="button" class="btn btn-danger clear anchor btn-sm pr-3 pl-3"><i class="fa fa-trash"/></button>
                                            </center>
                                        </td>
                                    </tr>


                                    <tr class="likert">
                                        <th scope="row">Voter Card</th>
                                        <td class="md-form">
                                            <input type="text" placeholder="Voter Card No." maxlength="20"
                                                   class="form-control likert-field" t-att-value="txtvoterid"
                                                   id="txtvoterid" name="txtvoterid"/>
                                        </td>
                                        
                                        <td class="md-form">
                                            <div class="file-upload mt-1">
                                                <div class="file-select">
                                                    <div class="file-select-button" id="fileName">Choose file</div>
                                                    <div class="file-select-name" id="noFile">No file chosen... </div>
                                                    <input  type="file" placeholder="Voter File"
                                                        accept="image/jpg,image/jpeg,image/png,application/pdf"
                                                        class="form-control-file clsfrmfile"
                                                            id="vflvoteriddocument" name="vflvoteriddocument"/>
                                                </div>
                                            </div>
                                            <span name="VoterIdFIleName" id="VoterIdFIleName" class="col-sm-3 text-success txt-filename"><t t-esc="voterfilename"/></span>
                                            <input title="Please upload document" type="hidden" class="form-control-file likert-field"
                                                   id="hiddenvotername" name="hiddenvotername"
                                                   t-att-value="voterfilename"/>
                                        </td>

                                        <td>
                                            <input type="hidden" class="form-control identification_datepicker"
                                                   t-att-value="vdtdoi" id="vdtdoi" name="vdtdoi" disabled="True"/>
                                        </td>
                                        <td>
                                            <input type="hidden" class="form-control identification_datepicker"
                                                   t-att-value="vdtdoe" id="vdtdoe" name="vdtdoe" disabled="True"/>
                                        </td>
                                        <td>
                                            <select class="form-control" id="vddlrenewal" name="vddlrenewal"
                                                    disabled="True" style="display:none;">
                                                <option value="0">Select</option>
                                                <option value="True" t-att-selected="'True'==vddlrenewal">Yes</option>
                                                <option value="False" t-att-selected="'False'==vddlrenewal">No</option>
                                            </select>
                                        </td>
                                        <td>
                                            <center>
                                                <button type="button" class="btn btn-danger clear anchor btn-sm pr-3 pl-3"><i class="fa fa-trash"/></button>
                                            </center>
                                        </td>
                                    </tr>


                                    <tr class="likert">
                                        <th scope="row">Passport</th>
                                        <td class="md-form">
                                            <input title="Please enter passport no." type="text" placeholder="Passport No." maxlength="10"
                                                   class="form-control likert-field" t-att-value="txtpassport"
                                                   id="txtpassport" name="txtpassport"/>
                                        </td>

                                        <td class="md-form">
                                            <div class="file-upload mt-1">
                                                <div class="file-select">
                                                    <div class="file-select-button" id="fileName">Choose file</div>
                                                    <div class="file-select-name" id="noFile">No file chosen... </div>
                                                    <input  type="file" placeholder="Passport File"
                                                        accept="image/jpg,image/jpeg,image/png,application/pdf"
                                                        class="form-control-file clsfrmfile" id="flpassportdocument"
                                                        name="flpassportdocument"/>
                                                </div>
                                            </div>
                                            <span name="PassportFIleName" id="PassportFIleName"
                                                  class="col-sm-3 txt-filename ">
                                                <t t-esc="passportfilename"/>
                                            </span>
                                            <input title="Please upload document" type="hidden" class="form-control-file likert-field"
                                                   id="hiddenpasportname" name="hiddenpasportname"
                                                   t-att-value="passportfilename"/>
                                        </td>
                                        <td class="md-form">
                                            <input type="text"  title="Please enter date of issue" placeholder="Date Of Issue"
                                                   class="form-control identification_datepicker likert-field"
                                                   t-att-value="passdtdoi" id="passdtdoi" name="passdtdoi" readonly="readonly"/>
                                        </td>

                                        <td class="md-form">
                                            <input type="text" title="Please enter date of expiry" placeholder="Date Of Expiry"
                                                   class="form-control identification_datepicker likert-field"
                                                   t-att-value="passdtdoe" id="passdtdoe" name="passdtdoe" readonly="readonly"/>
                                        </td>

                                        <td class="md-form">
                                            <select class="form-control likert-field" title="Please select renewal status"  placeholder="Renewal Applied"
                                                    id="passddlrenewal" name="passddlrenewal">
                                                <option value="">Select</option>
                                                <option value="True" t-att-selected="'True'==passddlrenewal">Yes</option>
                                                <option value="False" t-att-selected="'False'==passddlrenewal">No</option>
                                            </select>
                                        </td>

                                        <td>
                                            <center>
                                                <button type="button" class="btn btn-danger clear anchor btn-sm pr-3 pl-3"><i class="fa fa-trash"/></button>
                                            </center>
                                        </td>
                                    </tr>

                                    <tr class="likert">
                                        <th scope="row">Driving License</th>
                                        <td class="md-form">
                                            <input title="Please enter driving license no." type="text" placeholder="DL No." maxlength="15"
                                                   class="form-control likert-field" t-att-value="txtdl" id="txtdl"
                                                   name="txtdl"/>
                                        </td>
                                        
                                        <td class="md-form">
                                            <div class="file-upload mt-1">
                                                <div class="file-select">
                                                    <div class="file-select-button" id="fileName">Choose file</div>
                                                    <div class="file-select-name" id="noFile">No file chosen... </div>
                                                    <input type="file" placeholder="Dl File"
                                                        accept="image/jpg,image/jpeg,image/png,application/pdf"
                                                        class="form-control-file clsfrmfile" id="dlfldldocument" name="dlfldldocument"/>
                                                    </div>
                                                </div>
                                            <span name="DrivingLicenceFIleName" id="DrivingLicenceFIleName"
                                                  class="col-sm-3 txt-filename">
                                                <t t-esc="dlfilename"/>
                                            </span>
                                            <input title="Please upload document" type="hidden" class="form-control-file likert-field"
                                                   id="hiddendlname" name="hiddendlname" t-att-value="dlfilename"/>
                                        </td>
                                        <td class="md-form">
                                            <input title="Please enter date of issue" type="text" placeholder="Date Of Issue"
                                                   class="form-control identification_datepicker likert-field"
                                                   t-att-value="dldtdoi" id="dldtdoi" name="dldtdoi" readonly="readonly"/>
                                        </td>
                                        <td class="md-form">
                                            <input type="text" title="Please enter date of expiry"  placeholder="Date Of Expiry"
                                                   class="form-control identification_datepicker likert-field"
                                                   t-att-value="dldtdoe" id="dldtdoe" name="dldtdoe" readonly="readonly"/>
                                        </td>
                                        <td class="md-form">
                                            <select class="form-control likert-field"  title="Please select renewal status" placeholder="Renewal Applied"
                                                    id="dlddlrenewal" name="dlddlrenewal">
                                                <option value="">Select</option>
                                                <option value="True" t-att-selected="'True'==dlddlrenewal">Yes</option>
                                                <option value="False" t-att-selected="'False'==dlddlrenewal">No</option>
                                            </select>
                                        </td>
                                        <td>
                                            <center>
                                                <button type="button" class="btn btn-danger clear anchor btn-sm pr-3 pl-3"><i class="fa fa-trash"/></button>
                                            </center>
                                        </td>
                                    </tr>
                                  
                                </tbody>
                            </table>

                            <div>
                                <a href="/workexperiencedetails/">
                                    <button type="button" class="btn btn-danger btn-md" id="btnSubmitEDBack">Back</button>
                                </a>

                                <button type="submit" class="btn btn-success btn-md" id="btnSaveED" name="draft" value="draft"  >Save Identification Details</button>
                                <button type="submit" class="btn btn-success btn-md" id="btnSubmitED" data-toggle="modal" >Submit</button>
                                <!-- data-target="#thankyoumodal" -->
                            </div>

                            <div class="modal fade mx-auto" id="thankyoumodal" tabindex="-1" role="dialog" aria-labelledby="ThankYou_Modal" aria-hidden="true">
                                <div class="modal-dialog modal-dialog-centered modal-success" role="document">
                                    <div class="modal-content">
                                        <div class="modal-body" id="popup">
                                            <div class="text-center">
                                                <h1> Thank you </h1>
                                                <h3> You've submitted all the details. </h3>
                                                <a href="/" class="btn btn-info"> Go to Homepage </a>                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        </div>
                    </div>
                </div>
            </t>
        </template>
        <template id="assets_frontend_identify" name="assets_frontend_identify" inherit_id="kw_onboarding_process.kwonboard_identification_details">
            <xpath expr="." position="inside">
                <link href='https://fonts.googleapis.com/css?family=Noto Sans' rel='stylesheet' />
<!--                <link href="/kwantify_theme/static/src/mdb/css/mdb.min.css" rel="stylesheet"/>-->
<!--                <link rel="stylesheet" href="/kw_onboarding_process/static/src/css/bootstrap-datepicker.min.css"/>-->
                <link rel="stylesheet" href="/kw_onboarding_process/static/src/css/onboarding.css"/>

<!--                <script type="text/javascript" src="/kwantify_theme/static/src/mdb/js/mdb.min.js"/>-->
                <script type="text/javascript" src="/kw_onboarding_process/static/src/lib/jquery.validate.min.js"/>
                <script type="text/javascript" src="/kw_onboarding_process/static/src/lib/additional-methods.js"/>
                <!-- <script type="text/javascript" src="/kw_onboarding_process/static/src/js/bootstrap-datepicker.min.js"/> -->

                <script type="text/javascript" src="/kw_onboarding_process/static/src/js/kw_onboarding_process_validation.js"/>
                <script type="text/javascript" src="/kw_onboarding_process/static/src/js/kw_onboard_identification_details.js"/>
            </xpath>
        </template>
    </data>
</odoo>