<odoo>

    <data noupdate="0">

        <function name="write" model="ir.model.data">
            <function name="search" model="ir.model.data">
                <value eval="[('module', '=', 'bsscl_recruitment'), ('name', 'in', ['regular_id','cont_ag','cont_stpi'])]"/>
            </function>
            <value eval="{'noupdate': False}" />
        </function>


        <record model="employement.type" id="regular_id">
            <field name="name">Regular Employee</field>
            <field name="code">regular</field>
        </record>

        <record model="employement.type" id="cont_ag">
            <field name="name">Contractual with Agency</field>
            <field name="code">contract_agency</field>
        </record>

        <record model="employement.type" id="cont_stpi">
            <field name="name">Contractual with STPI</field>
            <field name="code">contract_stpi</field>
        </record>



        <record model="recruitment.type" id="dr">
            <field name="name">Direct Recruitment(DR)</field>
        </record>
        <record model="recruitment.type" id="transfer">
            <field name="name">Transfer(Absorption)</field>
        </record>
        <record model="recruitment.type" id="im_ab">
            <field name="name">Immediate Absorption</field>
        </record>
        <record model="recruitment.type" id="Deputation">
            <field name="name">Deputation</field>
        </record>
        <record model="recruitment.type" id="CompassionateAppointment">
            <field name="name">Compassionate Appointment</field>
        </record>
        <record model="recruitment.type" id="Promotion">
            <field name="name">Promotion</field>
        </record>

    </data>

</odoo>