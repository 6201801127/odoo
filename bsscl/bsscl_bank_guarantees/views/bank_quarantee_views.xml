<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="bank_guarantee_tree_view" model='ir.ui.view'>
        <field name="name">bank.guarantee.tree</field>
        <field name="model">bank.guarantee</field>
        <field name="arch" type="xml">
            <tree create="false" edit="false" delete="false">
                <field name="name"/>
                <field name="company_id" invisible="1"/>
                <field name="currency_id" invisible="1"/>
                <field name="bsscl_contract_id"/>
                <field name="bank_name"/>
                <field name="recieved_date"/>
                <field name="from_date"/>
                <field name="to_date"/>
                <field name="upload" filename="fname" widget="Binary"/>
                <field name="fname" invisible="1"/>
              </tree>
        </field>
    </record>



    <record model="ir.actions.act_window" id="bank_guarantee_action_view">
            <field name="name">Bank Guarantee</field>
            <field name="res_model">bank.guarantee</field>
            <field name="binding_view_types">tree</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="bank_guarantee_tree_view"/>
        </record>

</odoo>