<?xml version="1.0" ?>
<odoo>
    <record id="action_exit_tree" model="ir.actions.act_window">
        <field name="name">Hr Exit Management Rules </field>
        <field name="res_model">exception.rule</field>
        <!-- <field name="view_type">form</field> -->
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="base_exception_and_approval.view_exception_rule_tree"/>
        <field name="domain">[('rule_group', '=', 'exit_transfer_management')]</field>
        <field name="context">{'active_test': False, 'default_rule_group' : 'exit_transfer_management'}</field>
    </record>
    <menuitem
            id="menu_hr_exit_transfer_management"
            parent="exit_transfer_management.exit_transfer_management_create_list"
            sequence="20"
            action="action_exit_tree"
            name="Exceptions / अपवाद"
    />



</odoo>
