<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for crossovered.budget -->
        <record id="seq_budget" model="ir.sequence">
            <field name="name">Budget Sequence</field>
            <field name="code">crossovered.budget.seq</field>
            <field name="prefix">BGT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_investment_request" model="ir.sequence">
            <field name="name">Investment Request Sequence</field>
            <field name="code">investment.request.seq</field>
            <field name="prefix">IM/</field>
            <field name="padding">5</field>
        </record>


         <record id="seq_planned_investment_request" model="ir.sequence">
            <field name="name">Planned Investment Sequence</field>
            <field name="code">planned.investment.seq</field>
            <field name="prefix">PI/</field>
            <field name="padding">5</field>
        </record>

         <record id="seq_actual_investment_request" model="ir.sequence">
            <field name="name">Actual Investment Sequence</field>
            <field name="code">actual.investment.seq</field>
            <field name="prefix">AI/</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
