<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

    <record id="kw_kt_view_meeting_integration" model="ir.ui.view">
            <field name="model">kw_kt_view</field>
            <field name="inherit_id" ref="kw_kt.kw_kt_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//sheet/notebook/page/field[@name='time_line_plan_ids']/tree/field[@name='status']" position="before">
                    <field name="meeting_id" invisible="1"/>
                    <field name="kt_status" invisible="1"/>
                    <button name="schedule_meeting" type="object" attrs= "{'invisible': ['|','|',('meeting_id','!=',False),('kt_status','not in',['Approved']),('status','in',['completed'])]}"   class="btn btn-primary" string="Schedule Meeting"/>
                    <!-- <button name="view_meeting" type="object"   attrs= "{'invisible': ['|',('meeting_id','=',False),('kt_status','not in',['Applied'])]}"   class="btn btn-primary" string="Edit Meeting"/> -->
                </xpath>
            </field>
        </record>
    </data>
</odoo>




                                       