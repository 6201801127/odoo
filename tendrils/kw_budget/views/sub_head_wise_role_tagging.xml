<odoo>
    <data>

        <!--        Resource Budget Tree View    class="customer_tree"-->

        <record id="view_sub_head_wise_role_tagging_tree" model="ir.ui.view">
            <field name="name">sub_head_wise_role_tagging</field>
            <field name="model">sub_head_wise_role_tagging</field>
            <field name="arch" type="xml">
                <tree  class="customer_tree">
                    <field name="account_sub_head_id"/>
                    <field name="role_id"/>
                    <field name="category_ids"  widget="many2many_tags" force_save="1"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>

        <!--    Resource Budget Form View    -->

        <record model="ir.ui.view" id="view_sub_head_wise_role_tagging_form">
            <field name="name">sub_head_wise_role_tagging_form</field>
            <field name="model">sub_head_wise_role_tagging</field>
            <field name="arch" type="xml">
                <form duplicate="false">
                    <sheet>
                        <group>
                            <field name="account_sub_head_id"/>
                            <field name="role_id"/>
                            <field name="category_ids" widget="many2many_tags" force_save="1"/>
                            <field name="status"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_view_sub_head_wise_role_tagging_search" model="ir.ui.view">
            <field name="name">sub_head_wise_role_tagging_view_search</field>
            <field name="model">sub_head_wise_role_tagging</field>
            <field name="arch" type="xml">
                <search string="Search Description">
                    <field name="account_sub_head_id"/>
                    <group expand="0" string="Group By">
                        <filter name="group_by_account_sub_head_id" string="Account Sub-Head" context="{'group_by': 'account_sub_head_id'}"/>
                    </group>
                </search>
            </field>
        </record>


        <record model="ir.actions.act_window" id="sub_head_wise_role_tagging_action_window">
            <field name="name">Tagging Sub-Head Wise Role</field>
            <field name="res_model">sub_head_wise_role_tagging</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="search_view_id" ref="view_view_sub_head_wise_role_tagging_search"/>
            <field name="context">{'search_default_group_by_account_sub_head_id':1}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Click to create a new Record.
                </p>
                <p>
                </p>
            </field>
        </record>

    </data>
</odoo>