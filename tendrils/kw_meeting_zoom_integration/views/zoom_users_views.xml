<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <!-- Add you code here -->
        <record model="ir.ui.view" id="kw_zoom_users_tree">
            <field name="name">kw_res_user.tree</field>
            <field name="model">kw_zoom_users</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="zoom_id" invisible="1"/>
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="email"/>
                    <field name="type"/>
                    <field name="pmi"/>
                    <field name="use_pmi" invisible="1"/>
                    <field name="vanity_url" invisible="1"/>
                    <field name="personal_meeting_url" invisible="1"/>
                    <field name="timezone" invisible="1"/>
                    <field name="verified" invisible="1"/>
                    <field name="dept" invisible="1"/>
                    <field name="created_at"/>
                    <field name="last_login_time" invisible="1"/>
                    <field name="last_client_version"/>
                    <field name="pic_url" invisible="1"/>
                    <field name="host_key" invisible="1"/>
                    <field name="jid" invisible="1"/>
                    <field name="group_ids" invisible="1"/>
                    <field name="im_group_ids" invisible="1"/>
                    <field name="account_id" invisible="1"/>
                    <field name="phone_country" invisible="1"/>
                    <field name="phone_number" invisible="1"/>
                    <field name="status" invisible="1"/>
                    <field name="user_id"/>
                    <field name="company_id" invisible="1"/>
                    <field name="type_id"/>
                    <field name="mapped_user"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="kw_zoom_users_form">
            <field name="name">kw_res_users.form</field>
            <field name="model">kw_zoom_users</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <button name="sync_zoom_users" type="object" string="Sync Zoom User"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="zoom_id" attrs="{'invisible':[('zoom_id','=', False)]}"/>
                                <field name="first_name" required="1"/>
                                <field name="last_name" required="1"/>
                                <field name="email" required="1"/>
                                <field name="type_sel" required="1"/>
                                <field name="type" required="1" invisible="1"/>
                                <field name="user_id" required="1"/>
                                <field name="pmi" attrs="{'invisible':[('pmi','=', False)]}"/>
                                <field name="use_pmi" attrs="{'invisible':[('use_pmi','=', False)]}"/>
                                <field name="vanity_url" attrs="{'invisible':[('vanity_url','=', False)]}"/>
                                <field name="personal_meeting_url" attrs="{'invisible':[('personal_meeting_url','=', False)]}"/>
                                <field name="timezone" attrs="{'invisible':[('timezone','=', False)]}"/>
                                <field name="verified" attrs="{'invisible':[('verified','=', False)]}"/>
                                <field name="dept" attrs="{'invisible':[('dept','=', False)]}"/>
                                <field name="created_at" attrs="{'invisible':[('created_at','=', False)]}"/>
                                <field name="last_login_time" attrs="{'invisible':[('last_login_time','=', False)]}"/>
                                <field name="last_client_version" attrs="{'invisible':[('last_client_version','=', False)]}"/>
                                <field name="pic_url" attrs="{'invisible':[('pic_url','=', False)]}"/>
                                <field name="host_key" attrs="{'invisible':[('host_key','=', False)]}"/>
                                <field name="jid" attrs="{'invisible':[('jid','=', False)]}"/>
                                <field name="group_ids" attrs="{'invisible':[('group_ids','=', False)]}"/>
                            </group>
                            <group>
                                <field name="im_group_ids" attrs="{'invisible':[('im_group_ids','=', False)]}"/>
                                <field name="account_id" attrs="{'invisible':[('account_id','=', False)]}"/>
                                <field name="phone_country" attrs="{'invisible':[('phone_country','=', False)]}"/>
                                <field name="phone_number" attrs="{'invisible':[('phone_number','=', False)]}"/>
                                <field name="status" attrs="{'invisible':['|',('status','=', False),('id','=',False)]}" />
                                <field name="company_id" attrs="{'invisible':['|',('company_id','=', False),('id','=',False)]}"/>
                                <field name="type_id" attrs="{'invisible':[('type_id','=', False)]}"/>
                                <field name="mapped_user" attrs="{'invisible':[('mapped_user','=', False)]}"/>
                                <field name="active" attrs="{'invisible':[('active','=', False)]}"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="search_view_zoom_users" model="ir.ui.view">
            <field name="name">Zoom Users Search View</field>
            <field name="model">kw_zoom_users</field>
            <field name="arch" type="xml">
                <search>
                    <field name="first_name"/>
                    <filter string="firstname" name="filter_first_name"/>
                     <separator/>
                </search>
            </field>
        </record>
      
        <record model="ir.actions.act_window" id="kw_zoom_users_action">
            <field name="name">Zoom Users</field>
            <field name="res_model">kw_zoom_users</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name='search_view_id' ref='search_view_zoom_users'/>
            <field name="domain">['|',('active','=', True),('active','=', False)]</field>
            <field name="view_ids" eval="[(5, 0, 0),
		(0, 0, {'view_mode': 'tree', 'view_id': ref('kw_zoom_users_tree')}),
        (0, 0, {'view_mode': 'form', 'view_id': ref('kw_zoom_users_form')})]"/>
        </record>
    </data>
</odoo>
