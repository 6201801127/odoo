<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <!-- Add you code here -->
        <record model="ir.ui.view" id="kw_zoom_meeting_tree">
            <field name="name">kw_zoom_meeting.tree</field>
            <field name="model">kw_zoom_meeting</field>
            <field name="arch" type="xml">
                <tree create="false" import="false">
                    <field name="topic"/>
                    <field name="agenda" invisible="1"/>
                    <field name="zoom_id"/>
                    <field name="uuid_zoom" invisible="1"/>
                    <field name="host_id" invisible="1"/>
                    <field name="host_email" invisible="1"/>
                    <field name="type"/>
                    <field name="start_time"/>
                    <field name="duration"/>
                    <field name="schedule_for" invisible="1"/>
                    <field name="timezone" invisible="1"/>
                    <field name="status"/>
                    <field name="created_at"/>
                    <field name="start_url" invisible="1"/>
                    <field name="join_url" invisible="1"/>
                    <field name="password" invisible="1"/>
                    <field name="h323_password" invisible="1"/>
                    <field name="pstn_password" invisible="1"/>
                    <field name="encrypted_password" invisible="1"/>
                    <field name="misc_settings" invisible="1"/>
                    <field name="meeting_id"/>

                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="kw_zoom_meeting_form">
            <field name="name">kw_zoom_meeting.form</field>
            <field name="model">kw_zoom_meeting</field>
            <field name="arch" type="xml">
                <form create="false" edit="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="meeting_id"/>
                                <field name="topic"/>
                                <field name="created_at"/>
                                <field name="agenda"/>
                                <field name="zoom_id"/>
                                <field name="uuid_zoom"/>
                                <field name="host_id"/>
                                <field name="host_email"/>
                                <field name="type"/>
                            </group>

                            <group>
                                <field name="start_time"/>
                                <field name="duration"/>
                                <field name="schedule_for"/>
                                <field name="timezone"/>
                                <field name="password"/>
                                <field name="h323_password"/>
                                <field name="pstn_password"/>
                                <field name="encrypted_password"/>
                            </group>
                        </group>
                        <group>
                            <field name="start_url"/>
                            <field name="join_url"/>
                            <field name="misc_settings"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

      
        <record model="ir.actions.act_window" id="kw_zoom_meeting_action">
            <field name="name">Zoom Meeting</field>
            <field name="res_model">kw_zoom_meeting</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>