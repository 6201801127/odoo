<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <!-- Add you code here -->
        <record model="ir.ui.view" id="kw_zoom_event_log_tree">
            <field name="name">kw_zoom_event_log.tree</field>
            <field name="model">kw_zoom_event_log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="payload"/>
                    <field name="event"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="kw_zoom_event_log_form">
            <field name="name">kw_zoom_event_log.form</field>
            <field name="model">kw_zoom_event_log</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="payload"/>
                                <field name="event"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

      
        <record model="ir.actions.act_window" id="kw_zoom_event_log_action">
            <field name="name">Zoom Event Logs</field>
            <field name="res_model">kw_zoom_event_log</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>