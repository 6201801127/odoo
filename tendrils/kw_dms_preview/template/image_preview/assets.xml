<?xml version="1.0" encoding="UTF-8"?>


<odoo>
	<template id="assets_backend_image_preview" inherit_id="web.assets_backend">
	    <xpath expr="//link[last()]" position="after">
			<link rel="stylesheet" href="/kw_dms_preview/static/image_preview/src/scss/image.scss" />
			<link rel="stylesheet" href="/kw_dms_preview/static/image_preview/src/scss/vector.scss" />
		</xpath>
		<xpath expr="//script[last()]" position="after">
			<script type="text/javascript" src="/kw_dms_preview/static/image_preview/src/js/image.js"/>
			<script type="text/javascript" src="/kw_dms_preview/static/image_preview/src/js/vector.js"/>
		</xpath>
	</template>
</odoo>