<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="sms_log_tree_view" model="ir.ui.view">
      <field name="name">SMS Log tree</field>
      <field name="model">sms_log</field>
      <field name="arch" type="xml">
        <tree string="SMS Log Form">
          <field name="mobile_no"/>
          <field name="message_id"/>
          <field name="category"/>
          <field name="status"/>
          <field name="response_status"/>
        </tree>
      </field>
    </record>

    <record id="sms_log_tree_from" model="ir.ui.view">
      <field name="name">SMS Log form</field>
      <field name="model">sms_log</field>
      <field name="arch" type="xml">
        <form string="SMS Log Tree">
            <group>
                <group>
                    <field name="mobile_no"/>
                    <field name="message_id"/>
                    <field name="category"/>
                    <field name="status"/>
                    <field name="response_status"/>
                </group>
            </group>
        </form>
      </field>
    </record>

    <record id="action_sms_log" model="ir.actions.act_window">
      <field name="name">SMS Log</field>
      <field name="res_model">sms_log</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>
