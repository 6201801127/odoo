<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <!--Explicit form view of sms_track : Opening-->

    <record id="sms_track_form_view" model="ir.ui.view">
      <field name="name">Logs.form</field>
      <field name="model">sms_track</field>
      <field name="arch" type="xml">
        <form string="logs" create="false" edit="false">
          <group>
            <field name="model_id"/>
            <field name="gateway_id"/>
            <field name="mobile_no"/>
            <field name="response_id"/>
          </group>
          <separator/>
          <h2>Body</h2>
          <field name="message_id"/>
        </form>
      </field>
    </record>
    <!--Explicit form view of sms_track : Ending-->

    <!--Explicit tree view of sms_track : Opening-->
    <record id="sms_track_tree_view" model="ir.ui.view">
      <field name="name">Logs.list</field>
      <field name="model">sms_track</field>
      <field name="arch" type="xml">
        <tree string="logs" default_order="create_date desc" create="false" edit="false">
          <field name="create_date"/>
          <field name="model_id"/>
          <field name="mobile_no"/>
          <field name="gateway_id"/>
        </tree>
      </field>
    </record>
    <!--Explicit tree view of sms_track : Ending-->

    <!--Explicit tree view of sms_track : Opening-->
    <record id="action_sms_track" model="ir.actions.act_window">
      <field name="name">Logs</field>
      <field name="res_model">sms_track</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="search_view_id" eval="False"/>
      <field name="context">{}</field>
      <field name="help">use for send sms</field>
    </record>
    <!--Explicit tree view of sms_track : Ending-->

  </data>
</odoo>
