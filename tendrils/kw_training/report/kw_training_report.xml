<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data >

        <record id="kw_training_report_view_tree" model="ir.ui.view">
            <field name="name">Appraisal Page Wise Report</field>
            <field name="model">kw_training_report</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0' delete='0'>
                    <field name="fiscal_year"/>
                    <field name="course"/>
                    <field name="name"/>
                    <field name="instructor_type"/>
                    <field name="totalattendee"/>
                    <field name="totalpaticipatedper"/>
                    <field name="feedbackpercentage"/>
                </tree>
            </field>
        </record>
    
        <record id="kw_training_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Training Report</field>
            <field name="res_model">kw_training_report</field>
            <field name="view_mode">tree,graph</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    No training report(s) found...
                </p>
            </field>
        </record>

        <menuitem name="Training Report" id="kw_training_report_menu_item" parent='menu_kw_training_report' action="kw_training_report_act_window" sequence='99'/>

    </data>
</odoo>
