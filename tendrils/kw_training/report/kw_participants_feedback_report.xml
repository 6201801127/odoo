<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Tree view for "model" -->
        <record id="view_kw_participant_feedback_report_tree" model="ir.ui.view">
            <field name="name">view.kw_participant_feedback_report.tree</field>
            <field name="model">kw_participants_feedback_report</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <!-- <field name="priority" eval="16" />
            <field name="active" eval="True" /> -->
            <field name="arch" type="xml">
                <tree string="kw_participant_feedback_report_tree"  create='0' edit='0' delete='0' import='0'>
                    <field name="financial_year" />
                    <field name="course_type_id" />
                    <field name="course_id" />
                    <field name="training_id" />
                    <field name="emp_id" />
                    <field name="response_id" invisible="1"/>
                    <field name="question_id" invisible="1"/>
                    <field name="question_name" /> 
                    <field name="mark" />
                    <field name="remark" />
                </tree>
            </field>
        </record>


        <record id="kw_training_participants_feedback_search" model="ir.ui.view">
			<field name="name">kw_training_participants_feedback.search</field>
			<field name="model">kw_participants_feedback_report</field>
			<field name="type">search</field>
			<field name="arch" type="xml">
				<search  string="kw_training_participants_feedback_report_search" >
                    <field name="financial_year" />
                    <field name="course_type_id" />
                    <field name="course_id" />
                    <field name="training_id" />
                    <field name="emp_id" />
                    <field name="response_id" invisible="1"/>
                    <field name="question_id" invisible="1"/>
                    <field name="question_name" /> 
                    <field name="mark" />
                    <field name="remark" />
                </search>
			</field>
		</record>
       
        
        

        <!-- Main window action for "model" -->
        <record id="action_kw_participant_feedback_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Participant Feedback Report</field>
            <field name="res_model">kw_participants_feedback_report</field>
            <field name="view_mode">tree</field>
        </record>
        
        

        <menuitem name="Participants Feedback Report"
         parent="kw_training.menu_kw_training_report" 
         id="participants_feedback_report_menu" 
         action="action_kw_participant_feedback_report_act_window"
         sequence="120"/>

    </data>
</odoo>
