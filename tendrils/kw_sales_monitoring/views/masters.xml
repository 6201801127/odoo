<odoo>
  <data>
    <!-- Evalution Method Start -->
    <record model="ir.ui.view" id="evalution_method_master_list_view">
      <field name="name">evalution_method_master list</field>
      <field name="model">evalution_method_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name" />
          <field name="code" />
          <field name="active" widget="boolean_toggle" />
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="evalution_method_action_window">
      <field name="name">Evalution Method</field>
      <field name="res_model">evalution_method_master</field>
      <field name="view_mode">tree,form</field>
    </record>
    <!-- Evalution Method  End -->

    <!-- Funding Agency Start -->

    <record model="ir.ui.view" id="funding_agency_master_list_view">
      <field name="name">funding_agency_master list</field>
      <field name="model">funding_agency_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name" />
          <field name="code" />
          <field name="active" widget="boolean_toggle" />
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="funding_agency_action_window">
      <field name="name">Funding Agency Master</field>
      <field name="res_model">funding_agency_master</field>
      <field name="view_mode">tree,form</field>
    </record>
    <!-- Funding Agency view End -->

    <!-- Request for Service Master Start -->
    <record model="ir.ui.view" id="request_for_service_master_list_view">
      <field name="name">request_for_service_master list</field>
      <field name="model">request_for_service_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="department_id" widget="many2many_tags"/>
          <field name="item" />
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="request_for_service_master_action_window">
      <field name="name">Requesr for Service Master</field>
      <field name="res_model">request_for_service_master</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- Request for Service Master End -->

    <!-- Procurement Master Start -->
    <record model="ir.ui.view" id="procurement_master_list_view">
      <field name="name">procurement_master list</field>
      <field name="model">procurement_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name" />
          <field name="code" />
          <field name="active" widget="boolean_toggle" />

        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="procurement_master_action">
      <field name="name">Procurement Master</field>
      <field name="res_model">procurement_master</field>
      <field name="view_mode">tree,form</field>
    </record>
    <!-- Procurement Master End -->

    <record model="ir.ui.view" id="domain_offering_service_line_list_view">
      <field name="name">domain_offering_service_line list</field>
      <field name="model">domain_offering_service_line</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="line_of_business_id" />
          <field name="type" />
          <field name="name" />
          <field name="parent_id" />
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="domain_offering_service_line_master_action">
      <field name="name">Domain Offerings/ Solutions/ Services</field>
      <field name="res_model">domain_offering_service_line</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>