<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
      <record id="question_master_list_view" model="ir.ui.view">
        <field name="name">question_master list</field>
        <field name="model">question_master</field>
        <field name="arch" type="xml">
          <tree editable="bottom">
            <field name="question_name"/>
          </tree>
        </field>
      </record>

      <record id="question_action_window" model="ir.actions.act_window">
        <field name="name">Question Master</field>
        <field name="res_model">question_master</field>
        <field name="view_mode">tree,form</field>
      </record>
    </data>
  </odoo>