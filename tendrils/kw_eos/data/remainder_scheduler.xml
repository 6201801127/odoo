<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="resignation_remainder_mail" model="ir.cron">
            <field name="name">EOS: Resignation Reminder Scheduler</field>
            <field name="model_id" ref="model_kw_resignation" />
            <field name="state">code</field>
            <field name="code">model.run_remainder_mail_eos()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">6</field>
            <field name="active" eval="False" />
        </record>
    </data>
</odoo>
