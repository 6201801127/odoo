<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="kw_resignation_dynamic_workflow_main" model="odoo.workflow">
            <field name="name">Resignation Form Approval</field>
            <field name="activities_add" eval="True"/>
            <field name="followers_add" eval="False"/>
            <field name="mail_thread_add" eval="True"/>
            <field name="remove_default_attrs_mod" eval="False"/>
            <field name="model_id" ref="kw_eos.model_kw_resignation"/>
        </record>


        <record id="kw_resignation_dynamic_workflow_node_1" model="odoo.workflow.node">
            <field name="name">Applied</field>
            <field name="workflow_id" ref="kw_resignation_dynamic_workflow_main"/>
            <field name="node_name">apply</field>
            <field name="sequence" eval="1"/>
            <field name="is_visible" eval="True"/>
            <field name="flow_start" eval="True"/>
        </record>


        <record id="kw_resignation_dynamic_workflow_node_2" model="odoo.workflow.node">
            <field name="name">Forwarded</field>
            <field name="workflow_id" ref="kw_resignation_dynamic_workflow_main"/>
            <field name="node_name">forward</field>
            <field name="sequence" eval="3"/>
            <field name="is_visible" eval="False"/>
        </record>

        <record id="kw_resignation_dynamic_workflow_node_4" model="odoo.workflow.node">
            <field name="name">Approved</field>
            <field name="workflow_id" ref="kw_resignation_dynamic_workflow_main"/>
            <field name="node_name">confirm</field>
            <field name="sequence" eval="2"/>
            <field name="is_visible" eval="True"/>
        </record>

        <record id="kw_resignation_dynamic_workflow_node_5" model="odoo.workflow.node">
            <field name="name">Granted</field>
