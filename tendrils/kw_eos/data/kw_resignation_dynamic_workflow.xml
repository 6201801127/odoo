            <field name="node_name">grant</field>
            <field name="sequence" eval="4"/>
            <field name="is_visible" eval="True"/>
            <field name="flow_end" eval="True"/>
        </record>

        <record id="kw_resignation_dynamic_workflow_node_7" model="odoo.workflow.node">
            <field name="name">Applied For RL Cancellation</field>
            <field name="workflow_id" ref="kw_resignation_dynamic_workflow_main"/>
            <field name="node_name">waiting_for_rl_cancellation</field>
            <field name="sequence" eval="6"/>
            <field name="is_visible" eval="False"/>
            <field name="flow_end" eval="True"/>
