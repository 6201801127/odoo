<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <!-- model.name form view -->
        

        <!-- model.name tree view -->
        <record id="eos_QA_tree_view" model="ir.ui.view">
            <field name="name">EOS QA Tree</field>
            <field name="model">kw_eos_qa</field>
            <field name="priority" eval="48"/>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name" required="1"/>
                    <field name="question_type" required="1"/>
                    <field name="active" widget="boolean_toggle"/>
                </tree>
            </field>
        </record>

        <!-- kw_resignation_master action window -->
        <record id="eos_QA_action" model="ir.actions.act_window">
            <field name="name">EOS</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_eos_qa</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p><p>
                    Create a new record.
                </p>
            </field>
        </record>
        
    </data>
</odoo>