<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <!-- model.name form view -->
        <record id="kw_eos_log_view_form" model="ir.ui.view">
            <field name="name">EOS Log Form</field>
            <field name="model">kw_eos_log</field>
            <field name="priority" eval="46"/>
            <field name="arch" type="xml">
                <form string="NOC">
                    <sheet>
                        <group>
                            <group>
                                <field name="type"/>
                                <field name="response_res"/>
                                <field name="req_data"/>
                                <field name="mesg"/>
                                <field name="status"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- model.name tree view -->
        <record id="kw_eos_log_view_tree" model="ir.ui.view">
            <field name="name">EOS Log Tree</field>
            <field name="model">kw_eos_log</field>
            <field name="priority" eval="47"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="type"/>
                    <field name="response_res"/>
                    <field name="req_data"/>
                    <field name="mesg"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>

        <!-- kw_resignation_master action window -->
        <record id="kw_eos_log_action" model="ir.actions.act_window">
            <field name="name">EOS Log</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_eos_log</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available
                </p>
            </field>
        </record>
        
    </data>
</odoo>