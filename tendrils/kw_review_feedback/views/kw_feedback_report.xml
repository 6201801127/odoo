<odoo>
  <data>


  <!-- admin view -->
    <record model="ir.ui.view" id="kw_feedback_report_list_admin">
      <field name="name">kw_feedback Report list</field>
      <field name="model">kw_feedback</field>
      <field name="arch" type="xml">
        <tree create="false" delete="false" edit="false" import="false">
          <field name="admin_color_check" invisible='1'/>
          <!-- <field name="module_name" /> -->
          <field name="feedback_type"/>
          <field name="feedback_headline"/>
          <field name="computed_reply"/>
          <field name="create_uid"/>
          <field name="create_date"/>
          <field name="state"/>
        </tree>
      </field>
    </record>

  <!-- search -->
   <record id="kw_feedback_admin_report_search_view" model="ir.ui.view">
      <field name="name">kw_feedback Report Search View</field>
      <field name="model">kw_feedback</field>
      <field name="arch" type="xml">
          <search string="Search">
              <field name="state"/>
              <field name="create_uid"/>
              <field name="feedback_type"/>

                    <filter string="Inprogress" name="inprogress" domain="[('state','=','inprogress')]"
                            help="Records in state Inprogress"/>
                      <filter string="Close" name="inprogress" domain="[('state','=','close')]"
                            help="Records in state close"/>
                    <filter string="User ID" name="create_uid" context="{'group_by':'create_uid'}" help="Records in state close"/>
          </search>
      </field>
    </record>

   
    <record model="ir.actions.act_window" id="kw_feedback_report_action_window_admin">
      <field name="name">Feedback Report</field>
      <field name="res_model">kw_feedback</field>
      <field name="view_mode">tree</field>
      <field name="context">{'group_by':['module_name','feedback_type']}</field>
      <field name="view_ids" eval="[(5, 0, 0),
        (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_feedback_report_list_admin')})]"/><field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No report to view...
				</p>
			</field>
    </record>
  </data>
</odoo>