<odoo>
    <data>
        <record model="ir.ui.view" id="culture_monkey_log_list">
            <field name="name">culture monkey list</field>
            <field name="model">employee_list_log</field>
            <field name="arch" type="xml">
                <tree create="false" import="false" edit="false" decoration-danger="is_active != True">
                    <field name="full_name" invisible="1"/>
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="date_of_birth" invisible="1"/>
                    <field name="ph_no" invisible="1"/>
                    <field name="gender"/>
                    <field name="emp_id"/>
                    <field name="external_id" invisible="1"/>
                    <field name="email_addr"/>
                    <field name="date_joining"/>
                    <field name="team_name"/>
                    <field name="location_name"/>
                    <field name="designation"/>
                    <field name="level_emp"/>
                    <field name="is_active"/>
                    <field name="culture_monkey_id"/>
                    <field name="sync_required"/>
                    <field name="ex_employee_sync"/>
                    <button name="sync_data_log" string="Sync Employee" class="btn-primary" type="object"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="culture_monkey_log_search">
            <field name="name">culture monkey search</field>
            <field name="model">employee_list_log</field>
            <field name="arch" type="xml">
                <search>
                    <field name="full_name"/>
                    <field name="date_of_birth"/>
                    <field name="ph_no"/>
                    <field name="gender"/>
                    <field name="emp_id"/>
                    <field name="external_id"/>
                    <field name="email_addr"/>
                    <field name="date_joining"/>
                    <field name="team_name"/>
                    <field name="location_name"/>
                    <field name="designation"/>
                    <field name="level_emp"/>
                    <field name="culture_monkey_id"/>
                    <filter name="active_name" string="Active" domain="[('is_active','=',True)]"/>
                    <filter name="inactive_name" string="Archived" domain="[('is_active','=',False)]"/>

                </search>
            </field>
        </record>

        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="culture_monkey_log_action">
            <field name="name">Culture Monkey Employee List</field>
            <field name="res_model">employee_list_log</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="view_type">form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Records Found
                </p>
            </field>
        </record>


        <record model="ir.ui.view" id="culture_monkey_sync_list">
            <field name="name">culture monkey sync list</field>
            <field name="model">culture_monkey_sync_data</field>
            <field name="arch" type="xml">
                <tree create="false" import="false">
                    <field name="data_of_sync"/>
                    <field name="culture_monkey_id"/>
                    <field name="emp_id"/>
                    <field name="request_id"/>
                    <field name="response_id"/>
                    <field name="sync_required" groups="base.group_no_one"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="culture_monkey_sync_search">
            <field name="name">culture monkey sync search</field>
            <field name="model">culture_monkey_sync_data</field>
            <field name="arch" type="xml">
                <search>
                    <field name="data_of_sync"/>
                    <field name="culture_monkey_id"/>
                    <field name="emp_id"/>
                    <field name="request_id"/>
                    <field name="response_id"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="culture_monkey_log_sync_action">
            <field name="name">Culture Monkey Sync</field>
            <field name="res_model">culture_monkey_sync_data</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="view_type">form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Records Found
                </p>
            </field>
        </record>

        <menuitem name="Culture Monkey"
                  id="culture_monkey_log_menu"
                  parent="kw_surveys.kwantify_survey_menu_root"
                  groups='survey.group_survey_manager'
                  sequence="10"/>

        <menuitem name="Culture Monkey Sync"
                  id="culture_monkey_log_menu1"
                  parent="culture_monkey_log_menu"
                  sequence="2"/>
        <menuitem name="Employee List"
                  id="culture_monkey_log_menu_emp"
                  parent="culture_monkey_log_menu1"
                  action='culture_monkey_log_action'
                  sequence="3"/>

        <menuitem name="Sync Log Data"
                  id="culture_monkey_log_menu2"
                  parent="culture_monkey_log_menu1"
                  action='culture_monkey_log_sync_action'
                  sequence="4"/>

    </data>
</odoo>
