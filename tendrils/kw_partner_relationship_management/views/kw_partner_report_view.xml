<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>       

    <!-- Tree View -->
        <record model="ir.ui.view" id="view_kw_partner_report_list">
            <field name="name">Partner Report</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0'>
                    <field name="name"/>
                    <field name="partner_type_id"/>
                    <field name="city"/>
                    <field name="state_id"/>
                    <field name="country_id"/>
                    <field name="employee_strength"/>
                    <field name="average_turnover"/>
                    <field name="geography_of_business" widget="many2many_tags"/>
                    <field name="product_services_ids" invisible='1'/>
                    <field name="service_offering_ids" widget="many2many_tags"/>
                    <field name="tech_ids" widget="many2many_tags"/>
                    <field name="is_nda_uploaded"/>
                    <button name="btn_view_contacts" type="object" string="View Contacts" class="oe_highlight" attrs=""/>
                </tree>
            </field>
        </record>
       
        <!-- window action -->
        <record id="action_kw_partner_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Partner Report</field>
            <field name="res_model">res.partner</field>
            <field name="domain">[('is_partner','=',True),('partner_type_id','!=', False)]</field>
            <field name="context">{'is_partner':False, 'edit':False, 'create':False, 'partner_profile':True,'check_partner':True}</field>
            <field name="view_mode">tree</field>
            <field name="view_ids" eval="[(5, 0, 0),
        (0, 0, {'view_mode': 'tree', 'view_id': ref('view_kw_partner_report_list')})]"/>
            <field name="view_type">form</field>
        </record>


        <!-- DashBoard Tree View -->
        <record model="ir.ui.view" id="view_kw_partner_dashboard_report_list">
            <field name="name">Partner Dashboard Report</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0'>
                    <field name="partner_type_id" />
                    <field name="industy_domain_ids" widget="many2many_tags"/>
                    <field name="geography_of_business" widget="many2many_tags"/>
                    <field name="oppertunity_id" widget="many2many_tags"/>
                    <field name="tech_ids" widget="many2many_tags"/>
                    <field name="service_offering_ids" widget="many2many_tags"/>
                </tree>
            </field>
        </record>
       
        <!-- window action -->
        <record id="action_kw_partner_dashboard_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Dashboard</field>
            <field name="res_model">res.partner</field>
            <field name="domain">[('is_partner','=',True)]</field>
            <field name="context">{'is_partner':False, 'edit':False, 'create':False, 'partner_profile':True,'check_partner':True, 'group_by':'partner_type_id'}</field>
            <field name="view_mode">tree</field>
            <field name="view_ids" eval="[(5, 0, 0),
        (0, 0, {'view_mode': 'tree', 'view_id': ref('view_kw_partner_dashboard_report_list')})]"/>
            <field name="view_type">form</field>
        </record>


    </data>
</odoo>
