                (0, 0, {'view_mode': 'kanban', 'view_id': ref('kw_office_view_kanban')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    There is no documents..
                </p>
            </field>
        </record>
        
        <menuitem id="business_rule_menu"
                  name="Sales Processes"
                  parent="menu_handbook_child_root"
                  action="kw_business_actions_window"
                  sequence="2"/>



