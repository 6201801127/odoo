<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!--Templates-->
        <record model="ir.actions.act_window" id="templates_actions_window">
            <field name="name">Templates</field>
            <field name="res_model">kw_onboarding_handbook</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_type">form</field>
            <field name="domain">[('handbook_type_id.code','=','TEM')]</field>
            <field name="context">{'default_handbook_type_id':'TEM'}</field>
            <field name="view_ids" eval="[(5, 0, 0), 
                (0, 0, {'view_mode': 'kanban', 'view_id': ref('kw_office_view_kanban_template')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('kw_handbook_template_form_view')}),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_handbook_tree_view_template')})]"/>
        </record>

        <menuitem id="kw_onboarding_handbook_templates_menu"
		          name="Templates"
		          parent="kw_onboarding_handbook_parent_menu"
		          action="templates_actions_window"
                  groups="kw_handbook.group_kw_handbook_manager,kw_handbook.groups_handbook_officer"
		          sequence="9"/>

