    <!-- Filter Start -->
        <record id="kw_group_view_search" model="ir.ui.view">
            <field name="name">kw_group_search</field>
            <field name="model">res.groups</field>
            <field name="inherit_id" ref="base.view_groups_search"/>
            <field name="arch" type="xml">
                <xpath expr="//search" position="inside">
                <filter name="generic_group" string="Generic Group" domain="[('category_id','=',False)]" />
                <filter name="application_group" string="Application Group" domain="[('category_id','!=',False)]" />
                </xpath>
            </field>
        </record>
    <!-- Filter End -->

