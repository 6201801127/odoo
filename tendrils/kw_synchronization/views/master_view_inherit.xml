<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    <!-- Designation View inherit start -->
        <record id="view_job_form_inherit" model="ir.ui.view">
            <field name="name">HR Job Inherit Form</field>
            <field name="model">hr.job</field>
            <field name="inherit_id" ref="hr.view_hr_job_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/group/group/field[@name='no_of_recruitment']" position="after">
                    <field name="kw_id" groups="hr.group_hr_manager"/>
                    <field name="sync_status" readonly='1' groups="hr.group_hr_manager"/>
                </xpath>
            </field>
        </record>  

        <record id="kw_hr_job_tree_in" model="ir.ui.view">
			<field name="model">hr.job</field>
			<field name="inherit_id" ref="hr.view_hr_job_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='state']" position="after">
					<field name="sync_status" groups="hr.group_hr_manager"/>
                    <field name="kw_id" readonly='1' groups="base.group_no_one"/>
				</xpath>
			</field>
		</record>
    <!-- Designation View inherit end -->

    <!-- Grade view inherit Start -->
        <record id="view_grade_form_inherit" model="ir.ui.view">
            <field name="name">Grade Inherit Form</field>
            <field name="model">kwemp_grade</field>
            <field name="inherit_id" ref="kw_employee.kwemp_grade_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/group/group[1]/field[@name='band_id']" position="after">
                    <field name="sync_status" readonly='1' groups="hr.group_hr_manager"/>
                </xpath>
            </field>
        </record>  

        <record id="kw_grade_tree_in" model="ir.ui.view">
			<field name="model">kwemp_grade</field>
			<field name="inherit_id" ref="kw_employee.kwemp_grade_list"/>
			<field name="arch" type="xml">
