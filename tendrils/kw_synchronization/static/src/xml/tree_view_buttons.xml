<?xml version="1.0" encoding="UTF-8"?>

<template id="country_sync_template" xml:space="preserve">
    <t t-name="ListView.buttons" t-extend="ListView.buttons">
        <t t-jquery='.o_list_button_discard' t-operation='after'>
            <t t-if="widget.modelName=='res.country'">
                <button type="button" class="btn btn-primary oe_action_sync_country_button" accesskey="t">Country Sync</button>
            </t>
            <t t-if="widget.modelName=='hr.employee'">
                <button type="button" class="btn btn-primary  oe_action_sync_employee_button" accesskey="t">Sync Employee</button>
            </t>
			<t t-if="widget.modelName=='hr.job'">
                <button type="button" class="btn btn-primary oe_action_sync_designation_button" accesskey="t">Designation Sync</button>
            </t>
            <t t-if="widget.modelName=='kw_location_master'">
                <button type="button" class="btn btn-primary oe_action_sync_location_button" accesskey="t">Location Sync</button>
            </t>
			<!-- Button For technical category master START -->
			<t t-if="widget.modelName=='kwemp_technical_category'">
                <button type="button" class="btn btn-primary oe_action_sync_tech_skill_category_button" accesskey="t">Technical Category Sync</button>
            </t>
			<!-- Button For technical category master END -->

			<!-- Button For technical skill START -->
			<t t-if="widget.modelName=='kwemp_technical_skill'">
                <button type="button" class="btn btn-primary oe_action_sync_tech_skill_button" accesskey="t">Skill Sync</button>
            </t>
			<!-- Button For technical END -->

            <!-- Button For GradeBand START -->
			<t t-if="widget.modelName=='kwemp_grade'">
                <button type="button" class="btn btn-primary oe_action_sync_grade_button" accesskey="t">Grade Sync</button>
            </t>
			<!-- Button For GradeBand END -->

            <!-- Button For Employeemeny Type START -->
			<t t-if="widget.modelName=='kwemp_employment_type'">
                <button type="button" class="btn btn-primary oe_action_sync_emp_type_button" accesskey="t">Sync</button>
            </t>
			<!-- Button For Employeement Type END -->

            <!-- Button For stream master START -->
			<t t-if="widget.modelName=='kwmaster_stream_name'">
                <button type="button" class="btn btn-primary oe_action_sync_stream_button" accesskey="t">Stream Sync</button>
            </t>
			<!-- Button For stream master END -->

            <!-- Button For Specialization master START -->
			<t t-if="widget.modelName=='kwmaster_specializations'">
                <button type="button" class="btn btn-primary oe_action_sync_specialization_button" accesskey="t">Stream Sync</button>
            </t>
			<!-- Button For Specialization master END -->

            <!-- Button For Employee Categotry master START -->
			<t t-if="widget.modelName=='kwmaster_category_name'">
                <button type="button" class="btn btn-primary oe_action_sync_category_button" accesskey="t">Category Sync</button>
            </t>
			<!-- Button For Employee Category master END -->

            <!-- Button For Office unit master START -->
			<!-- <t t-if="widget.modelName=='kw_res_branch'">
                <button type="button" class="btn btn-primary oe_action_sync_branch_button" accesskey="t">Branch Sync</button>
            </t> -->
			<!-- Button For Office unit master END -->

            <!-- Button For Language master START -->
			<t t-if="widget.modelName=='kwemp_language_master'">
                <button type="button" class="btn btn-primary oe_action_sync_language_button" accesskey="t">Language Sync</button>
            </t>
			<!-- Button For Language master END -->
        </t>
    </t>
</template>
