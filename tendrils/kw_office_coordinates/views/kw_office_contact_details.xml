<odoo>
  <data>

    <!-- explicit tree view of kw office contact details : Opening -->
    <record model="ir.ui.view" id="kw_phone_master_list">
      <field name="name">Phone Number Master</field>
      <field name="model">kw_office_phone</field>
      <field name="arch" type="xml">
        <tree>
          <field name="isd_code" string="ISD Code"></field>
          <field name="tele_ph_num" string="Telephone Number"></field>
        </tree>
      </field>
    </record>
    <!-- explicit tree view of kw office contact details : Ending -->

    <!-- explicit Form view of kw office contact details : Opening -->
    <record id="kw_phone_master_form_view" model="ir.ui.view">
      <field name="name">Phone Number Master</field>
      <field name="model">kw_office_phone</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <label for="telphno" string="Tel. No" />
              <div class="address_format">
                <field name="isd_code" style="width: 30%" />
                <field name="tele_ph_num" style="width: 70%" />
              </div>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <!-- explicit form view of kw office contact details : Ending -->

    <!-- kw office contact details actions window :Opening -->
    <record model="ir.actions.act_window" id="kw_phone_master_action_window">
      <field name="name">Phone Number Master</field>
      <field name="res_model">kw_office_phone</field>
      <field name="view_mode">tree,form</field>
    </record>
    <!-- kw office contact details actions window :Ending -->


    <!-- Top menu item
    <menuitem name="Phone Number Master" id="kw_phno_master_menu"/>
    <menuitem name="Phone Number" id="kw_phone_master_child_menu" parent='kw_phno_master_menu' sequence="2" action="kw_phone_master_action_window" /> -->
  </data>
</odoo>