        <!-- Menu -->
        <menuitem name="Update Employee Document" id="kw_emp_update_doc_menu" parent="hr.menu_human_resources_configuration" groups="base.group_no_one"/>
        <menuitem name="Update Workexperience" id="kw_emp_update_workexperience_menu" parent="kw_emp_update_doc_menu" action="kw_emp_update_workexperience_action_window"  groups="base.group_no_one" sequence='2'/>
        <menuitem name="Update Educational details" id="kw_emp_update_educational_details_menu" parent="kw_emp_update_doc_menu" action="kw_emp_update_educational_details_action_window"  groups="base.group_no_one" sequence='3'/>
