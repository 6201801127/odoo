            </field>
        </record>
        <record id="kw_emp_update_educational_details_search" model="ir.ui.view">
            <field name="model">kw_emp_update_educational_details</field>
            <field name="arch" type="xml">
                <search>
                    <field name="status"/>
                    <field name="doc_id"/>
                    <field name="url"/>
