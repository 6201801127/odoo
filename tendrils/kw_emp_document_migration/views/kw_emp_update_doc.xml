            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_emp_update_educational_details_list')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No record to show...
                </p>
            </field>
        </record>

        <!-- Employee Identification details tree view definition Opening -->
