<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Employee workexperience tree view definition Opening -->
        <record model="ir.ui.view" id="kw_emp_update_workexperience_list">
            <field name="name">Updated Employee workexperience</field>
            <field name="model">kw_emp_update_workexperience</field>
            <field name="arch" type="xml">
                <tree create='true' edit='true' delete='true' import='true'>
                    <field name="doc_id" force_save='1'/>
                    <field name="url"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>
        <record id="kw_emp_update_workexperience_search" model="ir.ui.view">
            <field name="model">kw_emp_update_workexperience</field>
            <field name="arch" type="xml">
                <search>
                    <field name="status"/>
                    <field name="doc_id"/>
                    <field name="url"/>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="kw_emp_update_workexperience_action_window">
            <field name="name">Updated Employee workexperience</field>
            <field name="res_model">kw_emp_update_workexperience</field>
            <field name="view_mode">tree</field>
            <field name="search_view_id" ref="kw_emp_update_workexperience_search"/>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_emp_update_workexperience_list')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No record to show...
                </p>
            </field>
        </record>

        <!-- Employee Educational details tree view definition Opening -->
