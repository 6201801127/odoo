<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="sequence_kw_recruitment_budget" model="ir.sequence">
            <field name="name">Recruitment Budget Sequence</field>
            <field name="code">self.rb_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field> 
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
        </record>
        
        <record id="sequence_kw_recruitment_calendar" model="ir.sequence">
            <field name="name">Recruitment Calendar Sequence</field>
            <field name="code">self.rc_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field> 
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="sequence_kw_project_budget" model="ir.sequence">
            <field name="name">Project Budget Sequence</field>
            <field name="code">self.pb_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field> 
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="sequence_codification" model="ir.sequence">
            <field name="name">Treasury Budget Sequence</field>
            <field name="code">self.tr_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field> 
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
        </record>

        
    </data>
</odoo>