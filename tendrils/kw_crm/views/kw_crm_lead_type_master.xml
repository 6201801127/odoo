<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="kw_crm_lead_type_master_tree" model="ir.ui.view">
            <field name="name">kw_crm_lead_type_master.tree</field>
            <field name="model">kw_crm_lead_type_master</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name" />
                    <field name="code"/>
                    <field name="active" widget="boolean_toggle"/>
                </tree>
            </field>
        </record>

        <record id="kw_crm_lead_type_master_act_window" model="ir.actions.act_window">
            <field name="name">Lead type Master</field>
            <field name="res_model">kw_crm_lead_type_master</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>

        <menuitem name="Lead Type Master" id="menu_lead_type_master" parent="menu_lead_config"
        action="kw_crm_lead_type_master_act_window"/>

       
    </data>
</odoo>
