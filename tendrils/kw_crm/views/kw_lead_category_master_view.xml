<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

      
        <record id="kw_lead_category_master_form" model="ir.ui.view">
            <field name="name">kw_lead_category_master.form</field>
            <field name="model">kw_lead_category_master</field>
            <field name="arch" type="xml">
                <form >
                    <sheet>
                    <group>              
                        <group>
                            <field name="name" />
                            <field name="project_id" options="{'no_create_edit': True,'no_open':True,'no_create':True}"/>
                        </group>
                    </group>   
                    </sheet>
                </form>
            </field>
        </record>

        <record id="kw_lead_category_master_tree" model="ir.ui.view">
            <field name="name">kw_lead_category_master.tree</field>
            <field name="model">kw_lead_category_master</field>
            <field name="arch" type="xml">
                <tree >
                    <field name="name" />
                    <field name="project_id" />
                    <field name="active" widget="boolean_toggle" />
                </tree>
            </field>
        </record>

        <record id="kw_lead_category_master_act_window" model="ir.actions.act_window">
            <field name="name">Product Interest Category Master</field>
            <field name="res_model">kw_lead_category_master</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="view_ids" eval="[(5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_lead_category_master_tree')}),
            (0, 0, {'view_mode': 'form', 'view_id': ref('kw_lead_category_master_form')})]"/>
        </record>


        <menuitem name="Product Lead Master" id="menu_lead_master" parent="menu_lead_config"
        action="kw_lead_category_master_act_window"/>

       
    </data>
</odoo>
