<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_rcm_requisition" model="ir.sequence">
            <field name="name">Requisition Request</field>
            <field name="code">kw_rcm</field>
            <field name="prefix">RCM/%(y)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
         <record id="kw_unit_bench_to_central_bench" model="ir.cron">
            <field name="name">Central Bench : Unit Bench To Central Bench</field>
            <field name="model_id" ref="kw_resource_management.model_unit_bench_resource"/>
            <field name="state">code</field>
            <field name="code">model.unit_to_central_bench()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">1</field>
            <field name="active" eval="False" />
        </record>
         <record id="update_central_bench_ctc_count_cron" model="ir.cron">
            <field name="name">Central Bench : Create/Update Central Bench CTC Count</field>
            <field name="model_id" ref="kw_resource_management.model_central_bench_weekly_ctc_report"/>
            <field name="state">code</field>
            <field name="code">model.update_central_ctc_count()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">2</field>
            <field name="active" eval="False" />
        </record>


        <record id="kw_resource_mapping_data_sync_cron" model="ir.cron">
            <field name="name">Resource Data: Resource Mapping Data</field>
            <field name="model_id" ref="kw_resource_management.model_kw_resource_mapping_data"/>
            <field name="state">code</field>
            <field name="code">model.sync_resource_mapping_data()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">3</field>
            <field name="active" eval="False"/>
        </record>
        <record id="kw_emp_skill_update" model="ir.cron">
            <field name="name">Resource Skill : Resource Skill Updatation</field>
            <field name="model_id" ref="kw_resource_management.model_resource_skill_data"/>
            <field name="state">code</field>
            <field name="code">model.update_emp_skill_on_resource_skill()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">20</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">1</field>
            <field name="active" eval="False" />
        </record>

    </data>
</odoo>
