<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="central_bench_weekly_ctc_report_list">
            <field name="name">Central Bench Weekly CTC count Report</field>
            <field name="model">central_bench_weekly_ctc_report</field>
            <field name="arch" type="xml">
                <tree create='false' edit='false' import='false' delete='false'>
                    <field name="technology_id"/>
                    <field name="year_data"/>
                    <field name="month" invisible='1'/>
                    <field name="week" invisible='1'/>
                    <field name="month_selection" force_save="1" />
                    <field name="week_selection" force_save="1" />
                    <field name="count"/>
                    <field name="week_start" invisible='1'/>
                    <field name="week_end" invisible='1'/>
                    <field name="total_ctc" force_save="1"/>
                </tree>
            </field>
        </record>


        <record id="central_bench_weekly_ctc_report_pivot_action" model="ir.actions.act_window">
            <field name="name">Central Bench Weekly CTC count Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">central_bench_weekly_ctc_report</field>
            <field name="view_mode">tree</field>
            <field name="context">{'search_default_group_by_tech_wise':1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                </p>
                <p>
                </p>
            </field>
        </record>
        <!-- <menuitem id="central_bench_weekly_ctc_menu" name="Central Bench Weekly CTC count" 
        parent="sbu_parent_menu" action="kw_resource_management.central_bench_head_count_report_action" sequence="100" groups="kw_resource_management.group_budget_manager" /> -->

    </data>
</odoo>