			<field name="type">ir.actions.act_window</field>
			<field name="res_model">kw_resource_mapping_data</field>
			<field name="view_mode">tree,search</field>
			<field name="view_type">form</field>
			<field name="domain">[]</field>

		</record> -->

	<record id="kw_resource_mapping_data_action" model="ir.actions.server">
      <field name="name">Resource Mapping</field>
      <field name="model_id" ref="kw_resource_management.model_kw_resource_mapping_data"/>
      <field name="state">code</field>
      <field name="code">
action=model.get_resource_mapping_data()
      </field>
    </record>
  
		<menuitem id="kw_resource_mapping_data_menu"
				  name="Resource Mapping"
				  action="kw_resource_mapping_data_action"
				  parent="kw_resource_management.sbu_parent_menu"
				  sequence="22"
				  active='False'
		/>
		<record id="kw_resource_quantity_update_action" model="ir.actions.act_window">
			<field name="name">Update Resource Quantity</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">kw_resource_mapping_data</field>
			<field name="view_mode">tree,search</field>
			<field name="view_type">form</field>
		</record>
		<menuitem id="kw_resource_quantity_update_menu"
				  name="Update Resource Quantity"
				  action="kw_resource_quantity_update_action"
				  parent="hr_timesheet.menu_timesheets_reports"
				  groups='group_sbu_representative'
				  sequence="999"/>

