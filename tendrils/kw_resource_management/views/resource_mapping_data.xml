<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="kw_resource_mapping_data_tree" model="ir.ui.view">
			<field name="name">Resource Mapping Report</field>
			<field name="model">kw_resource_mapping_data</field>
			<field name="arch" type="xml">
				<tree create='0' edit='0' delete='1' import="0">
					<!-- <field name="sl_no"/> -->
					<field name="csg_id"/>
					<field name="account_holder_id"/>
					<field name="opp_code"/>
