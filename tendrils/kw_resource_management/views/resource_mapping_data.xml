					<field name="work_location"/>   
					<field name="technology"/>
					<field name="designation"/>
					<field name="desg_type"/>
					<field name="resource_no"/>
					<field name="resource_available"/>
					<field name="resource_required"/>
					<field name="from_date"/>
					<field name="to_date"/>
					<field name="kw_sbu_id" invisible='1'/>
					<field name="sbu_name" invisible='1'/>
					<field name="check_representative" invisible='1'/>
					<field name="opportunity_status"  invisible='1'/>
					<button string="Update Quantity" name="update_quantity" type="object" class="btn-primary" 
					attrs="{'invisible': [('check_representative', '=', False)]}"/>
					<!-- <field name="action_status"/>
					<field name="desgtype"/>
					<field name="experience"/>
					<field name="min_experience"/>
					<field name="month_bill"/>
					<field name="engagement_type"/>
					<field name="qualification_id"/>
					<field name="resource_id"/>
					<field name="no_of_month"/>
					<field name="project_type_name"/>
					<field name="fiscal_yr"/>
					<field name="prof_qualification"/>
					<field name="qualification"/>
					<field name="acc_holder"/> 
					<field name="technology_id"/>  
					<field name="user_name"/>
					<field name="technology"/> -->
				</tree>
			</field>
		</record>
		<record id="kw_resource_mapping_data_search" model="ir.ui.view">
            <field name="name">Resource Mapping search View</field>
            <field name="model">kw_resource_mapping_data</field>
            <field name="arch" type="xml">
                <search string="Resource Mapping Search">
					<field name="module_short_name"/>
					<field name="technology"/>
					<field name="designation"/>
					<field name="desg_type"/>
					<field name="resource_no"/>
					<field name="tagged_res" invisible="True"/>
					<field name="from_date"/>
					<field name="to_date"/>
                </search>
            </field>
        </record>
		
		<!-- kw_contract_end_log action window -->
