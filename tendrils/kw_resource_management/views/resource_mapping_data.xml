	<record id="kw_resource_mapping_action_id" model="ir.actions.server">
      <field name="name">Resource Mapping</field>
      <field name="model_id" ref="kw_resource_management.model_kw_resource_mapping_data"/>
      <field name="state">code</field>
      <field name="code">
action=model.get_resource_mapping_data_view()
      </field>
    </record>


		<menuitem id="kw_resource_mapping_data_dev_menu"
				  name="Resource Mapping"
				  parent="hr_timesheet.menu_timesheets_reports"
				  action="kw_resource_mapping_action_id"
				  groups='disable_debug_mode.group_developer'
				  sequence="1000"
		/> 
	</data>
	
