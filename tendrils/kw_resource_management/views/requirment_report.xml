<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="requirment_report_list_tree" model="ir.ui.view">
			<field name="name">Requirment Report List</field>
			<field name="model">requirment_report_applicants</field>
			<field name="arch" type="xml">
				<tree create='false' import='false'>
					<field name="type_project"/>
					<field name="code"/>
					<field name="state"/>
					<field name="dept" />
					<field name="project"/>
					<field name="skill"/>
                    <field name="job_position"/>
					<field name="branch_id"/>
                    <field name="experience"/>
                    <field name="resource"/>
                    <field name="employee_id"/>
                    <field name="no_of_resource"/>
                    <field name="date_of_joining"/>
				</tree>
			</field>
		</record>

        <record id="requirment_report_list_action" model="ir.actions.act_window">
			<field name="name">Requirment Report</field>
			<field name="res_model">requirment_report_applicants</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No records found...
				</p>
			</field>
		</record>
		<menuitem id="requirment_report_menu"
				  name="Requirment Report"
				  action="requirment_report_list_action"
				  parent="sbu_parent_menu"
				  sequence='10'
				  
		/>
    </data>
</odoo>