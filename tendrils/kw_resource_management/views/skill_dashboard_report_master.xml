<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="skill_dashboard_analytics_report_tree" model="ir.ui.view">
            <field name="name">skill_dashboard_analytics_report.tree</field>
            <field name="model">skill_dashboard_analytics_report</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0' delete='0' import='0'>
                    <field name="emp_id"/>
                    <field name="emp_name" invisible='1'/>
                    <field name="designation"/>
                    <field name="club_role"/>
                    <field name="emp_location"/>
                    <field name="emp_role"/>
                    <field name="emp_category"/>
                    <field name="employement_type"/>
                    <field name="sbu_type"/>
                    <field name="sbu_master_id"/>
                    <field name="primary_skill"/>
                    <!-- <field name="primary_skill_type"/> -->
                    <field name="secondary_skill"/>
                    <!-- <field name="secondary_skill_type"/> -->
                    <field name="tertiarry_skill"/>
                    <!-- <field name="tertiary_skill_type"/> -->
                    <field name="certification_course_1"/>
                    <field name="certification_course_2"/>
                    <field name="certification_course_3"/>
                    <field name="certification_course_4"/>
                    <field name="certification_course_5"/>

                </tree>
            </field>
        </record>

        <record id="skill_dashboard_analytics_report_search_view" model="ir.ui.view">
            <field name="name">skill_dashboard_analytics_report search view</field>
            <field name="model">skill_dashboard_analytics_report</field>
            <field name="arch" type="xml">
                <search>
                    <field name="emp_id" string='Employee'/>
                    <field name="designation" string="Department"/>
                    <field name="emp_location" string='Location'/>
                    <field name="emp_role" string="Emp Role"/>
                    <field name="emp_category" string="Emp Category"/>
                    <field name="employement_type" string="Emp Type"/>
                    <field name="sbu_type" string="SBU Type"/>
                    <field name="sbu_master_id" string="SBU Name"/>
                    <field name="primary_skill" string="Primary Skill"/>
                    <field name="secondary_skill" string="Secondary Skill"/>
                    <field name="tertiarry_skill" string="Tertiarry Skill"/>
                </search>
            </field>
        </record>

        <!-- kw_contract_end_log action window -->
        <record id="skill_dashboard_analytics_report_action" model="ir.actions.act_window">
            <field name="name">Work Force Analytics Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">skill_dashboard_analytics_report</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>
        
    </data>
</odoo>