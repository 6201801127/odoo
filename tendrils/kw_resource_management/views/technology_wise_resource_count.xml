<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="technology_wise_resource_count_report_tree" model="ir.ui.view">
			<field name="name">technology_wise_resource_count_report.tree</field>
			<field name="model">technology_wise_resource_count_report</field>
			<field name="arch" type="xml">
				<tree create='false' delete='false'>
                    <field name="id" string='Sl No.' class="text-center" invisible="1"/>
                    <field name="technology_id"/>
					<!-- <field name="sbu_name" /> -->
					<field name="team_lead_count" sum="team_lead_count"/>
					<field name="developer_count" sum="developer_count"/>
					<button name="view_details_button" type="object" string="View Details" class="oe_highlight"/>
				</tree>
			</field>
		</record>
        
		<record id="technology_wise_resource_count_report_search" model="ir.ui.view">
			<field name="name">technology_wise_resource_count_report.search</field>
			<field name="model">technology_wise_resource_count_report</field>
			<field name="arch" type="xml">
				<search string="Search Description">
                    <field name="technology_id"/>
					<!-- <field name="sbu_name" /> -->
				</search>
			</field>
		</record>

		<record id="technology_wise_resource_count_report_action" model="ir.actions.act_window">
			<field name="name">Technology wise Report</field>
			<field name="res_model">technology_wise_resource_count_report</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			
			
		</record>
	</data>
</odoo>