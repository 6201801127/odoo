<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="resource_wise_project_tree" model="ir.ui.view">
			<field name="name">Resource Wise Report</field>
			<field name="model">resource_wise_project</field>
			<field name="arch" type="xml">
				<tree create='0' edit='0' delete='0'>
					<field name="code"/>
					<field name="employee_id"/>
					<field name="designation"/>
					<field name="emp_category"/>
					<field name="employement_type"/>
					<field name="sbu_id"/>
					<field name="project_name"/>
					<field name="emp_sbu_id" invisible='1'/>
				</tree>
			</field>
		</record>
		
		<record id="resource_wise_project_search" model="ir.ui.view">
			<field name="name">Resource Wise Report Search</field>
			<field name="model">resource_wise_project</field>
			<field name="arch" type="xml">
				<search>
					<!-- Add your fields here -->
					<field name="employee_id"/>
					<!-- <field name="code"/>
					<field name="designation"/>
					<field name="emp_category"/>
					<field name="employement_type"/>
					<field name="sbu_id"/>
					<field name="project_name"/> -->
					<filter string="Tagged" domain="[('project_name','!=',False)]" name="sbu_tagged"/>
					<filter string="Un Tagged" domain="[('project_name','=',False)]" name="sbu_untagged"/>
				</search>
			</field>
		</record>
			

		<!-- kw_contract_end_log action window -->
		<record id="resource_wise_project_action" model="ir.actions.act_window">
			<field name="name">Resource Wise Report</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">resource_wise_project</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<!-- <field name="context">{'search_default_order_oppurtinity':1}</field> -->
		</record>

	</data>
</odoo>