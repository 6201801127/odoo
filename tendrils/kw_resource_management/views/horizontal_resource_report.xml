<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="horizontal_resource_tree" model="ir.ui.view">
            <field name="name">horizontal resource tree view</field>
            <field name="model">horizontal_wise_resource</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0' delete='0'>
                    <!-- <field name="id" string="Sl.No"/> -->
                    <field name="code"/>
                    <field name="name"/>
                    <field name="employee_id" invisible='1'/>
                    <field name="designation"/>
                    <field name="date_of_joining"/>
                    <field name="emp_role"/>
                    <field name="primary_skill_id"/>
                    <field name="emp_category"/>
                    <field name="employement_type"/>
                     <field name="sbu_type" />
					<field name="sbu_name"/>
                    <field name="sbu_id" invisible='1'/>
                    <field name="job_branch_id"/>
                    <field name="project_name"/>
                </tree>
            </field>
        </record>


        <!-- kw_contract_end_log action window -->
        <record id="horizontal_resource_action" model="ir.actions.act_window">
            <field name="name">Horizontal Resource Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">horizontal_wise_resource</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
    </odoo>