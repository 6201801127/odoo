<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="work_force_analytics_report_tree" model="ir.ui.view">
            <field name="name">work_force_analytics_report.tree</field>
            <field name="model">work_force_analytics_report</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0' delete='0'>
                    <field name="emp_id" invisible='1'/>
                    <field name="emp_code"/>
                    <field name="emp_name" />
                    <field name="department_id"/>
                    <field name="job_branch_id"/>
                    <field name="designation_id"/>
                    <field name="emp_role"/>
                    <!-- <field name="club_role"/> -->
                    <field name="emp_category"/>
                    <field name="employement_type"/>
                    <field name="sbu_type"/>
                    <field name="sbu_master_id"/>
                    <field name="primary_skill_id"/>
                </tree>
            </field>
        </record>

        <record id="work_force_analytics_report_search_view" model="ir.ui.view">
            <field name="name">work_force_analytics_report search view</field>
            <field name="model">work_force_analytics_report</field>
            <field name="arch" type="xml">
                <search>
                    <field name="emp_id" string='Employee'/>
                    <field name="department_id" string="Department"/>
                    <field name="job_branch_id" string='Location'/>
                    <field name="emp_role" string="Emp Role"/>
                    <field name="emp_category" string="Emp Category"/>
                    <field name="employement_type" string="Emp Type"/>
                    <field name="sbu_type" string="SBU Type"/>
                    <field name="sbu_master_id" string="SBU Name"/>
                    <field name="primary_skill_id" string="Skill"/>
                </search>
            </field>
        </record>

        <!-- kw_contract_end_log action window -->
        <record id="work_force_analytics_report_action" model="ir.actions.act_window">
            <field name="name">Work Force Analytics Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">work_force_analytics_report</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>
        
    </data>
</odoo>