<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sbu_tag_untag_log_view_form" model="ir.ui.view">
        <field name="name">Sbu Tag Untag Log Form</field>
        <field name="model">sbu_tag_untag_log</field>
        <field name="arch" type="xml">
            <tree create="false" import="false">
                <field name="employee_id"/>
                <field name="status"/>
                <field name="date"/>
                <field name="action_by"/>
                <field name="sbu_status"/>
                <field name="sbu_type"/>
                <field name="sbu_id"/>
                
                
            </tree>
        </field>
    </record>
    <record id="sbu_tag_untag_log_action" model="ir.actions.act_window">
        <field name="name">SBU Tagging History</field>
        <field name="res_model">sbu_tag_untag_log</field>
        <field name="view_mode">tree</field>    
       
    </record>

    <record id="sbu_tag_untag_mail_log_view_form" model="ir.ui.view">
        <field name="name">Sbu Tag Untag Log Form</field>
        <field name="model">sbu_tag_untag_mail_log</field>
        <field name="arch" type="xml">
           <tree create="false" import="false">
                <field name="employee_id" invisible="1"/>
                <field name="employee_name" string = "Employee Name"/>
                <field name="designation"/>
                <field name="joining_date"/>
                <field name="technology"/>
                <field name="department"/>
                <field name="location"/>
                <field name="type_of_employee"/>
                <!-- <field name="resource"/> -->
                <!-- <field name="replacement_of"/> -->
                <!-- <field name="experience"/> -->
                <!-- <field name="qualification"/> -->
                <!-- <field name="planned_project"/> -->
                <field name="date"/>
            </tree>
        </field>
    </record>

    <record id="sbu_tag_untag_mail_log_action" model="ir.actions.act_window">
        <field name="name">SBU Tagging Mail History</field>
        <field name="res_model">sbu_tag_untag_mail_log</field>
        <field name="view_mode">tree</field>    
    </record>




