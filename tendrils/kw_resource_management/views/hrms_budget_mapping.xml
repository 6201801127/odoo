		<record id="outsource_hrms_budget_mapping_search_view" model="ir.ui.view">
			<field name="name">outsource_hrms_budget_mapping search view</field>
			<field name="model">outsource_hrms_budget_mapping</field>
			<field name="arch" type="xml">
				<search>
