<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="hrms_budget_mapping_tree" model="ir.ui.view">
			<field name="name">hrms_budget_mapping.tree</field>
			<field name="model">hrms_budget_mapping</field>
			<field name="arch" type="xml">
				<tree create='0' edit='0' delete='0' decoration-danger="applied_eos == True">
					<!-- <field name="id" string="Sl.No"/> -->
					<field name="emp_code"/>
					<field name="employee_id" invisible='1'/>
					<field name="name"/>
					<field name="designation"/>
					<field name="date_of_joining"/>
					<field name="department_id"/>
					<field name="emp_role"/>
					<field name="emp_category"/>
					<field name="job_branch_id"/>
					<field name="engagement_unit"/>
					<field name="project_manager"/>
					<field name="workorder_code"/>
					<field name="project_name"/>
					<field name="tagging_start_date"/>
					<field name="tagging_end_date"/>
					<field name="primary_skill_id"/>
					<field name="edu_qualification"/>
					<field name="certification"/>
					<field name="csm_experience_display"/>
					<field name="previous_company_experience_display_abbr"/>
					<field name="total_experience_display"/>
					<field name="joining_type"/>
					<field name="employement_type"/>
					<field name="budget_type"/>
					<field name="division" invisible='1'/>
					<field name="emp_grade_band"/>
					<field name="parent_id"/>
					<field name="gender"/>
					<field name="applied_eos" invisible='1'/>
					<field name="category_kw_id" invisible='1'/>
					<!-- <field name="budget_type"/> -->
				</tree>
			</field>
		</record>

		<record id="hrms_budget_mapping_search_view" model="ir.ui.view">
			<field name="name">hrms_budget_mapping search view</field>
			<field name="model">hrms_budget_mapping</field>
			<field name="arch" type="xml">
				<search>
					<field name="employee_id" string='Employee'/>
					<field name="designation" string="Employee Designation"/>
					<field name="emp_category" string=' Employee Category'/>
					<field name="emp_role" string='Employee Role'/>
					<field name="employement_type" string="Employment Type"/>
					<field name="job_branch_id" string='Location'/>
					<!-- <field name="budget_type" string='Budget Type'/> -->
					<!-- <separator string='Employee Role'/>
					<filter string="Role - Consultancy Services" domain="[('role_kw_id','=',4)]" name="Consultancy"/>
					<filter string="Role - Overhead" domain="[('role_kw_id','=',2)]" name="Overhead"/>
					<filter string="Role - Resource Deployment" domain="[('role_kw_id','=',5)]" name="Resource"/>
					<filter string="Role - Software Solution" domain="[('role_kw_id','=',1)]" name="Software"/> -->
					<!-- <filter string="Technical Content writer/Test role" domain="[('role_kw_id','=',0)]" name="Technical"/> -->
					<!-- <separator string='Employee Category'/>
					<filter string="Coding" domain="[('category_kw_id','=',3)]" name="Coding"/>
					<filter string="Delivery" domain="[('category_kw_id','=',7)]" name="Delivery"/>
					<filter string="Project Management" domain="[('category_kw_id','=',1)]" name="Management"/>
					<filter string="Team/Tech Lead" domain="[('category_kw_id','=',19)]" name="Team"/> -->
				</search>
			</field>
		</record>

		<!-- kw_contract_end_log action window -->
		<record id="hrms_budget_mapping_action" model="ir.actions.act_window">
			<field name="name">Resource MIS(CSM)</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">hrms_budget_mapping</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
		</record>

