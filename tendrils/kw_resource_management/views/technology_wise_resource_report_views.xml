<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="technology_wise_report_tree" model="ir.ui.view">
			<field name="name">Technology Wise Project Report</field>
			<field name="model">technology_wise_resource</field>
			<field name="arch" type="xml">
				<tree create='false' import='false'>
                    <field name="id" string='Sl No.' invisible="1"/>
                    <field name="primary_skill_id"/>
                    <field name="sbu"/>
					<field name = "job_branch_id"/>
					<field name="name" />
					<field name="job_id"/>
					<field name="emp_code"/>
					<field name="emp_category"/>
				</tree>
			</field>
		</record>
		
		<record id="technology_wise_report_list_action" model="ir.actions.act_window">
			<field name="name">Technology Wise Report-(Total)</field>
			<field name="res_model">technology_wise_resource</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No records found...
				</p>
			</field>
		</record>
		
	</data>
</odoo>