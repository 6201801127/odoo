<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="sbu_report_gap_status_list" model="ir.ui.view">
			<field name="name">sbu_report_gap_status List</field>
			<field name="model">sbu_report_gap_status</field>
			<field name="arch" type="xml">
				<tree create='false' import='false' class="text-center">
					<field name="location_id" class="text-center"/>
                    <field name="skill_id" class="text-center"/>

                    <field name="existing_manger" class="text-center fte_opening"/>
                    <field name="existing_tl_pl" class="text-center fte_opening"/>
                    <field name="existing_sr_dev" class="text-center fte_opening"/>
                    <field name="existing_dev" class="text-center fte_opening"/>
                    <field name="existing_intern" class="text-center fte_opening"/>
                    <field name="existing_ba" class="text-center fte_opening"/>

                    <field name="required_manger" class="text-center fte_totalonboard"/>
                    <field name="required_tl_pl" class="text-center fte_totalonboard"/>
                    <field name="required_sr_dev" class="text-center fte_totalonboard"/>
                    <field name="required_dev" class="text-center fte_totalonboard"/>
                    <field name="required_intern" class="text-center fte_totalonboard"/>
                    <field name="required_ba" class="text-center fte_totalonboard"/>

                    <field name="offered_manger" class="text-center fte_closing"/>
                    <field name="offered_tl_pl" class="text-center fte_closing"/>
                    <field name="offered_sr_dev" class="text-center fte_closing"/>
                    <field name="offered_dev" class="text-center fte_closing"/>
                    <field name="offered_intern" class="text-center fte_closing"/>
                    <field name="offered_ba" class="text-center fte_closing"/>
				</tree>
			</field>
		</record>
		
		<record id="sbu_report_gap_status_action" model="ir.actions.act_window">
			<field name="name">SBU Gap/Status Report</field>
			<field name="res_model">sbu_report_gap_status</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
		</record>
		<menuitem id="sbu_report_gap_status_menu"
				  name="SBU Gap/Status Report"
				  action="sbu_report_gap_status_action"
				  parent="sbu_parent_menu"
		/>
	</data>
</odoo>