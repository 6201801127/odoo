<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="kw_engagement_form_tree" model="ir.ui.view">
            <field name="name">central_bench_engagement_log.tree</field>
            <field name="model">central_bench_engagement_log</field>
            <field name="arch" type="xml">
                <tree  create='0' edit='0' delete='0' import='0'>
                        <field name="emp_id"/>
                        <field name="engagement_plan" />
                        <field name="till_date"/>
                        <field name="future_engagement"/>
                        <field name="future_projection"/>
                        <field name="plan_details"/> 
                        <!-- <field name="project_id"/>-->
                </tree>
            </field>
        </record>

        <record id="kw_central_engagement_action" model="ir.actions.act_window">
            <field name="name">Central Bench Engagement Plan</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">central_bench_engagement_log</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>
       
    </data>
</odoo>