<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="kw_Employee_Skill_Wise_Report_list" model="ir.ui.view">
			<field name="name">Employee Skill Wise Report</field>
			<field name="model">employee_skill_wise_report</field>
			<field name="arch" type="xml">
				<tree create='false' import='false' class="text-center" >
                <field name="skill_id"/>
                <field name="employee_roll"/>
                <field name="total_employee"/>
				</tree>
			</field>
		</record>
		
		<record id="kw_Employee_Skill_Wise_Report_list_action" model="ir.actions.act_window">
			<field name="name">Employee Skill Wise Report</field>
			<field name="res_model">employee_skill_wise_report</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
		</record>
		<menuitem id="kw_skill_wise_report_menu"
				  name=" Employee Skill Wise Report"
				  action="kw_Employee_Skill_Wise_Report_list_action"
				  parent="sbu_parent_menu"
		/>
	</data>
</odoo>