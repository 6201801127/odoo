<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="kw_emp_count_project_report_list" model="ir.ui.view">
			<field name="name">Project Report Employee List</field>
			<field name="model">employee_count_report_project</field>
			<field name="arch" type="xml">
				<tree create='false' import='false'>
					<field name="project_id" />
					<field name="employee_count"/>
					<field name="project_stage"/>
				</tree>
			</field>
		</record>
		
		<record id="kw_emp_count_project_report_list_action" model="ir.actions.act_window">
			<field name="name">Project Report</field>
			<field name="res_model">employee_count_report_project</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No records found...
				</p>
			</field>
		</record>
		<menuitem id="kw_emp_count_project_report_menu"
				  name="Project Report"
				  action="kw_emp_count_project_report_list_action"
				  parent="sbu_parent_menu"
		/>
	</data>
</odoo>