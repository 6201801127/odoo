<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="kw_location_wise_report_list" model="ir.ui.view">
			<field name="name">Location Wise Report List</field>
			<field name="model">location_wise_report</field>
			<field name="arch" type="xml">
				<tree create='false' import='false' class="text-center">
					<field name="location_id" class="text-center"/>
                    <field name="total_employee" class="text-center"/>
				</tree>
			</field>
		</record>
		
		<record id="kw_location_wise_report_list_action" model="ir.actions.act_window">
			<field name="name">Location Wise Report</field>
			<field name="res_model">location_wise_report</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
		</record>
		<menuitem id="kw_emp_count_location_report_menu"
				  name="Location Wise Report"
				  action="kw_location_wise_report_list_action"
				  parent="sbu_parent_menu"
		/>
	</data>
</odoo>