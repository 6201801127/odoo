<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="kw_resource_management.rcm_work_force_analytics_dashboard">
        <div class="p-5 container-fluid">
            <div class="row mb-4">
                <div class="col-12">
                    <div class="card rcm-card">
                        <div class="card-body text-center rcm-secondary-card-sub-body-head">
                            <span class="rcm-h1 font-weight-bold text-center">Resource Nexus</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row mb-4 ">
                <div class="col resource_nexus_count">
                    <div class="card rcm-wf-card">
                        <div  class="card-body text-center rcm-secondary-card-sub-body">
                            <span class="mb-4 h2 font-weight-bold" style="color:black"> Total CSM </span>
                            <p class="rcm-h2 font-weight-bold" style="color:black"><t t-esc="widget.csm_resource_count"/> 
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col company_overhead_csm_resource">
                    <div class="card rcm-wf-card">
                        <div  class="card-body text-center rcm-secondary-card-sub-body">
                            <span class="mb-4 h2 font-weight-bold" style="color:black"> Company Overhead </span>
                            <p class="rcm-h2 font-weight-bold" style="color:black"><t t-esc="widget.cmpn_ovhd_csm_resource_count"/> 
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col delivery_lab_csm_resource">
                    <div class="card rcm-wf-card">
                        <div  class="card-body text-center rcm-secondary-card-sub-body">
                            <span class="mb-4 h2 font-weight-bold" style="color:black"> Delivery Lab </span>
                            <p class="rcm-h2 font-weight-bold" style="color:black"><t t-esc="widget.dlvr_lab_csm_resource_count"/> 
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col delivery_overhead_csm_resource">
                    <div class="card rcm-wf-card">
                        <div  class="card-body text-center rcm-secondary-card-sub-body">
                            <span class="mb-4 h2 font-weight-bold" style="color:black"> Delivery Overhead </span>
                            <p class="rcm-h2 font-weight-bold" style="color:black"><t t-esc="widget.dlvr_ovhd_csm_resource_count"/> 
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col resource_deployement_csm_resource">
                    <div class="card rcm-wf-card">
                        <div  class="card-body text-center rcm-secondary-card-sub-body">
                            <span class="mb-4 h2 font-weight-bold" style="color:black"> Resource Deployement </span>
                            <p class="rcm-h2 font-weight-bold" style="color:black"><t t-esc="widget.resource_dplmnt_csm_resource_count"/> 
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">   
                <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="department_wise_resource_distribution_container"></div>
                        </div>
                    </div>
                </div>       
                <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="total_delivery_resource_distribution_container"></div>
                        </div>
                    </div>
                </div> 
                <div class="col-4">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="sbu_wise_resource_count_container"></div>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="count_of_horizontal_resource_container"></div>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="engagement_plan_wise_distribution_container"></div>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="emp_role_wise_resourcecontainer"></div>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="sbu_wise_role_distribution_container"></div>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="location_wise_role_distribution_container"></div>
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="database_resource_container"></div>
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center rcm-portlet-card-body">
                            <div id="emtech_skill_resource_container"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>