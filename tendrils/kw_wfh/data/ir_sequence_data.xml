<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for work from home -->
        <record id="seq_kw_wfh" model="ir.sequence">
            <field name="name">Work From Home</field>
            <field name="code">kw_wfh_sequence</field>
            <field name="prefix">WFH/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for work from home Deliverables-->
        <record id="seq_kw_wfh_deliverables" model="ir.sequence">
            <field name="name">Work From Home Deliverables</field>
            <field name="code">kw_wfh_deliverables</field>
            <field name="prefix">WFHDT/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
