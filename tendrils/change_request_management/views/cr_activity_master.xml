<odoo>
    <data>
        <!-- explicit list view definition -->
        <record id="kw_cr_activity_master_form" model="ir.ui.view">
            <field name="name">kw_cr_activity_master form</field>
            <field name="model">kw_cr_activity_master</field>
            <field name="arch" type="xml">
                <form duplicate='0'>
                    <sheet>
                        <group>
                            <group>
                                <field name="cr_type" widget="radio" options="{'horizontal': true}"/>
                                <field name="name"/>
                                <field name="code"/>
                                <field name="active" widget="boolean_toggle"/>
                                <field name="estimate_time" attrs="{'invisible': [('cr_type', '=', 'SR')]}"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="Activity" string="Sub Activity"  attrs="{'invisible': [('cr_type', '=', 'CR')]}">
                                
                                <field name="cr_sub_activity_ids">
                                    <tree editable="bottom" delete='true'>
                                        <field name="name"
                                               required="1"/>
                                        <field name="estimate_time_sr"
                                                required="1"/>
                                        <field name="active"
                                               required="1"/>
                                        <field name="sequence"
                                               required="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="kw_cr_activity_master_list">
            <field name="name">kw_cr_activity_master list</field>
            <field name="model">kw_cr_activity_master</field>
            <field name="arch" type="xml">
                <tree import='0'>
                    <field name="name"/>
                    <field name="cr_type"/>
                    <field name="active" widget="boolean_toggle" invisible="1"/>
                </tree>
            </field>
        </record>

        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="kw_cr_activity_master_action">
            <field name="name">Activity Master</field>
            <field name="res_model">kw_cr_activity_master</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Activity Master"
                  id="kw_cr_activity_master_sbu_menu"
                  parent='cr_master_config_menu'
                  action='kw_cr_activity_master_action'
                  sequence="2"/>

