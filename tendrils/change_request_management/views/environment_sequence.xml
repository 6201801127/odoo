<odoo>
    <data>
        <!-- explicit list view definition -->
        <record id="kw_environment_sequence_form" model="ir.ui.view">
            <field name="name">kw_environment_sequence form</field>
            <field name="model">kw_environment_sequence</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="project_id" options="{'no_open': True,'no_create':True,'no_edit':True}"
                                       required="1"/>
                                <field name="environment_id" options="{'no_open': True,'no_create':True,'no_edit':True}"
                                       required="1"/>
                                <field name="cr_sequence"/>
                                <field name="service_sequence"/>
                                <field name="environment_management_id" invisible='1'/>
                                <field name="cr_type"/>
                            </group>
                        </group>
                    </sheet>

                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="kw_environment_sequence_list">
            <field name="name">kw_environment_sequence list</field>
            <field name="model">kw_environment_sequence</field>
            <field name="arch" type="xml">
                <tree create='false' import='false' delete='false'>
                    <field name="project_id"/>
                    <field name="environment_id"/>
                    <field name="cr_sequence"/>
                    <field name="service_sequence"/>
                </tree>
            </field>
        </record>
