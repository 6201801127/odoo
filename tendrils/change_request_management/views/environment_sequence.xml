        <record model="ir.ui.view" id="cr_officer_access_list">
            <field name="name">cr_officer_access list</field>
            <field name="model">cr_officer_access</field>
            <field name="arch" type="xml">
                <tree import='0' editable='bottom' delete='1'>
                    <field name="employee_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>

                </tree>
            </field>
        </record>


        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="cr_officer_access_action">
            <field name="name">Server Admin</field>
            <field name="res_model">cr_officer_access</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Server Admin" id="cr_officer_access_menu" parent='kw_environment_master_root_menu'
                  action='cr_officer_access_action'
                  sequence="4"/>


        <record model="ir.ui.view" id="cr_configuration_access_list">
            <field name="name">cr_configuration_access list</field>
            <field name="model">cr_configuration_access</field>
            <field name="arch" type="xml">
                <tree import='0' editable='bottom' delete='1'>
                    <field name="empl_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>

                </tree>
            </field>
        </record>


        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="cr_configuration_access_action">
            <field name="name">Configuration Manager</field>
            <field name="res_model">cr_configuration_access</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Configuration Manager" id="cr_configuration_access_menu" parent='kw_environment_master_root_menu'
