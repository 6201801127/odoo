                        <filter name="project_env_report" string="Project" domain="" context="{'group_by':'project_id'}"/>
                    </group>
                </search>
            </field>
        </record> -->
        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="kw_environment_sequence_action">
            <field name="name">Project Environment Statistics</field>
            <field name="res_model">kw_environment_sequence</field>
            <!-- <field name="search_view_id" ref="view_environment_sequence_search"/>
            <field name="context">{'group_by':'project_id'}</field> -->
            <field name="view_mode">tree</field>
        </record>

