<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="cr_project_manager_tagging_cron" model="ir.cron">
            <field name="name">CR Management: Update PM id in Environment</field>
            <field name="model_id" ref="change_request_management.model_kw_project_environment_management"/>
            <field name="state">code</field>
            <field name="code">model.pm_tagging_to_project()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">3</field>
            <field name="active" eval="False"/>
        </record>
        <record id="cr_project_manager_tagging_in_employee_cron" model="ir.cron">
            <field name="name">CR Management: Update PM in employee to raise CR</field>
            <field name="model_id" ref="change_request_management.model_kw_project_environment_management"/>
            <field name="state">code</field>
            <field name="code">model.pm_tagging_in_employee()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">4</field>
            <field name="active" eval="False"/>
        </record>
        <record id="cr_update_server_management_sequence" model="ir.cron">
            <field name="name">CR Management: Update sequence num Server Management</field>
            <field name="model_id" ref="change_request_management.model_kw_cr_server_configuration"/>
            <field name="state">code</field>
            <field name="code">model.sequence_num_update_server_management()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>
    </data>
    <data>
        <record id="server_config_code" model="ir.sequence">
            <field name="name">Server Config Number</field>
            <field name="code">self.kw_cr_server_configuration</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
