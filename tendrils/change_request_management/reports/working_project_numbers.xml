<odoo>
    <data>
        <!-- Define the tree view for ServerAdminProjects -->
        <record id="view_server_admin_projects_tree" model="ir.ui.view">
            <field name="name">Working in Total Number of Projects</field>
            <field name="model">server_admin_projects</field>
            <field name="arch" type="xml">
                <tree create="false" delete="false" import="false">
                    <field name="uploaded_by"/>
                    <field name="project_id"></field>
                    <field name="project_count"/>

                </tree>
            </field>
        </record>
        <record id="view_server_admin_projects_search" model="ir.ui.view">
            <field name="name">Working in Total Number of Projects Search</field>
            <field name="model">server_admin_projects</field>
            <field name="arch" type="xml">
                <search string="Search Working in Total Number of Projects">
                    <field name="uploaded_by" string="Uploaded By"/>
                </search>
            </field>
        </record>


        <!-- Define the action to open the Server Admin Projects tree view -->
        <record id="action_server_admin_projects" model="ir.actions.act_window">
            <field name="name">Working in Total Number of Projects</field>
            <field name="res_model">server_admin_projects</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_server_admin_projects_tree"/>

        </record>
<!-- tree view for Working in number of Projects by server admin -->

        <record id="working_projects_count_tree" model="ir.ui.view">
            <field name="name">Working in Number of Projects By Server Admin</field>
            <field name="model">working_projects_details_by_server_admin</field>
            <field name="arch" type="xml">
                <tree create="false" delete="false" import="false">
                    <field name="uploaded_by"/>
                    <field name="project_code"/>
                    <field name="project_name"/>
                    <field name="cr_count" sum="cr_count"/>
                    <field name="service_count" sum="service_count"/>
                </tree>
            </field>
        </record>
        <record id="working_projects_count_search" model="ir.ui.view">
            <field name="name">Working in Number of Projects By Server Admin Search</field>
            <field name="model">working_projects_details_by_server_admin</field>
            <field name="arch" type="xml">
                <search string="Search Working in Total Number of Projects">
                    <field name="uploaded_by" string="Uploaded By"/>
               
                    <group expand="0" string="Group By">
                        <filter string="Uploaded By" domain="[]" name="uplod_group" context="{'group_by': 'uploaded_by'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="action_working_projects_count" model="ir.actions.act_window">
            <field name="name">Working in Number of Projects By Server Admin</field>
            <field name="res_model">working_projects_details_by_server_admin</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="working_projects_count_tree"/>
        </record>


        <menuitem id="menu_server_admin_projects_count" name="User wise Project Count Report" parent="menu_report_root" action="action_server_admin_projects" sequence="16" groups="change_request_management.group_cr_manager"/>
        <menuitem id="menu_working_projects_count" name="User wise CR/SR Report" parent="menu_report_root" action="action_working_projects_count" sequence="17" groups="change_request_management.group_cr_manager"/>

    </data>
</odoo>
