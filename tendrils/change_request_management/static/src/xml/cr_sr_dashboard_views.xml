<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="CrsrDashboard">
        <div class="p-5 container-fluid">
            <div class="row mb-4">
                <div class="col-12">
                    <div class="card cr-sr-card">
                    
                        <div class="card-body text-center cr-sr-portlet-card-body">
                            <span class="rcm-h1 font-weight-bold text-center">CR SR Dashboard </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
            
                <div class="col-6">
                    <div class="card cr-sr-card mb-4">
                        <div class="project-wise-filter-wrapper">
                            <div class="project_wise_filter_container d-flex align-items-center flex-column justify-content-center">
                                <div class="card">
                                    <div class="card-header d-flex justify-content-between align-items-center">
                                        <div><p class="h5 mb-0"> Filter </p></div>
                                        <div><button class="ks_dashboard_item_action  btn dropdown-toggle btn-xs o-no-caret btn" id="close_project_wise_filter"> <span class="fa fa-times" /></button></div>
                                    </div>
                                    <div class="card-body">
                                        <table class="table table-borderless">
                                            <tr>
                                                <td class="font-weight-bold">Year</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="project-years-select">
                                                       
                                                    </select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="font-weight-bold">Month</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="project-months-select">
                                                        
                                                    </select>
                                                </td>
                                            </tr>
                                        </table>
                                        <div class="clearfix"/>
                                        <div class="col-12 text-center"><button class="btn btn-primary btn-sm" id="filter-project-wise"> Search </button></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="card-body text-right cr-sr-portlet-card-body">
                            <div class="portlet_widgets h3 ml-auto">
                                <button class="btn text-center project_wise_filter dashboard-button btn-outline-primary font-weight-bold" title="Filter" id="project-wise-filter">
                                    <span class="fa fa-filter" height="20px" width="20px" />
                                    
                                </button>
                                
                                <button title="Refresh" class="ks_dashboard_item_action btn btn-xs refresh_project_portlet dashboard-button btn-outline-primary font-weight-bold" type="button" aria-expanded="true">
                                    <i class="fa fa-repeat"/>
                                </button>
                            </div>
                       
                             <div id="project_wise_cr_sr_count_container">
                             </div>
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="card cr-sr-card mb-4">
                        <div class="uploaded-by-filter-wrapper">
                            <div class="uploaded_by_filter_container d-flex align-items-center flex-column justify-content-center">
                                <div class="card">
                                    <div class="card-header d-flex justify-content-between align-items-center">
                                        <div><p class="h5 mb-0"> Filter </p></div>
                                        <div><button class="ks_dashboard_item_action  btn dropdown-toggle btn-xs o-no-caret btn" id="close_uploaded_by_filter"> <span class="fa fa-times" /></button></div>
                                    </div>
                                    <div class="card-body">
                                        <table class="table table-borderless">
                                            <tr>
                                                <td class="font-weight-bold">Year</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="upload-years-select">
                                                       
                                                    </select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="font-weight-bold">Month</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="upload-months-select">
                                                       
                                                    </select>
                                                </td>
                                            </tr>
                                        </table>
                                        <div class="clearfix"/>
                                        <div class="col-12 text-center"><button class="btn btn-primary btn-sm" id="filter-uploaded-by-administrator"> Search </button></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="card-body text-right cr-sr-portlet-card-body">
                            <div class="portlet_widgets h3 ml-auto">
                                <button class="btn text-center uploaded_filter dashboard-button btn-outline-primary font-weight-bold" title="Filter" id="uploaded-by-filter">
                                    <span class="fa fa-filter" height="20px" width="20px" />
                                    
                                </button>
                                
                                <button title="Refresh" class="ks_dashboard_item_action btn btn-xs refresh_employee_portlet dashboard-button btn-outline-primary font-weight-bold" type="button" aria-expanded="true">
                                    <i class="fa fa-repeat"/>
                                </button>
                            </div>
                       
                             <div id="cr_sr_container">
                             </div>
                        </div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="card cr-sr-card mb-4">
                        <div class="card-body text-center cr-sr-portlet-card-body">
                             <div id="server_instance_cr_sr_container"></div>
                        </div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="card cr-sr-card mb-4">
                        <div class="card-body text-center cr-sr-portlet-card-body">
                             <div id="working_projects_count_container"></div>
                        </div>
                    </div>
                </div>

                <!-- <div class="col-6">
                    <div class="card cr-sr-card mb-4">
                        <div class="card-body text-center cr-sr-portlet-card-body">
                             <div id="working_projects_by_server_admin_count_container"></div>
                        </div>
                    </div>
                </div> -->

            

                <div class="col-12">
                    <div class="card cr-sr-card mb-4">
                        <div class="day-wise-statictics-wrapper">
                            <div class="day_wise_statictics_container d-flex align-items-center flex-column justify-content-center">
                                <div class="card">
                                    <div class="card-header d-flex justify-content-between align-items-center">
                                        <div><p class="h5 mb-0"> Filter </p></div>
                                        <div><button class="ks_dashboard_item_action  btn dropdown-toggle btn-xs o-no-caret btn" id="close_day_wise_filter"> <span class="fa fa-times" /></button></div>
                                    </div>
                                    <div class="card-body">
                                        <table class="table table-borderless">
                                            <tr>
                                                <td class="font-weight-bold">Year</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="day-wise-years-select">
                                                       
                                                    </select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="font-weight-bold">Month</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="day-wise-months-select">
                                                        
                                                    </select>
                                                </td>
                                            </tr>
                                        </table>
                                        <div class="clearfix"/>
                                        <div class="col-12 text-center"><button class="btn btn-primary btn-sm" id="filter-day-wise"> Search </button></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="card-body text-right cr-sr-portlet-card-body">
                            <div class="portlet_widgets h3 ml-auto">
                                <button class="btn text-center day_wise_filter dashboard-button btn-outline-primary font-weight-bold" title="Filter" id="day-wise-filter">
                                    <span class="fa fa-filter" height="20px" width="20px" />
                                    
                                </button>
                                
                                <button title="Refresh" class="ks_dashboard_item_action btn btn-xs refresh_day_wise_portlet dashboard-button btn-outline-primary font-weight-bold" type="button" aria-expanded="true">
                                    <i class="fa fa-repeat"/>
                                </button>
                            </div>
                       
                             <div id="day_wise_staticstis_container">
                             </div>
                        </div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="card cr-sr-card mb-4">
                        <div class="card-body text-center cr-sr-portlet-card-body">
                             <div id="time_range_of_the_day_container"></div>
                        </div>
                    </div>
                </div>
               
            </div> 
            
                
        </div>
    </t>


</templates>
