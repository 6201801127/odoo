<odoo>
	<data>
		
	   <record model="ir.ui.view" id="view_timesheet_inherit_search">
			<field name="name">timesheet paycut and el search view filter</field>
			<field name="model">kw_timesheet_payroll_report</field>
			<field name="inherit_id" ref="kw_timesheets.view_kw_timesheet_payroll_report_search"/>
			<field name="arch" type="xml">
				<xpath expr="//search/field['division']" position="after">
					<filter string="Timesheet EL Deduction"  name="timesheet_el_days" domain="[('timesheet_el_days','&gt;',0)]"/>
					<filter string="Timesheet Paycut Deduction" name="timesheet_paycut_days"  domain="[('timesheet_paycut_days','&gt;',0)]" />
				</xpath>
			</field>
		</record>

	</data>
</odoo>
