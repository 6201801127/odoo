<odoo>
	<data>
		<record model="ir.ui.view" id="timesheet_payroll_inherit_view">
			<field name="model">kw_payroll_monthly_attendance_report</field>
			<field name="inherit_id" ref="payroll_inherit.view_kw_payroll_monthly_attendance_report_tree" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='num_leave_lwop']" position="after">
					<field name="timesheet_paycut_in_day" string='Timesheet Paycut (F)'/>
          			<!-- <field name="timesheet_el_deduct_in_day" /> -->
				</xpath>
				<xpath expr="//field[@name='total_pay_cut']" position="attributes">
					<attribute name='string'>Total Pay Cut (I) (C+D+E+F)</attribute>
				</xpath>
				<xpath expr="//field[@name='num_shift_working_days']" position="attributes">
					<attribute name='string'>Total Working Days (G)</attribute>
				</xpath>
				<xpath expr="//field[@name='total_days_payable']" position="attributes">
					<attribute name='string'>Total Days Payable (H-I)</attribute>
				</xpath>
				<xpath expr="//field[@name='actual_working']" position="attributes">
					<attribute name='string'>Actual Working Days (H)</attribute>
				</xpath>
			</field>
		</record>

		<!--Inherit search view-->
		<record id="view_payroll_timesheet_inherit_search" model="ir.ui.view">
			<field name="name">timesheet search view filter</field>
			<field name="model">kw_payroll_monthly_attendance_report</field>
			<field name="inherit_id" ref="payroll_inherit.view_kw_payroll_monthly_attendance_report_search"/>
			<field name="arch" type="xml">
				<xpath expr="//search/field['paycut_employees']" position="after">
					<filter string="Timesheet EL Deduction" name="timesheet_el_deduct_in_day" domain="[('timesheet_el_deduct_in_day','&gt;',0)]"/>
					<filter string="Timesheet Paycut Deduction" name="timesheet_paycut_in_day" domain="[('timesheet_paycut_in_day','&gt;',0)]"/>
				</xpath>
			</field>
		</record>

		<!-- Timesheet Paroll report Inherit view -->
		<record model="ir.ui.view" id="timesheet_payroll_report_inherit_view">
			<field name="model">kw_timesheet_payroll_report</field>
			<field name="inherit_id" ref="kw_timesheets.view_kw_timesheet_payroll_report_tree" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='total_effort_day']" position="after">
					<field name="timesheet_el_days" />
					<field name="timesheet_paycut_days"/>
					<field name="status"/>
				</xpath>

			</field>
		</record>

		<record model="ir.ui.view" id="timesheet_payroll_report_inherit_search_view">
			<field name="model">kw_timesheet_payroll_report</field>
			<field name="inherit_id" ref="kw_timesheets.view_kw_timesheet_payroll_report_search" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='division']" position="after">
					<filter string="Timesheet EL Deduction" name="filter_timesheet_el_days" domain="[('timesheet_el_days','&gt;',0)]"/>
					<filter string="Timesheet Paycut Deduction" name="filter_timesheet_paycut_days" domain="[('timesheet_paycut_days','&gt;',0)]"/>
					<separator/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
