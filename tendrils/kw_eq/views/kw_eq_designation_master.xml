<odoo>
    <data>
      <record id="kw_eq_designation_master_view_tree" model="ir.ui.view">
        <field name="name">kw_eq_designation_master.view.tree</field>
        <field name="model">kw_eq_designation_master</field>
        <field name="arch" type="xml">
          <tree editable='top'>
            <field name="functional_category"/>
            <field name="designation_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
            <field name="section"/>
          </tree>
        </field>
      </record>
  
      <record id="kw_eq_designation_master_action" model="ir.actions.act_window">
        <field name="name">Designation Master</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">kw_eq_designation_master</field>
        <field name="view_mode">tree</field>
        <field name="view_type">form</field>
      </record>
  
      
    </data>
  </odoo>