<odoo>
  <data>
    <!-- kw_eq_acc_head_sub_head tree view -->
    <record id="kw_eq_acc_head_sub_head_view_tree" model="ir.ui.view">
      <field name="name">kw_eq_acc_head_sub_head.view.tree</field>
      <field name="model">kw_eq_acc_head_sub_head</field>
      <field name="arch" type="xml">
        <tree editable='top'>
          <field name="account_head_id" options="{'no_open': True,'no_create':True,'no_edit':True}" required='1'/>
          <field name="account_subhead_id" options="{'no_open': True,'no_create':True,'no_edit':True}" required='1'/>
          <field name="total_profit_percentage" groups='kw_eq.group_eq_manager'/>
          <field name="code" groups='kw_eq.group_eq_manager'/>
          <field name="effective_date" groups='kw_eq.group_eq_manager'/>
          <field name="sort_no" />
        </tree>
      </field>
    </record>

    <!-- kw_eq_acc_head_sub_head action window -->
    <record id="kw_eq_acc_head_sub_head_action" model="ir.actions.act_window">
      <field name="name">Account Head &amp; Sub head Configuration</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">kw_eq_acc_head_sub_head</field>
      <field name="view_mode">tree</field>
      <field name="view_type">form</field>
    </record>
    
  </data>
</odoo>