<odoo>
    <data>
        <!-- model.name form view -->
        <record id="kw_eq_paticulars_master_view_tree" model="ir.ui.view">
            <field name="name">kw_eq_paticulars_master.view.tree</field>
            <field name="model">kw_eq_paticulars_master</field>
            <field name="arch" type="xml">
                <tree editable='top'>
                    <field name="paticulars" required='1' />
                    <field name="code" required='1' />
                </tree>
            </field>
        </record>


        <record id="kw_eq_paticulars_master_action" model="ir.actions.act_window">
            <field name="name">Paticulars Master</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_eq_paticulars_master</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>


    </data>
</odoo>