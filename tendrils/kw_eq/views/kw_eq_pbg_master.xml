<odoo>
    <data>
      <!-- kw_eq_pbg_master tree view -->
      <record id="kw_eq_pbg_master_view_tree" model="ir.ui.view">
        <field name="name">kw_eq_pbg_master.view.tree</field>
        <field name="model">kw_eq_pbg_master</field>
        <field name="arch" type="xml">
          <tree editable='top'>
            <field name="item" required="1"/>
            <field name="implementation_percentage" />
            <field name="support_percentage" />
            <field name="percentage_edit_bool"/>
            <field name="value_edit_bool" />
            <field name="code" />
            <field name="effective_from"/>
          </tree>
        </field>
      </record>
  
      <!-- kw_eq_pbg_master action window -->
      <record id="kw_eq_pbg_master_action" model="ir.actions.act_window">
        <field name="name">PBG Master</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">kw_eq_pbg_master</field>
        <field name="view_mode">tree</field>
        <field name="view_type">form</field>
      </record>
  
      
    </data>
  </odoo>