<odoo>
  <data>
    <!-- kw_eq_overhead_percentage_master tree view -->
    <record id="kw_eq_overhead_percentage_master_view_tree" model="ir.ui.view">
      <field name="name">kw_eq_overhead_percentage_master.view.tree</field>
      <field name="model">kw_eq_overhead_percentage_master</field>
      <field name="arch" type="xml">
        <tree editable='top'>
          <field name="overhead_type" required="1"/>
          <field name="percentage"/>
          <field name="company_ovhead_percentage" attrs="{'invisible':[('overhead_type','=','production')]}"/>
          <field name="effective_date"/>
        </tree>
      </field>
    </record>

    <!-- kw_eq_overhead_percentage_master action window -->
    <record id="kw_eq_overhead_percentage_master_action" model="ir.actions.act_window">
      <field name="name">Overhead Percentage Configuration</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">kw_eq_overhead_percentage_master</field>
      <field name="view_mode">tree</field>
      <field name="view_type">form</field>
    </record>

    
  </data>
</odoo>