<odoo>
    <data>
      <!-- kw_eq_it_infra_master tree view -->
      <record id="kw_eq_it_infra_master_view_tree" model="ir.ui.view">
        <field name="name">kw_eq_it_infra_master.view.tree</field>
        <field name="model">kw_eq_it_infra_master</field>
        <field name="arch" type="xml">
          <tree editable='top'>
            <field name="name" required='1'/>
            <field name="type" required='1'/>
          </tree>
        </field>
      </record>
  
      <!-- kw_eq_it_infra_master action window -->
      <record id="kw_eq_it_infra_master_action" model="ir.actions.act_window">
        <field name="name">IT Infra Master</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">kw_eq_it_infra_master</field>
        <field name="view_mode">tree</field>
        <field name="view_type">form</field>
      </record>
  
      
    </data>
  </odoo>