<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate='0'>
		<record model="ir.ui.view" id="log_sync_social_image_tree_view">
			<field name="name">log_sync_social_image_tree_view</field>
			<field name="type">tree</field>
			<field name="model">log_sync_social_image</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
					<field name="payload"/>
					<field name="status"/>
				</tree>
			</field>
		</record>

		<record id="log_sync_social_image_form_view" model="ir.ui.view">
			<field name="name">log_sync_social_image_form_view</field>
			<field name="model">log_sync_social_image</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name="name"/>
							<field name="payload"/>
							<field name="status"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="log_sync_social_image_action_window" model="ir.actions.act_window">
			<field name="name">Employee Candid Image Sync Log</field>
			<field name="res_model">log_sync_social_image</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					Upload a new Employee Candid Image
				</p>

			</field>
		</record>
	</data>
</odoo>