        <!-- RA Approve Mail -->
        <record id="kw_kt_ra_approve_mail_template" model="mail.template">
            <field name="name">KT: KT RA Approve Mail Template</field>
            <field name="email_from">${ctx['ra_email']}</field>
            <field name="model_id" ref="kw_kt.model_kw_time_line_plan"/>
            <field name="subject">KT Plan of ${ctx['applicant_name']} ( ${ctx['applicant_code']})| Approved </field>
            <field name="email_to">${ctx['applicant_email']}</field>
            <field name="email_cc">${ctx['cc']}</field>
            <field name="body_html" >
                 <![CDATA[

                      <table border="0" style="width: 100%;">
                        <tr>
                            <td> 
                                <div>
                                        
                                    <h5 style="font-weight: 400;margin-bottom: 0;font-size: 16px;color:#676767;">
                                        <span style="color:rgb(22, 123, 158);font-size: 16px;margin-right: 2px;font-weight: 600;">Dear</span>
                                        ${ctx['applicant_name']},
                                    </h5>

                                    <p style="color: #676767;line-height: 145%;margin: 10px 0 0 0;font-size: 16px;">
                                        Your KT Plan has been approved by ${ctx['ra_name']}.
                                    </p>
                                    <p style="color: #676767;line-height: 145%;margin: 10px 0 0 0;font-size: 16px;">Remarks : ${ctx['remark']}</p>
                                </div>
                        </td>
                    </tr>
                    </table>
                    <table border="1" cellpadding="5" cellspacing="0" style="width:50%;border-collapse: collapse;font-size: 16px; margin-top:1rem; ">
                        <tr>
                            <th style="height: 30px;width:30%;padding-left:5px; font-weight:700;color:#676767;text-align:left;"> Type </th>
                            <th style="height: 30px;width:70%;padding-left:5px;font-weight:700;color:#676767;text-align:left;"> Activities/Project Name </th>
                            <th style="height: 30px;width:70%;padding-left:5px;font-weight:700;color:#676767;text-align:left;"> Description </th>
                            <th style="height: 30px;width:70%;padding-left:5px;font-weight:700;color:#676767;text-align:left;"> Attendees </th>
                            <th style="height: 30px;width:70%;padding-left:5px;font-weight:700;color:#676767;text-align:left;"> Scheduled  Date </th>
                            <th style="height: 30px;width:70%;padding-left:5px;font-weight:700;color:#676767;text-align:left;"> Start Time </th>
                            <th style="height: 30px;width:70%;padding-left:5px;font-weight:700;color:#676767;text-align:left;"> End Time </th>
                        </tr>
                        %for data in ctx['kt_data'].values():
                        <tr>
                            <td style="padding-left:5px;color:#676767;height: 34px;width:10%;">
                                ${data.get('kt_type')}
                            </td>
                            <td style="padding-left:5px;color:#676767;height: 34px;width:25%;">
                                ${data.get('kt_activies')}
                            </td>
                            %if data.get('kt_activies'):
                                <td style="padding-left:5px;color:#676767;height: 34px;width:25%;">
                                    ${data.get('kt_activies')}
                                </td>
                            %endif
                            %if data.get('kt_project_id'):
                                <td style="padding-left:5px;color:#676767;height: 34px;width:10%;">
                                    ${data.get('kt_project_id')}
                                </td>
                            %endif
                            <td style="padding-left:5px;color:#676767;height: 34px;width:10%;">
                                ${data.get('kt_attendies')}
                            </td>
                            <td style="padding-left:5px;color:#676767;height: 34px;width:25%;">
                                ${data.get('kt_date')}
                            </td>
                            <td style="padding-left:5px;color:#676767;height: 34px;width:10%;">
                                ${data.get('kt_start_time')}
                            </td>
                            <td style="padding-left:5px;color:#676767;height: 34px;width:25%;">
                                ${data.get('kt_end_time')}
                            </td>
                        </tr>
                    %endfor
                </table>
                    <div style="margin-bottom:1em;font-size: 16px;color:#676767;padding-left: 1px;">
                        <p>Thank You.</p>
                        % if object.env.user and object.env.user.signature:
                            ${object.env.user.signature | safe}
                        % endif
                    </div>
 			]]>

            </field>
            <field name="user_signature" eval="False"/>
        </record>
