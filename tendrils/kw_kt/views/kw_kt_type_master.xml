<?xml version="1.0" encoding="UTF-8"?>
<odoo>  
    <data noupdate="0">
        <!-- Tree view for "model" -->
        <record id="view_kw_kt_type_master_tree" model="ir.ui.view">
            <field name="name">view.kw_kt_type_master.tree</field>
            <field name="model">kw_kt_type_master</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree string="kw_kt_type_master_tree" editable="top" import='0'>
                    <field name="name" groups="kw_kt.group_kt_manager"/>
                    <field name="code" groups="kw_kt.group_kt_manager"/>
                    <field name="is_manual" groups="kw_kt.group_kt_manager"/>
                </tree>
            </field>
        </record>
        
        <!-- Search view for "model" -->
        <record id="view_kw_kt_type_master_search" model="ir.ui.view">
            <field name="name">view.kw_kt_type_master.search</field>
            <field name="model">kw_kt_type_master</field>
            <field name="type">search</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <search string="kw_kt_type_master_search">
                    <field name="name" />
                    <field name="code" />
                    
                </search>
            </field>
        </record>
        
        <!-- Main window action for "model" -->
        <record id="action_kw_kt_type_master_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">KT Type Master</field>
            <field name="res_model">kw_kt_type_master</field>
            <field name="view_mode">tree</field>
            <field name="target">current</field>
            <field name="domain">[]</field>
            <field name="context">\{}</field>
            <field name="search_view_id" ref="view_kw_kt_type_master_search" />
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Records not available.
                </p>
            </field>
        </record>
    </data>
</odoo>
