<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <!-- model.name form view -->
        <record id="kw_project_document_view_form" model="ir.ui.view">
            <field name="name">Project Document Form</field>
            <field name="model">kw_project_document</field>
            <field name="arch" type="xml">
                <form string="Project Document Form">
                    <sheet>
                        <group>
                            <group>
                                <field name="document_attach"/>
                                <!-- <field name="project_id"/>
                                <field name="description"/> -->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- model.name tree view -->
        <record id="kw_project_document_view_tree" model="ir.ui.view">
            <field name="name">Project Document Tree</field>
            <field name="model">kw_project_document</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="document_attach"/>
                    <!-- <field name="project_id"/> -->
                    <!-- <field name="description"/> -->
                </tree>
            </field>
        </record>

        <!-- kw_resignation_master action window -->
        <record id="kw_project_document_action" model="ir.actions.act_window">
            <field name="name">Project Document</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_project_document</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Records not available.
                </p>
            </field>
        </record>

       
    </data>
</odoo>