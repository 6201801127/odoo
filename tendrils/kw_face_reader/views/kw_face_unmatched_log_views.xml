<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="view_kw_face_unmatched_log_list">
      <field name="name">kw_face_unmatched_log list</field>
      <field name="model">kw_face_unmatched_log</field>
      <field name="arch" type="xml">
        <tree  editable="bottom"  default_order="create_date desc">
          <field name="employee_id"/>        
          
           <field name="image_binary" string="Photo" class="oe_avatar" widget="image"/>
           <field name="create_date" />
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="kw_face_unmatched_log_form_view">
        <field name="name">kw_face_unmatched_log form</field>
        <field name="model">kw_face_unmatched_log</field>
        <field name="arch" type="xml">
            <form string="Enrollment Form">
                <group >
                    <field name="employee_id" 
                    attrs="{'readonly':[('employee_id','!=',False)]}"
                    />
                    <field name="image_name"  readonly="1"/>
                    
                </group>
                <group  >

                    <field name="image_binary" readonly="1" />

                </group>
            </form>
        </field>
    </record>
   


    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="kw_face_unmatched_log_action_window">
      <field name="name">Unmatched Log</field>
      <field name="res_model">kw_face_unmatched_log</field>
      <field name="view_mode">tree,form</field>
    </record>
   

    
  </data>
</odoo>