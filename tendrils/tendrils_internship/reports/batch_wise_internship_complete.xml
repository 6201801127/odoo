        <record id="batch_wise_internship_tree" model="ir.ui.view">
            <field name="name">batch_wise_internship.tree</field>
            <field name="model">batch_wise_internship</field>
            <field name="arch" type="xml">
                <tree create='0' edit='0' delete='0' import='0'>
                    <field name="batch_name"/>
                    <field name="financial_year_id"/>
                    <field name="start_date"/>
                    <field name="close_date"/>
                    <field name="batch_stage"/>
                    <field name="intern_name"/>

                </tree>
            </field>
        </record>

        <record id="batch_wise_internship_search_view" model="ir.ui.view">
            <field name="name">batch_wise_internship_search_view</field>
            <field name="model">batch_wise_internship</field>
            <field name="arch" type="xml">
             
                <search>
                    <field name="financial_year_id"/>
                    <field name="batch_name"/>
                    <field name="start_date"/>
                    <field name="close_date"/>
                    <field name="intern_name"/>
                    <field name="batch_stage"/>
                </search>
            </field>
        </record>

        <record id="batch_wise_internship_action" model="ir.actions.act_window">
            <field name="name">Batch Wise Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">batch_wise_internship</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
            <!-- <field name="context">{'search_default_current_month_task':1,}</field> -->

        </record>

        
       
        <menuitem id="batch_wise_internships_menu"
                  name="Batch Wise Internship Complete"
                  parent="reports_parent_menu"
                  action="batch_wise_internship_action"
                  sequence="1"/>

    </data>
</odoo>