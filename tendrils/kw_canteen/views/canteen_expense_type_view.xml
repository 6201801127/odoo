<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record model="ir.ui.view" id="kw_canteen_expense_type_list">
            <field name="name">Canteen Expenses Type</field>
            <field name="model">kw_canteen_expense_type_master</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="expense_on"/>
                    <field name="expense_head_code"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="canteen_expense_type_view_form">
            <field name="name">Canteen Expenses Type</field>
            <field name="model">kw_canteen_expense_type_master</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="expense_on"/>
                                <field name="expense_head_code"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
   
     
        <record id="kw_canteen_expense_type_action_window" model="ir.actions.act_window">
            <field name="name">Canteen Expenses Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_canteen_expense_type_master</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p><p>
                    <!-- More details about what a user can do with this object will be OK --> 
                </p>
            </field>
        </record>
    </data>
</odoo>