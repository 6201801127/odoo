<odoo>
	<data>
		<record id="kw_canteen_expense_wizard_view_form" model="ir.ui.view">
			<field name="name">kw_canteen_expense_wizard_form</field>
			<field name="model">kw_canteen_expense_wizard</field>
			<field name="arch" type="xml">
				<form string="">
					<sheet>
						<group>
                            <field name="year"/>
                            <field name="month"/>
						</group>
					</sheet>
					<footer>
						<button name="get_employee_collection" class="btn-success" string="Get Collection From Employee" type="object" groups="kw_canteen.canteen_manager_group"/>
					</footer>
				</form>
			</field>
		</record>
		<record id="kw_canteen_expense_wizard_view_action" model="ir.actions.act_window">
			<field name="type">ir.actions.act_window</field>
			<field name="name">Canteen Collection</field>
			<field name="res_model">kw_canteen_expense_wizard</field>
			<field name="view_mode">form</field>
			<field name="view_type">form</field>
			<field name="target">new</field>
		</record>
	</data>
</odoo>