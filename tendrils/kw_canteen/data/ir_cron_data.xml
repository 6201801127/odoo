<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="smart_office_correspondence_text_extraction_scheduler" model="ir.cron">
            <field name="name">Canteen : Create Daily Meal Scheduler</field>
            <field name="model_id" ref="model_meal_bio_log"/>
            <field name="state">code</field>
            <field name="code">model.create_daily_meal_log()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 04:30:20')" /> 
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <!-- <field name="active" eval="True"/> -->
        </record>

        <record id="send_msg_to_autority" model="ir.cron">
            <field name="name">Canteen : Send Current Day Info</field>
            <field name="model_id" ref="model_current_day_meal_report"/>
            <field name="state">code</field>
            <field name="code">model.send_today_meals_details()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <!-- <field name="active" eval="True"/> -->
        </record>
    </data>
</odoo>
