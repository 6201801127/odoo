<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="0">
		<record id="kw_uom_default_check" model="res.config.settings">
			<field name="group_multi_company" eval="True"/>
			<field name="company_share_partner" eval="False"/>
			<field name="group_uom" eval="True"/>
		</record>

		<function model="res.config.settings" name="execute">
			<value model="res.config.settings" search="[('id', '=', ref('kw_uom_default_check'))]"/>
		</function>

		<record id="kw_location_default_check" model="res.config.settings">
			<field name="group_multi_company" eval="True"/>
			<field name="company_share_partner" eval="False"/>
			<field name="group_stock_multi_locations" eval="True"/>
		</record>

		<function model="res.config.settings" name="execute">
			<value model="res.config.settings" search="[('id', '=', ref('kw_location_default_check'))]"/>
		</function>

		<!-- Purchase Data file overwrite -->
		<function name="write" model="ir.model.data">
			<function name="search" model="ir.model.data">
				<value eval="[('module', '=', 'purchase'), ('name', '=', 'seq_purchase_order')]"/>
			</function>
			<value eval="{'noupdate': False}"/>
		</function>

		<!-- purchase sequence Overwrite -->
		<record id="purchase.seq_purchase_order" model="ir.sequence">
			<field name="name">Purchase Order</field>
			<field name="code">purchase.order</field>
			<field name="prefix">1%(y)s05</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

	</data>
</odoo>
