<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="0">
		<record model="kw_sequence" id="kw_sequence1">
			<field name="deptartment_id" eval="1"/>
			<field name="code">101</field>
		</record>
		<record model="kw_sequence" id="kw_sequence2">
			<field name="deptartment_id" eval="4"/>
			<field name="code">102</field>
		</record>
		<record model="kw_sequence" id="kw_sequence3">
			<field name="deptartment_id" eval="5"/>
			<field name="code">103</field>
		</record>
		<record model="kw_sequence" id="kw_sequence4">
			<field name="deptartment_id" eval="6"/>
			<field name="code">104</field>
		</record>
		<!-- <record forcecreate="True" id="decimal_subtotal" model="decimal.precision">
			<field name="name">Price Subtotal</field>
			<field name="digits">3</field>
		</record> -->
		<record id="seq_stock_move" model="ir.sequence">
            <field name="name">Fa code</field>
            <field name="code">fa_code_sequence</field>
            <field name="prefix">TEMP/FA/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
		
		<record id="scheduler_for_uncreated_fa" model="ir.cron">
			<field name="name">Inventory: Uncreated FA reminder</field>
			<field name="active" eval="False"/>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="doall">0</field>
			<field name="numbercall">-1</field>
			<field name="model_id" ref="model_stock_quant"/>
			<field name="code">model.send_email_for_uncreated_fa()</field>
			<field name="state">code</field>
		</record>
		<record id="scheduler_for_grn_qc_srv_validation" model="ir.cron">
			<field name="name">Inventory: GRN,QC,SRV validation reminder</field>
			<field name="active" eval="False"/>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="doall">0</field>
			<field name="numbercall">-1</field>
			<field name="model_id" ref="model_stock_picking"/>
			<field name="code">model.grn_qc_srv_validation_reminder()</field>
			<field name="state">code</field>
		</record>
	</data>


	<data noupdate="1">
		<record id="sequence_update_inventory" model="ir.cron">
			<field name="name">Inventory: Sequence Update</field>
			<field name="active" eval="False"/>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="doall">0</field>
			<field name="numbercall">-1</field>
			<field name="model_id" ref="model_stock_picking"/>
			<field name="code">model.sequence_update_from_sequence_master()</field>
			<field name="state">code</field>
		</record>
	</data>
</odoo>
