<?xml version="1.0" encoding="UTF-8"?>
<templates id="InventoryUserGuideTemplate" xml:space="preserve">
	<t t-name="ListView.buttons" t-extend="ListView.buttons">
		<t t-jquery='.o_list_button_discard' t-operation='after'>
			
			<t t-if="widget.modelName=='kw_material_management'">
				<button type="button" class="btn btn-warning btn-lg  oe_action_inventory_guide_button" accesskey="t">User Guide</button>
			</t>
			<t t-if="widget.modelName=='kw_purchase_requisition'">
				<button type="button" class="btn btn-warning btn-lg  oe_action_purchase_requisition_guide" accesskey="t">User Guide</button>
			</t>
			<t t-if="widget.modelName=='kw_requisition_requested'">
				<button type="button" class="btn btn-warning btn-lg  oe_action_purchase_requested_guide_button" accesskey="t">User Guide</button>
			</t>
			<t t-if="widget.modelName=='stock.picking'">
				<button type="button" class="btn btn-warning btn-lg  oe_action_stock_guide_button" accesskey="t">User Guide</button>
			</t>
			<t t-if="widget.modelName=='purchase.order.line'">
				<button type="button" class="btn btn-warning btn-lg  oe_action_ses_guide_button" accesskey="t">User Guide</button>
			</t>
			<!-- <t t-if="widget.modelName=='kw_service_register'">
				<button type="button" class="btn btn-warning btn-lg  oe_action_service_register_guide_button" accesskey="t">User Guide</button>
			</t> -->
		</t>
	</t>
</templates>
