<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="paperformat_grn_landscape" model="report.paperformat">
			<field name="name">GRN Landscape</field>
			<field name="default" eval="True"/>
			<field name="format">custom</field>
			<field name="page_height">297</field>
			<field name="page_width">220</field>
			<field name="orientation">Landscape</field>
			<field name="margin_top">0</field>
			<field name="margin_bottom">0</field>
			<field name="margin_left">5</field>
			<field name="margin_right">5</field>
			<field name="header_line" eval="False"/>
			<field name="header_spacing">40</field>
			<field name="dpi">90</field>
		</record>
		<report
				string="GRN Report"
				id="action_report_picking"
				model="stock.picking"
				report_type="qweb-pdf"
				name="kw_inventory.report_grn"
				file="kw_inventory.report_grn"
				paperformat="paperformat_grn_landscape"
				menu="False"
		/>
		<report
				string="SRV Report"
				id="action_report_picking_SRV"
				model="stock.picking"
				report_type="qweb-pdf"
				name="kw_inventory.report_srv"
				file="kw_inventory.report_srv"
				paperformat="paperformat_grn_landscape"
				menu="False"
		/>
		<report
				string="Inspection Report"
				id="action_report_picking_INSPECTION"
				model="stock.picking"
				report_type="qweb-pdf"
				name="kw_inventory.report_inspection"
				file="kw_inventory.report_inspection"
				paperformat="paperformat_grn_landscape"
				menu="False"
		/>
		<!-- <report
				string="Gate Pass"
				id="action_report_gate_pass"
				model="stock.picking"
				report_type="qweb-pdf"
				name="kw_inventory.report_gatepass"
				file="kw_inventory.report_gatepass"
				paperformat="paperformat_grn_landscape"
				menu="False"
		/> -->

	</data>
</odoo>