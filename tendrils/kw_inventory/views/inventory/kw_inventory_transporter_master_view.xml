<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        
        <record id="kw_inventory_transporter_master_view_tree" model="ir.ui.view">
            <field name="name">kw_inventory_transporter_master_view_tree</field>
            <field name="model">kw_inventory_transporter_master</field>
            <field name="arch" type="xml">
                <tree  string="Transport Master">
                    <field name="transporter_name" />
                    <field name="transporter_code"/>
                   
                                  
                </tree>
            </field>
        </record>

       
        <record id="kw_inventory_transporter_master_view_form" model="ir.ui.view">
            <field name="name">kw_inventory_transporter_master_view_form</field>
            <field name="model">kw_inventory_transporter_master</field>
            <field name="arch" type="xml">
                <form string="Transport Master" >
                   <sheet>
                        <group>
                            <group>
                            <field name="transporter_name" />
                            <field name="transporter_code"/>
                            
                        </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        
        <record id="kw_inventory_transporter_master_action_window" model="ir.actions.act_window">
            <field name="name">Transport Master</field>
            <field name="res_model">kw_inventory_transporter_master</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
        </record>

        <menuitem id="transporter_master_parent_menu" name="Transporter" parent="stock.menu_stock_config_settings"
        sequence="99"/>
        <menuitem id="transporter_master_menu" action="kw_inventory_transporter_master_action_window" name="Transport Master" parent="transporter_master_parent_menu"
        sequence="1"/>
    </data>
</odoo>