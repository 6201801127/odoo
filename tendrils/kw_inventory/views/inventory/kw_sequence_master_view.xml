<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="kw_sequence_tree" model="ir.ui.view">
		<field name="name">Sequence Master Tree</field>
		<field name="model">kw_sequence</field>
		<field name="arch" type="xml">
			<tree editable="top">
				<field name="department_id"/>
				<field name="code"/>
				<field name="picking_type_id"/>
			</tree>
		</field>
	</record>

	<record id="kw_sequence_from" model="ir.ui.view">
		<field name="name">Sequence Master Form</field>
		<field name="model">kw_sequence</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="department_id"/>
						<field name="code"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="kw_sequence_action" model="ir.actions.act_window">
		<field name="type">ir.actions.act_window</field>
		<field name="name">Sequence Master</field>
		<field name="res_model">kw_sequence</field>
		<field name="view_mode">tree,form</field>
		<field name="view_type">form</field>
	</record>

	<menuitem id="kw_sequence_menu" parent="stock.menu_stock_inventory_control" action="kw_sequence_action"/>

</odoo>