<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="kw_inventory_order_endpoint_inherited">
			<field name="name">stock.warehouse.orderpoint.tree.inherit</field>
			<field name="model">stock.warehouse.orderpoint</field>
			<field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='warehouse_id']" position="replace"/>
				<xpath expr="//field[@name='location_id']" position="replace"/>

				<xpath expr="//field[@name='product_max_qty']" position="after">
                    <field name="warehouse_id" />
                    <field name="location_id" />
                </xpath>
			</field>
		</record>
