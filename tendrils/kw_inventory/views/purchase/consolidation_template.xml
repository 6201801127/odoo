<odoo>
    <data>
        <template id="kw_consolidation_details" name="Consolidation Details">
            <t t-call="website.layout">
                <div class="container-fluid mt-5">
                    <form name="frm_indent_consolidation" id="frm_indent_consolidation" class="form-horizontal indicator-form" method="post" action="" autocomplete="off" enctype="multipart/form-data">
                    <h3 class="text-center">Quotation consolidation &amp; decision stage</h3>
                    <h4><div class="d-flex justify-content-center">
                            Q: 
                            <t t-foreach="quotation_dict" t-as="q">
                                <p t-esc="q"/>
,
                            </t>
                        </div>
                    </h4>
                    <div class="table-responsive">
                        <table class="table table-hover table-bordered ">
                            <thead style="background: #108fbb; color: #fff;">
                                
                                <tr>
                                    <td class="text-center"> Item Code </td>
                                    <td class="text-center" width="200">Item Name </td>

                                    <t t-foreach="vendor_name" t-as="v">
                                        <td class="text-center" colspan="3">
                                            <p t-esc="v['name']" />
                                        </td>
                                    </t>

                                    <td class="text-center" colspan="2"> Last procurement Price</td>
                                    <td class="text-center" colspan="3"> Decision</td>
                                </tr>
                                <tr>
                                    <td colspan="2"></td>
                                    <t t-foreach="vendor_name" t-as="v">
                                        <td class="text-center"> Unit Price </td>
                                        <td class="text-center"> Quantity</td>
                                        <td class="text-center"> Total </td>
                                    </t>
                                    <td colspan="2"></td>
                                    <td class="text-center"> L1</td>
                                    <td class="text-center"> L2</td>
                                    <td class="text-center"> L3 </td>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach="p_name" t-as="p">
                                    <tr>
                                        <t t-set="product_id" t-value="p['id']"/>
                                        <td class="text-center">
                                            <p t-esc="product_id"/>
                                        </td>
                                        <td class="text-center">
                                            <p t-esc="p['name']"/>
                                        </td>
                                        <t t-foreach="vendor_name" t-as="v">
                                            <t t-set="vendor_id" t-value="v['id']"/>
                                           
                                            <t t-if="product_id in v_data and vendor_id in v_data[product_id] and v_data[product_id][vendor_id]">
                                                <t t-set="item" t-value="v_data[product_id][vendor_id]"/>
                                                <td class="text-center">
                                                    <t t-esc="item[0]"/>
                                                </td>
                                                <td class="text-center">
                                                    <t t-esc="item[1]"/>
                                                </td>
                                                <td class="text-center">
                                                    <t t-esc="item[2]"/>
                                                </td>
                                            </t>
                                            <t t-else="">
                                                <td class="text-center">NA</td>
                                                <td class="text-center">NA</td>
                                                <td class="text-center">NA</td>
                                            </t>
                                        </t>
                                        <td colspan="2"></td>
                                        <t t-if="product_id in min_value">
                                             <t t-set="min_val" t-value="min_value[product_id]"/>
                                            <td class="text-center"><t t-esc="min_val[0]"/></td>
                                            <td class="text-center"><t t-esc="min_val[1]"/></td>
                                            <td class="text-center"><t t-esc="min_val[2]"/></td>
                                        </t>
                                        <t t-else="">
                                            <td class="text-center">NA</td>
                                            <td class="text-center">NA</td>
                                            <td class="text-center">NA</td>
                                        </t>
                                    </tr>
                                </t>

                            </tbody>
                        </table>
                    </div>
                    </form>
                </div>
            </t>
        </template>
