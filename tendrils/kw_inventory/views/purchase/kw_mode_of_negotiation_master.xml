<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        
        <record id="kw_negotiation_mode_view_tree" model="ir.ui.view">
            <field name="name">kw_negotiation_mode_view_tree</field>
            <field name="priority" eval="1"/>
            <field name="model">kw_negotiation_mode_master</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="mode"/>
                </tree>
            </field>
        </record>

       
        <record id="kw_negotiation_mode_view_form" model="ir.ui.view">
            <field name="name">kw_negotiation_mode_view_form</field>
            <field name="priority" eval="1"/>
            <field name="model">kw_negotiation_mode_master</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                        <group>
                            <field name="mode" />
                        </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        

        
        <record id="kw_negotiation_mode_action_window" model="ir.actions.act_window">
            <field name="name">Mode Of Negotiation</field>
            <field name="res_model">kw_negotiation_mode_master</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
        </record>
    </data>
</odoo>





