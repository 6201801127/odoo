<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="material_request_button_redirect" name="Material Request">
			<t t-call="website.layout">
				<div class="wrap">
					<div class="container">
						<section class="service-request mt32">
							<div class="container card">
								<div class="card-body">
									<div class="row">
										<div class="col-12 mx-auto">
											<div class="section_content text-center">

												<h2 class="display-6 text-center font-weight-bold mt-6 mb-4">
													Material Request
													<br/>
												</h2>
												<t t-if="state == 'Approved'">Material request(<t t-esc="request_no"/>) has been Approved successfully!</t>
												<t t-if="state == 'Rejected'">Material request(<t t-esc="request_no"/>) has been rejected successfully!</t>
												<br/><br/>
												<a class="btn btn-secondary"
													style="background-color: #f7941d !important;"
													t-att-href="'/web'">
													Back To Dashboard
													&amp;nbsp;
													<i class="fa fa-arrow-circle-right "/>
												</a>
											</div>
										</div>
									</div>
								</div>
							</div>
						</section>
					</div>
				</div>
			</t>
		</template>
