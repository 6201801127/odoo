<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="kwdms_hr_storage" model="kw_dms.storage">
            <field name="name">HR+</field>  
            <field name="save_type">file</field>          
            <field name="activate_revision">True</field>          
            <field name="activate_versioning">True</field>    
        </record>

        <record id="kwdms_digilocker_storage" model="kw_dms.storage">
            <field name="name">Digi Locker</field>  
            <field name="save_type">file</field>          
            <field name="activate_revision">True</field>  
        </record>

       
        <record id="kwdms_all_access_admin_grp" model="kw_dms_security.access_groups">
            <field name="name">All Folder Access Admin Group</field>  
            <field name="perm_create">True</field>          
            <field name="perm_read">True</field>  
            <field name="perm_write">True</field>          
            <field name="perm_unlink">True</field>  
            <field name="explicit_users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

     
       
    </data>
</odoo>
