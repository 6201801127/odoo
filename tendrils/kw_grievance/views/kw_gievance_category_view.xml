<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="kw_grievance_category_master_form_view">
			<field name="name">kw Grievance Category Master form view</field>
			<field name="model">kw.grievance.category</field>
			<field name="priority" eval="15"/>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<group>
								<field name="main_category"/>
								<field name="sub_category"/>

							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>
        <record model="ir.ui.view" id="kw_grievance_category_master_tree_view">
			<field name="model">kw.grievance.category</field>
			<field name="arch" type="xml">
				<tree>
					<field name="main_category"/>
					<field name="sub_category"/>

				</tree>
			</field>
		</record>
        <record model="ir.actions.act_window" id="kw_grievance_category_master_action_window">
			<field name="name">Grievance Category Master</field>
			<field name="res_model">kw.grievance.category</field>
			<field name="view_mode">tree,form</field>

		</record>
     
	</data>
</odoo>