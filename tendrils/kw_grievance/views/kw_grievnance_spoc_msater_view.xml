<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
		<record model="ir.ui.view" id="kw_grievance_spoc_master_form_view">
			<field name="name">kw Grievance Spoc Master form view</field>
			<field name="model">kw_grievance_spoc_master</field>
			<field name="priority" eval="15"/>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<group>
								<field name="spoc_person_id"/>
							
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

        <record model="ir.ui.view" id="kw_grievance_spoc_master_tree_view">
            <field name="model">kw_grievance_spoc_master</field>
            <field name="arch" type="xml">
                <tree >
                    <field name="spoc_person_id"/>

                </tree>
            </field>
        </record>


        <record model="ir.actions.act_window" id="kw_grievance_spoc_master_action_window">
            <field name="name">Grievance SPOC Master</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_grievance_spoc_master</field>
            <field name="view_mode">tree,form</field>

        </record>

    </data>

</odoo>