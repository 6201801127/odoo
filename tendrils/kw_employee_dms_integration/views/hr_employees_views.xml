<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!--START : kwemp_identity_docs, view -->


        <record id="kwemp_identity_docs_integration_view_form" model="ir.ui.view">
            <field name="name">kwemp_identity_docs.form</field>
            <field name="model">kwemp_identity_docs</field>
            <field name="inherit_id" ref="kw_employee.kwemp_identity_docs_view_form" />
            <field name="arch" type="xml">
               
                <field name="uploaded_doc" position="replace">
                    <field name="content_file" widget="binary" filename="file_name" />
                    <field name="file_name" invisible="1"/>
                </field>                

            </field>
        </record>

        <record model="ir.ui.view" id="kwemp_identity_docs_integration_view_tree">
            <field name="name">kwemp_identity_docs.tree</field>
            <field name="model">kwemp_identity_docs</field>
             <field name="inherit_id" ref="kw_employee.kwemp_identity_docs_view_tree"/>
            <field name="arch" type="xml">
             
                   
                 <field name="uploaded_doc" position="replace">
                     <field name="content_file" filename="file_name" />
                     <field name="file_name" invisible="1" />
                </field>  
               
            </field>
        </record>

        <!--END : kwemp_identity_docs, view -->

