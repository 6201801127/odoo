<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="kw_kwantify_integration.list">
      <field name="name">kw_kwantify_integration list</field>
      <field name="model">kw_kwantify_integration_log</field>
      <field name="arch" type="xml">
        <tree 
        default_order="create_date desc"
        >
          <field name="create_date"/>
          <field name="name"/>
          <field name="request_params"/>
        </tree>
      </field>
    </record>
   

    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="kw_kwantify_integration_log_action_window">
      <field name="name">Integration Sync Log</field>
      <field name="res_model">kw_kwantify_integration_log</field>
      <field name="view_mode">tree,form</field>
    </record>
   


    <!--   <menuitem id="menu_kw_odoo_kwantify_resource_sync_form"
            parent="base.menu_automation" action="kw_odoo_kwantify_resource_sync.action_window" sequence="20"/> -->
    
   
  </data>
</odoo>