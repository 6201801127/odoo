<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="kw_sendwish_email_template" model="mail.template">
          <field name="name">Greetings e-mail template</field>
          <field name="email_from">${object.create_uid.email}</field>
          <field name="subject">${object.subject}</field>
          <field name="email_to">${object.send_wish_to.work_email}</field>
         <!--  <field name="lang">${object.lang}</field> -->
          <field name="model_id" ref="kw_greetings.model_kw_greetings_send_wishes"/>
          <field name="auto_delete" eval="False"/>
            <field name="body_html">
                    <![CDATA[
        
                            ${object.template_body}
                            
                    ]]>
            </field>
       </record>

    </data>
</odoo>