<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="starlight_migration_log_tree_view">
            <field name="name">Reward Recognition Action Log Tree</field>
            <field name="model">starlight_migration_log</field>
            <field name="arch" type="xml">
                <tree create='0' delete='0' edit='0'>
                    <field name="migration_sequence" invisible='1'/>
                    <field name="migrated_rnr_id" string='Starlight Reference'/>
                    <field name="employee_id"/>
                    <field name ='nominee_id'/>
                    <field name ='department_id'/>
                    <field name ='division'/>
                    <field name ='section'/>
                    <field name ='practise'/>
                    <field name ='job_id'/>
                    <field name ='location'/>
                    <field name="migrated_on"/>
                    <field name="migrated_to"/>
                    <field name ='migration_remark'/>
                    <field name ='migrated_state'/>                    
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="starlight_migration_log_actions_window">
			<field name="name">Migration Log</field>
			<field name="res_model">starlight_migration_log</field>
			<field name="view_mode">tree</field>
		</record>
    </data>
</odoo>