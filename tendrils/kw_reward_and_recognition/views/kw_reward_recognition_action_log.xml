<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="reward_recognition_action_log_tree_view">
            <field name="name">Reward Recognition Action Log Tree</field>
            <field name="model">reward_recognition_action_log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="action_taken_by"/>
                    <field name="action_taken_on"/>
                    <field name="action_remark" />
                    <field name="state"/>
                    <field name ='rnr_id' invisible='1'/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="reward_recognition_log_actions_window">
			<field name="name">User Action Log</field>
			<field name="res_model">reward_recognition_action_log</field>
			<field name="view_mode">tree</field>
		</record>
    </data>
</odoo>