<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sequence_reward_and_recognition" model="ir.sequence">
        <field name="name">STARLIGHT</field>
        <field name="code">starlight</field>
        <field name="active">TRUE</field>
        <field name="prefix">SL</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
     </record>

     <record id="sequence_starlight_reason" model="ir.sequence">
        <field name="name">STARLIGHT REASON</field>
        <field name="code">reason_master</field>
        <field name="active">TRUE</field>
        <field name="prefix">SR</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
     </record>

</odoo>
