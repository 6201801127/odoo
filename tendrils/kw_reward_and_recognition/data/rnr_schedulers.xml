<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="rnr_nomination_reminder" model="ir.cron">
            <field name="name">Starlight | Nomination Reminder</field>
            <field name="model_id" ref="model_reward_and_recognition"/>
            <field name="state">code</field>
            <field name="code">model._nomination_reminder()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">1</field>
            <field name="active" eval="False"/>
        </record>

        <record id="rnr_congratulation_reminder" model="ir.cron">
            <field name="name">Starlight | Congratulation Mailer</field>
            <field name="model_id" ref="model_reward_and_recognition"/>
            <field name="state">code</field>
            <field name="code">model._congratulation_reminder()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">1</field>
            <field name="active" eval="False"/>
        </record>
        
        <record id="starlight_migrate_to_next_month_scheduler" model="ir.cron">
            <field name="name">Starlight | Migrate to next Month</field>
            <field name="model_id" ref="model_reward_and_recognition"/>
            <field name="state">code</field>
            <field name="code">model._migrate_startlight_to_next_month()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">1</field>
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>