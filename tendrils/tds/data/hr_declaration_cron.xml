<odoo>
    <data noupdate="1">
        <record id="hr_declaration_cron" model="ir.cron">
            <field name="name">TDS: HR Declaration Compute Tax</field>
            <field name="model_id" ref="model_hr_declaration"/>
            <field name="state">code</field>
            <field name="code">model.hr_declaration_cron()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>


        <record id="hr_declaration_creation_cron" model="ir.cron">
            <field name="name">TDS: Create HR Declaration</field>
            <field name="model_id" ref="model_hr_declaration"/>
            <field name="state">code</field>
            <field name="code">model.create_default_tds()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>

        <record id="hr_declaration_declaration_update" model="ir.cron">
            <field name="name">TDS: Remainder For IT Declaration</field>
            <field name="model_id" ref="model_hr_declaration"/>
            <field name="state">code</field>
            <field name="code">model.fillup_it_declaration()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>

        <record id="hr_declaration_user_access_update" model="ir.cron">
            <field name="name">TDS: User Access For IT Declaration</field>
            <field name="model_id" ref="model_hr_declaration"/>
            <field name="state">code</field>
            <field name="code">model.user_access_for_tds()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
        <record id="create_ex_emp_record" model="ir.cron">
            <field name="name">TDS : Crete Ex-employee TDS</field>
            <field name="model_id" ref="model_hr_declaration"/>
            <field name="state">code</field>
            <field name="code">model.create_ex_emp_tds()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>