<odoo>
    <data>
        <record id="employee_pan_view_tree" model="ir.ui.view">
            <field name="name">Employee Update PAN</field>
            <field name="model">kw_employee_update_pan_log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="employee_id" />
                    <field name="is_correct"/>
                    <field name="skips_check" />
                </tree>
            </field>
        </record>
                        
        <record model="ir.actions.act_window" id="action_update_emp_pan">
            <field name="name">Employee Update PAN</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_employee_update_pan_log</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>
        <menuitem name="PAN Upadate Log"
                parent="disable_debug_mode.menu_technical_developer"
                id="employee_pan_menu_id"
                sequence="27"
                action="action_update_emp_pan"
        />


        <record id="employee_project_data_view_tree" model="ir.ui.view">
            <field name="name">Employee project</field>
            <field name="model">kw_emp_profile_employee_project_data</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="employee_id" />
                    <field name="emp_proj_name"/>
                    <field name="emp_proj_manager" />
                    <field name="emp_proj_start_date"/>
                    <field name="duration"/>
                    <!-- <field name="emp_id"/> -->
                </tree>
            </field>
        </record>
        <record id="emp_project_data_action" model="ir.actions.act_window">
            <field name="name">Employee project</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">kw_emp_profile_employee_project_data</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>

        <menuitem id="project_assign_emp_menu"
                  name="Project Data"
                  parent="kw_employee.kw_master_menu"
                  action="emp_project_data_action"
                  sequence="1001"/>
    </data>
</odoo>
