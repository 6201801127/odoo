<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="kw_lv_cron" model="ir.cron">
            <field name="name">Auto Approve Scheduler || Local Visit</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_lv_apply"/>
            <field name="code">model.LocalVisitAutoApprove()</field>
            <field name="state">code</field>
        </record>
        <record id="kw_lv_cron_for_monthly_notification" model="ir.cron">
            <field name="name">Monthly Mail of Auto Approved Scheduler || Local Visit</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_lv_apply"/>
            <field name="code">model.LocalVisitAutoApproveMonthlyMail()</field>
            <field name="state">code</field>
        </record>
        <record id="kw_lv_cron_for_reminder_notification" model="ir.cron">
            <field name="name">Reminder Mail Scheduler || Local Visit</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_lv_apply"/>
            <field name="code">model.LocalVisitAutoReminders()</field>
            <field name="state">code</field>
        </record>
    </data>
</odoo>