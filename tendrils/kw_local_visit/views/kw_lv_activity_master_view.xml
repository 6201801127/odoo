<odoo>
	<data>
		<record id="kw_lv_activity_master_action_window" model="ir.actions.act_window">
			<field name="type">ir.actions.act_window</field>
			<field name="name">Local visit Activity</field>
			<field name="res_model">kw_lv_activity_master</field>
			<field name="view_mode">tree,form</field>
			<field name="view_type">form</field>
			<field name="target">current</field>
			<field name="domain">[]</field>
		</record>

		<record id="kw_lv_activity_master_tree" model="ir.ui.view">
			<field name="name">kw_lv_activity_master.tree</field>
			<field name="model">kw_lv_activity_master</field>
			<field name="arch" type="xml">
				<tree string="Local visit Activity">
					<field name="name"/>
					<field name="description"/>
				</tree>
			</field>
		</record>

		<record id="kw_lv_activity_master_master_form" model="ir.ui.view">
			<field name="name">kw_lv_activity_master.form</field>
			<field name="model">kw_lv_activity_master</field>
			<field name="arch" type="xml">
				<form string="Local visit Activity">
					<sheet>
						<group>
							<field name="name"/>
							<field name="description"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>