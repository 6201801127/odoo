<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_kw_survey_result_report_tree" model="ir.ui.view">
            <field name="name">view.kw_survey_result_report.tree</field>
            <field name="model">kw_survey_result_report</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree create="false" delete="false" edit="false" import="false">
                    <field name="employee" />
                    <field name="designation" />
                    <field name="department" />
                    <field name="division" />
                    <field name="work_location" />
                    <field name="question" />
                    <field name="answer" widget="html"/> 
                </tree>
            </field>
        </record>
              
        <record id="action_kw_survey_result_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Survey Result</field>
            <field name="res_model">kw_survey_result_report</field>
            <field name="view_mode">tree</field>
<!--            <field name="view_type">form</field>-->
            <field name="target">current</field>
            <field name="domain">[]</field>
            <field name="context">\{}</field>
            <field name="help" type="html">
                <p>
                    No result found...
                </p>
            </field>
        </record>
       
    </data>
</odoo>
