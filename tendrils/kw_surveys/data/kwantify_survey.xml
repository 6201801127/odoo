<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model="kw_survey_type" id="kw_surveys_survey_type">
            <field name="name">Covid-19 Survey</field>
            <field name="code">covid19survey</field>
        </record>
        <record id="kw_surveys_survey_type_form" model="survey.survey">
            <field name="title">Work From Home Survey</field>
            <field name="stage_id" ref="survey.stage_permanent"/>
            <field name="auth_required" eval="False"/>
            <field name="users_can_go_back" eval="True"/>
            <field name="survey_type" ref="kw_surveys_survey_type"/>
            <field name="description"></field>
            <field name="thank_you_message">&lt;p&gt;Thanks for submitting feedback.&lt;/p&gt;</field>
        </record>
        <record model="survey.page" id="kw_survey_page_1">
            <field name="title">Tendrils Survey</field>
            <field name="survey_id" ref="kw_surveys_survey_type_form" />
            <field name="sequence">1</field>
            <field name="description"></field>
        </record>

        <record model="survey.question" id="kw_survey_question_1">
            <field name="page_id" ref="kw_survey_page_1" />
            <field name="sequence">1</field>
            <field name="question">Mode of work</field>
            <field name="type">simple_choice</field>
            <field name="column_nb">12</field>
            <field name="constr_mandatory" eval="True" />
        </record>
        <record model="survey.label" id="kw_survey_question_1_choice_1">
            <field name="question_id" ref="kw_survey_question_1"/>
            <field name="sequence">1</field>
            <field name="value">WFH (Work from Home)</field>
        </record>
        <record model="survey.label" id="kw_survey_question_1_choice_2">
            <field name="question_id" ref="kw_survey_question_1"/>
            <field name="sequence">2</field>
            <field name="value">WFO (Work from Office)</field>
        </record>

        <record model="survey.question" id="kw_survey_question_2">
            <field name="page_id" ref="kw_survey_page_1" />
            <field name="sequence">2</field>
            <field name="question">Current location</field>
            <field name="type">simple_choice</field>
            <field name="column_nb">12</field>
            <field name="comments_allowed">True</field>
            <field name="comments_message">If outside, please mention the name of current location :</field>
            <field name="comment_count_as_answer">True</field>
            <field name="constr_mandatory" eval="True" />
        </record>
        <record model="survey.label" id="kw_survey_question_2_choice_1">
            <field name="question_id" ref="kw_survey_question_2"/>
            <field name="sequence">1</field>
            <field name="value">Bhubaneswar</field>
        </record>
        
