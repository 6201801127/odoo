<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_kw_qualification_master_category" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Qualification</field>
            <field name="res_model">kw_qualification_master</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="target">current</field>
            <field name="domain">[]</field>
            <field name="context">{'active_test': False}</field>
        </record>

        <record id="view_kw_qualification_master" model="ir.ui.view">
            <field name="name">kw_qualification_master.tree</field>
            <field name="model">kw_qualification_master</field>
            <field name="arch" type="xml">
                <tree string="Qualification">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="campus_drive" widget="boolean_toggle"/>
                    <field name="active" widget="boolean_toggle"/>
                </tree>
            </field>
        </record>

        <record id="view_kw_qualification_master_form" model="ir.ui.view">
            <field name="name">kw_qualification_master.form</field>
            <field name="model">kw_qualification_master</field>
            <field name="arch" type="xml">
                <form string="Qualification" duplicate="false">
                    <sheet>
                        <group>
                           
                                <field name="name"/>
                                <field name="code"/>
                                <field name="sequence"/>
                            <field name="campus_drive"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <menuitem name="Qualification" parent="hr_recruitment.menu_hr_recruitment_configuration" id="menu_kw_job_category" action="action_kw_qualification_master_category"/>

    </data>
</odoo>