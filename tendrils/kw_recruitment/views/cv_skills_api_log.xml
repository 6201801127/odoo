<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="cv_skill_api_log_tree" model="ir.ui.view">
            <field name="name">cv_skills_api_log.tree</field>
            <field name="model">cv_skills_api_log</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Cv Parser Api log tree" create="0" delete="0" edit="0">
                    <field name="payload"/>
                    <field name="response"/>
                    <field name="ocr"/>
                    <field name="status"/>
                    <field name="create_date"/>
                </tree>
            </field>
        </record>

        <record id="cv_skills_api_log_form" model="ir.ui.view">
            <field name="name">cv_skills_api_log.form</field>
            <field name="model">cv_skills_api_log</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Cv Parser Api log form" create="0" delete="0" edit="0">
                    <header>
                    </header>
                    <style>
                        .o_form_view .o_group.o_inner_group > tbody > tr > td.o_td_label{min-width: 200px !important;}
                    </style>
                    <sheet>
                        <group>
                            <group>
                                <field name="payload"/>
                                <field name="response"/>
                                <field name="ocr"/>
                                <field name="status"/>
                                <field name="create_date"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cv_skills_api_log_action_window" model="ir.actions.act_window">
            <field name="name">Cv Parser API Response log</field>
            <field name="res_model">cv_skills_api_log</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
        </record>

        <menuitem id="cv_skills_api_log_action_menu"
                  parent="hr_recruitment.menu_hr_recruitment_configuration"
                  name="CV Parser API Response log"
                  sequence="605"
                  groups="base.group_no_one"
                  action="cv_skills_api_log_action_window"/>

    </data>
</odoo>
