<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="cv_rating_api_response_log_tree" model="ir.ui.view">
            <field name="name">cv_rating_api_response_log_tree</field>
            <field name="model">cv_rating_api_response_log</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree create="0" delete="0" edit="0" import="0">
                    <field name="applicant_id"/>
                    <field name="payload"/>
                    <field name="response"/>
                    <field name="status"/>
                    <field name="rating"/>
                    <field name="status_code"/>
                    <field name="create_date"/>
                </tree>
            </field>
        </record>


        <record id="cv_rating_api_response_log_action_window" model="ir.actions.act_window">
            <field name="name">Cv Rating API Response log</field>
            <field name="res_model">cv_rating_api_response_log</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="cv_rating_api_response_log_action_menu"
                  parent="hr_recruitment.menu_hr_recruitment_configuration"
                  name="CV Rating API Response log"
                  sequence="606"
                  groups="base.group_no_one"
                  action="cv_rating_api_response_log_action_window"/>

    </data>
</odoo>