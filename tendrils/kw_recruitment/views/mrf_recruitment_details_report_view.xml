<?xml version="1.0"?>
<odoo>
  <data>   
    
      <record model="ir.ui.view" id="kw_mrf_button_view_tree">
        <field name="name">Master Recruitment Details</field>
        <field name="model">kw_recruitment_requisition</field>
        <field name="arch" type="xml">
          <tree create="false" edit="false" >              
              <field name="dept_name" string='Department' />  
              <field name="job_position" string ='Position' />
              <field name="project" string ='Project'/>
              <field name="skill_set" string ='Skill' />           
              <field name="type_employment" string ='Type of Employment' /> 
              <field name="duration" string ='Duration'/> 
              <field name="resource" string ='Type of Hiring' /> 
              <field name="branch_id" string ='Job Location'/>  
              <field name="no_of_resource" string ='Numbers' /> 
              <field name="qualification" string ='Qualification'/>   
              <field name="max_exp_year" string ='Exp (Yrs.)' /> 
              <field name="date_of_joining" string ='Deployment Month'/> 
              <field name="create_uid" string ='Responsibility'/> 
              <field name="state" string ='Status' /> 
          </tree>
        </field>
      </record>


        <record model="ir.actions.act_window" id="action_mrf_view_tree">
            <field name="name">MRF Report</field>
            <field name="res_model">kw_recruitment_requisition</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="kw_mrf_button_view_tree"/>
        </record>
    

      <menuitem parent="hr_recruitment.report_hr_recruitment" id="mrf_report_menu"
      name="Manpower Requisition Form Report"
      action="action_mrf_view_tree"/>

    </data>
</odoo>