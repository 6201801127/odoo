<?xml version="1.0"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="kw_ir_attachment_inherited">
            <field name="model">ir.attachment</field>
            <field name="inherit_id" ref="mail.view_document_file_kanban"/>
            <field name="arch" type="xml">
            
                <xpath expr="//div[@class='oe_kanban_bottom_right']" position="replace">
                </xpath>
                <xpath expr="//div[@class='o_kanban_record_body']//field[@name='res_name']" position="replace">
                    <field name="job_title"/>
                </xpath>
                <xpath expr="//div[@class='o_kanban_record_body']//field[@name='job_title']" position="after">
                    <field name="job_position"/>
                </xpath>
                
            </field>
        </record>
        <record model="ir.ui.view" id="kw_ir_attachment_inherited_field">
            <field name="model">ir.attachment</field>
            <field name="inherit_id" ref="base.view_attachment_form"/>
            <field name="arch" type="xml">
            
                <xpath expr="//group/field[@name='res_name']" position="after">
                    <field name="job_title"  invisible="1"/>
                    <field name="job_position"  invisible="1"/>

                </xpath>
    
                
            </field>
        </record>
    </data>
</odoo>