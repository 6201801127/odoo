<?xml version="1.0"?>
<odoo>
  <data>   
    
      <record model="ir.ui.view" id="kw_candidates_interviewed_view_tree">
        <field name="name">Recruitment candidates interviewed Details</field>
        <field name="model">hr.applicant</field>
        <field name="arch" type="xml">
          <tree create="false" edit="false" >  
                <field name="partner_name" string='Name' />
                <field name="job_id" string='Designation'/>  
                <field name="department_id" string='Dept.'/>  
                <field name="qualification" string='Qualification' /> 
                <field name="exp_year" string='Experience'/> 
                <field name="evaluators" string='Evaluators'/> 
                <field name="create_date" string='Date'/> 
                <field name="stage_id"  type="row" />   
          </tree>
        </field>
      </record>


        <record model="ir.actions.act_window" id="action_candidates_interviewed_view_tree">
            <field name="name">Recruitment candidates interviewed Details</field>
            <field name="res_model">hr.applicant</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="kw_candidates_interviewed_view_tree"/>
        </record>


