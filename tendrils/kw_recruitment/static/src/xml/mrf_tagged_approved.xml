                                    <span class="fa fa-filter" height="20px" width="20px"></span>
                                </button>
                                <button title="Refresh" class="btn text-center employee_filter dashboard-button btn-outline-primary font-weight-bold refresh_mrf_tagged_dept_portlet" type="button" aria-expanded="true">
                                    <i class="fa fa-repeat"/>
                                </button>
                                
