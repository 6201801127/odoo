                                    <div class="card-header d-flex justify-content-between align-items-center">
                                        <div><p class="h5 mb-0"> FY Filter </p></div>
                                        <div>
                                            <button class="ks_dashboard_item_action  btn dropdown-toggle btn-xs o-no-caret btn" id="close_mrf_tagged_dept_ratio_filter"> <span class="fa fa-times" />
                                            </button>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <table class="table table-borderless" >
                                            <tr>
                                                <td class="font-weight-bold">Choose FY</td>
                                                <td class="font-weight-bold">:</td>
                                                <td>
                                                    <select class="form-control" id="filter-mrf-tagged-dept">
                                                        <option value="0" selected="selected">Select</option>
                                                        <t t-foreach="widget.mrf_fy_filters" t-as="fy">

                                                            <option t-att-value="fy['id']"><t t-esc="fy['name']"/></option>
                                                        </t>
                                                    </select>
                                                </td>
                                            </tr>
                                        </table>
                                        <div class="clearfix"/>
                                        <div class="col-12 text-center"><button class="btn btn-primary btn-sm" id="filter-mrf-tagged-dept-button"> Search </button></div>
                                    </div>
                                </div>
