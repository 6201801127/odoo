<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="recruitment_calendar_tree_view" model="ir.ui.view">
			<field name="name">Recruitment Calendar Tree</field>
			<field name="model">kw_recruitment_requisition</field>
			<field name="type">tree</field>
			<field name="mode">primary</field>
			<field name="arch" type="xml">
				<tree import="false" create="false" default_order='id desc'>
					<field name="order_sequence" invisible="1"/>
					<field name="branch_id"/>
					<field name="dept_name"/>
					<field name="role_id"/>
					<field name="categ_id"/>
					<field name="job_position"/>
					<field name="qualification"/>
					<field name="qualification"/>
				</tree>
			</field>
		</record>
    <record id="recruitment_calendar_act_window" model="ir.actions.act_window">
			<field name="type">ir.actions.act_window</field>
			<field name="name">Recruitment Calendar Report</field>
			<field name="res_model">kw_recruitment_requisition</field>
			<field name="view_mode">tree</field>
			<field name="domain">[]</field>
			<field name="context">{}</field>
			<field name="view_type">form</field>
			<field name="view_ids" eval="[(5, 0, 0),
		(0, 0, {'view_mode': 'tree', 'view_id': ref('view_kw_recruitment_requisition_approve_action_tree')})]"/>

			<!-- <field name="view_id" ref="view_kw_recruitment_requisition_tree"/> -->
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
				    No pending request found..
				</p>
			</field>
		</record>

</odoo>
