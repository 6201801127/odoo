<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
                
        <record id="sequence_kw_manpower_indent_form" model="ir.sequence">
            <field name="name">Manpower Indent Sequence</field>
            <field name="code">self.mif_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix">/%(prefix)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field> 
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="sequence_kw_recruitment_resource_date_range" model="ir.sequence.date_range">
            <field name="date_from">2020-04-01</field>
            <field name="date_to">2021-03-31</field>
            <field name="prefix">20-21</field>
            <field name="sequence_id" ref="sequence_kw_manpower_indent_form"/>
        </record>
        <record id="sequence_kw_recruitment_resource_date_range_2021_2022" model="ir.sequence.date_range">
            <field name="date_from">2021-04-01</field>
            <field name="date_to">2022-03-31</field>
            <field name="prefix">21-22</field>
            <field name="sequence_id" ref="sequence_kw_manpower_indent_form"/>
        </record>
        <record id="sequence_kw_recruitment_resource_date_range_2022_2023" model="ir.sequence.date_range">
            <field name="date_from">2022-04-01</field>
            <field name="date_to">2023-03-31</field>
            <field name="prefix">22-23</field>
            <field name="sequence_id" ref="sequence_kw_manpower_indent_form"/>
        </record>
        <record id="sequence_kw_recruitment_resource_date_range_2023_2024" model="ir.sequence.date_range">
            <field name="date_from">2023-04-01</field>
            <field name="date_to">2024-03-31</field>
            <field name="prefix">23-24</field>
            <field name="sequence_id" ref="sequence_kw_manpower_indent_form"/>
        </record>
        <record id="sequence_kw_recruitment_resource_date_range_2024_2025" model="ir.sequence.date_range">
            <field name="date_from">2024-04-01</field>
            <field name="date_to">2025-03-31</field>
            <field name="prefix">24-25</field>
            <field name="sequence_id" ref="sequence_kw_manpower_indent_form"/>
        </record>

    </data>
</odoo>