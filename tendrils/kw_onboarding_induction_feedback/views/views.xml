<odoo>
  <data>
    <!-- explicit list view definition -->



    <record model="ir.ui.view" id="kw_feedback_page_list">
      <field name="name">kw_feedback_list</field>
      <field name="model">kw_onboarding_feedback</field>
      <field name="arch" type="xml">
        <tree import="false" create="false">
          <field name="emp_id" string="Employee"/>
          <field name="deg_id" string="Designation"/>
          <field name="dept_id" string="Department"/>
          <field name="work_email" string="Work mail"/>
          <field name="onboard_ind_bool" string="Onboarding and Induction Feedback"/>
          <!-- <field name="hr_ind_bool" string="HR Induction Assessment Form"/>
          <field name="it_ind_bool" string="IT Induction Assessment Form"/>
          <field name="comp_ind_boolean" string="Know Your Company Induction Assessment Form"/> -->
          <field name="status_feedback"/>
        </tree>
      </field>
    </record>
    
    <record id="kw_onboarding_feedback_form" model="ir.ui.view">
      <field name="name">kw_onboarding_induction_feedback_form</field>
      <field name="model">kw_onboarding_feedback</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form import="false" create="false">
        <style>

        </style>
          <header>
            <button string="Submit" name="feedback_submit_button" type="object" 
                    class = "btn-success" attrs="{'invisible':[('submit_bool','=',True)]}" 
                    groups="kw_onboarding_induction_feedback.group_kw_onboarding_induction_user"/>

            <button string="Confirm" name="feedback_reviewer_submit_button" type="object" 
                    confirm="Are you sure to confirm ?"
                    context="{'button':'onboard'}"
                    class = "btn-success" attrs="{'invisible':['|',('onboard_forward_to_manager','=',True),('officer_manger','=',True)]}" 
                    groups="kw_onboarding_induction_feedback.group_kw_onboarding_induction_officer"/>

            <button string="Submit" name="feedback_manager_submit_button" type="object" 
                    confirm="Are you sure to submit the onboarding induction feedback ?"
                    class = "btn-success" attrs="{'invisible':['|','|',('manager_confirm_form','=',True),('submit_bool','=',False),('officer_manger','=',True)]}"
                    groups="kw_onboarding_induction_feedback.group_kw_onboarding_induction_manager"/>
            <button string="Submit" name="feedback_submit_manger_officer_check" type="object" 
                    confirm="Are you sure to submit the onboarding induction feedback ?"
                    class = "btn-success" attrs="{'invisible':['|','|','|',('onboard_forward_to_manager','=',True),('submit_bool','=',False),('officer_manger','=',False),('manager_confirm_form','=',True)]}"
                    />


          </header>
          <sheet>
            <style>
              .table-responsive .o_list_view thead tr:nth-child(1) th{
                background-color: #8585ad;
                color:#fff;
                }
            </style>
            <group>
              <group>
                <field name="emp_id" string="Employee" readonly="1" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
                <field name="deg_id" string="Designation" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
                <field name="dept_id" string="Department" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
                <field name="work_email" string="Work mail"/>
                <field name="remark_reviewer" attrs="{'invisible':[('user_check_bool','=',True)],'required':[('onboard_ind_officer_bool','=',True)]}"/>
                <field name="user_check_bool"  invisible="1"/>
                <field name="officer_manger"  invisible="1"/>


                <field name="form_assign" widget="many2many_tags" readonly="1" invisible="1"/>
                <field name="status_feedback"  invisible="1"/>
                <field name="manager_confirm_form"  invisible = '1'/>
                <field name="tagged_id"  invisible = '1'/>
                <field name ="manager_check_bool"  invisible = '1'/>
                <field name ="onboard_forward_to_manager" invisible = '1'/>
               
                <field name="onboard_ind_bool" string="Onboard" invisible = '1'/>
                <field name="submit_bool" invisible="1" force_save = "1"/>
               
                <field name="onboard_ind_officer_bool" invisible="1"/>
                <field name="reviewer_employees" invisible="1" widget="many2many_tags"/>

              </group>
            
            </group>
            <notebook>
              <page string="Onboarding and Induction Feedback"   
                  attrs="{'invisible':[('onboard_ind_bool', '=', False)],
                         'required':[('onboard_ind_bool','=',True)]}">
                <h5> Did you get off to a good start?
                  <br></br>
                  <br></br>
                  <p>Please take out 5 minutes of your valuable time and complete this Onboarding and Induction Feedback sheet.</p>
                </h5>
                <br></br>
                <br></br>
                  <div class=" table-responsive" style="width:100%">
                    <table class="o_list_view table table-sm table-hover table-striped o_list_view_ungrouped">
                      <tbody class="ui-sortable">
                          <tr>
                          <th class="o_column_sortable"
                            style='background-color: #e6f9ff !important;color:black;width:25%'>Onboarding and Induction Feedback Form
                          </th>
                          <th class="o_column_sortable"
                            style='background-color: #e6f9ff !important;color:black;width:25%'>Option
                          </th>
                          </tr>
                          <tr>
                            <td scope="row" style="width: 55%;">1. Did you feel welcomed by Onboarding SPOC when you reached office on the date of joining?</td>
                            <td>
                              <field name="onboarding_spoc" required='1' widget="radio" options="{'horizontal': false}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('manager_check_bool','=',True),('submit_bool','=',True)]}"/>
                            </td>
                          </tr>
                          <tr>
                            <td scope="row">2. Is your Organizational Tour completed on the first day and toured through all the important places of your workplace?</td>
                            <td>
                              <field name="tour_completed"  widget="radio" options="{'horizontal': false}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('manager_check_bool','=',True),('submit_bool','=',True)]}"/>
                            </td>
                          </tr>
                          <tr>
                              <td scope="row">3. I was assigned with a buddy and got all assistance to settle on the first day.</td>
                              <td>
                                <field name="all_assistance"  widget="radio" options="{'horizontal': true}"  attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('manager_check_bool','=',True),('submit_bool','=',True)]}"/>
                              </td>
                          </tr>
                          <tr>
                              <td scope="row">4. How was your onboarding experience at CSM?</td>
                              <td>
                                  <field name="onboarding_experience"  widget="radio" options="{'horizontal': false}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('manager_check_bool','=',True),('submit_bool','=',True)]}"/>
                              </td>
                          </tr>
                          <tr>
                              <td scope="row">5. Are your all Inductions completed? (Know About the Company, Digital Presence,
                                      IT Induction and HR Induction)</td>
                              <td>
                                <field name="inductions_completed" widget="radio" options="{'horizontal': true}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('manager_check_bool','=',True),('submit_bool','=',True)]}" />
                                <field name="inductions_completed_specify" placeholder="Please specify which Induction is still pending" attrs="{'invisible':[('inductions_completed', 'in', ['yes',False])],
                                                      'required':[('inductions_completed','=',True)],'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                              </td>
                          </tr>
                          <tr>
                              <td scope="row">6. How satisfied are you with the information provided to you during your induction period?</td>
                              <td>
                                <field name="how_satisfied"  widget="radio" options="{'horizontal': true}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}" />
                              </td>
                          </tr>
                          <tr>
                              <td scope="row">7. I have read &amp; understood the HR processes mentioned in the employee handbook.</td>
                              <td>
                                <field name="hr_processes"  widget="radio" options="{'horizontal': true}"  attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                              </td>
                          </tr>
                            <tr>
                              <td scope="row">8. I was provided with workstation, Tendrils user ID and Email ID within 1 day of my joining.</td>
                              <td>
                                <field name="with_workstation" widget="radio" options="{'horizontal': true}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                                <field name="specify" placeholder="Please specify" attrs="{'invisible':[('with_workstation', 'in', ['yes',False])],'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                              </td>
                          </tr>
                            <tr>
                              <td scope="row">9. I am being introduced to my Reporting Authority on the day of joining.</td>
                              <td>
                                <field name="reporting_authority"  widget="radio" options="{'horizontal': false}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                              </td>
                          </tr>
                            <tr>
                              <td scope="row">10. I am confident I have understood how to use Tendrils Portal.</td>
                              <td>
                                <field name="use_kwantify"  widget="radio" options="{'horizontal': false}" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                              </td>
                          </tr>
                            <tr>
                              <td scope="row">11. Please provide any further comments you think would be helpful, including any issues that you would like addressing.</td>
                              <td>
                                <field name="additional_comments" attrs="{'readonly':['|','|',('onboard_ind_officer_bool','=',True),('submit_bool','=',True),('manager_check_bool','=',True)]}"/>
                              </td>
                          </tr>
                          <tr>
                              <td></td>
                              <td></td>
                          </tr>
                      </tbody>
                    </table>
                  </div>
              </page>
            </notebook>
          </sheet>
        </form>
      </field>
    </record>

    <record model="ir.ui.view" id="kw_onboard_induction_answer_list">
      <field name="name">kw_feedback_induction_Answer_list</field>
      <field name="model">onboard_induction_answer_sheet</field>
      <field name="arch" type="xml">
        <tree import="false" create="true">
          <field name="levels_hierarchy"/>
          <field name="many_departments" />
          <field name="procurement_division" />
          <field name="attendance_procedure" />
          <field name="regular_shift" />
          <field name="approves_leaves" />
          <field name="is_en_cashable" />
          <field name="consecutive_medical_leaves" />
          <field name="maternity_leave" />
          <field name="paternity_leave" />
          <field name="special_leave" />
          <field name="tour_settlement" />
          <field name="fixed_holidays" />
          <field name="amount_accidental" />
          <field name="conducts_appraisals" />
          <field name="minimum_effort" />
          <field name="Salary_linked" />
          <field name="dress_code" />
          <field name="use_local_language" />
          <field name="how_many_days" />
          <field name="csm_firewall" />
          <field name="iP_extension_phone" />
          <field name="server_30" />
          <field name="server_9" />
          <field name="extension_number_it" />
          <field name="established_yr" />
          <field name="acronym_system" />
          <field name="csm_values" />
          <field name="crossed_onshore" />
          <field name="landmark_project" />
          <field name="e_Gov_Award" />
          <field name="certified_for_cmmi" />
          <field name="csm_hockey_world_cup" />
          <field name="handling_the_recruitment" />
          <field name="heads_the_hr" />
          <field name="hold_quality" />
          <field name="transformation_framework" />

        </tree>
      </field>
    </record>
    <record id="kw_onboarding_induction_feedback_form" model="ir.ui.view">
      <field name="name">kw_onboarding_induction_answer_form</field>
      <field name="model">onboard_induction_answer_sheet</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group string="HR Induction Assessment Form" width="60%" >
                <field name="levels_hierarchy" string="1. How many levels are there in CSM’s Organizational hierarchy?"/>
                <field name="many_departments" string="2. How many departments are there in CSM?"/>
                <field name="procurement_division" string="3. Procurement Division comes under which Department?" />
                <field name="attendance_procedure" string="4. Which attendance procedure is applicable for people working from Bhubaneswar City Office and Campus?" />
                <field name="regular_shift" string="5. What is the Regular shift timing for CSM?" />
                <field name="approves_leaves" string="6. Who approves your leaves?" />
                <field name="is_en_cashable" string="7. Which Leave Type is en-cashable and can be carried forwarded to the next FY?" />
                <field name="consecutive_medical_leaves" string="8. If you apply ___consecutive medical leaves, Medical certificate needs to be submitted in Tendrils" />
                <field name="maternity_leave" string="9. The Maternity Leave will comprise of how many days of consecutive leave for women employees?" />
                <field name="paternity_leave" string="10. Paternity leave/Maternity leave can be applied how many times in an employee’s total tenure?" />
                <field name="special_leave" string="11. Special Leave can be taken for which reasons?"/>
                <field name="tour_settlement" string="12. Tour Settlement to be done within how many days of return from Tour?"/>
                <field name="fixed_holidays" string="13. CSM provides how many fixed holidays in a year?" />
                <field name="amount_accidental" string="14. What is the total coverage amount for each employee under Accidental policy?" />
                <field name="conducts_appraisals" string="15. In how many Intervals does CSM conducts Appraisals?" />
                <field name="minimum_effort" string="16. What is the minimum effort hours per day to be filled in timesheet?" />
                <field name="Salary_linked" string="17. Is salary linked with Attendance and Timesheet?" />
                <field name="dress_code" string="18. Which dress code is to be followed from Monday to Thursday?" />
                <field name="use_local_language" string="19. One can use Local Language in official meetings." />
                <field name="how_many_days" string="20. We work for how many days a week?" />
              </group>
            </group>
            <group>
              <group string="IT Induction Assessment Form">
                <field name="csm_firewall" string="CSM is using which of the below Firewall ?" />
                <field name="iP_extension_phone" string=" While dialling a local mobile phone number  from IP/Extension Phone, use prefix ?" />
                <field name="server_30" string="Server 30 is which of the below  folder ?"/>
                <field name="server_9" string="Server 9 is which of the below folder ?" />
                <field name="extension_number_it" string="What is the extension number of IT Helpdesk ?" />
              </group>
            </group>
            <group string="Know Your Company Induction Assessment Form">
              <field name="established_yr" string="When was CSM Tech established?" />
              <field name="acronym_system" string="The acronym for our value system is:" />
              <field name="csm_values" string="Which among the below is one of the CSM Values?" />
              <field name="crossed_onshore" string="CSM crossed 100 onshore employees in which year?" />
              <field name="landmark_project" string="In 2009, CSM launched a landmark project in the field of education for the Govt of Odisha. What was it?" />
              <field name="e_Gov_Award" string="SAMS bagged an important award in the form of National e-Gov Award. Which year did it happen?"/>
              <field name="certified_for_cmmi" string="CSM currently is certified with CMMI Level 5 v2.0. Which year did we get certified for CMMI Level 5?" />
              <field name="csm_hockey_world_cup" string="The 2018 hockey world cup was special for the state of Odisha and for CSM too. What role did we play in the world cup?" />
              <field name="handling_the_recruitment" string="Which department is responsible for handling the recruitment, and acts as a liaison between management and CSMers?" />
              <field name="heads_the_hr" string="Who heads the HR department?" />
              <field name="hold_quality" string="Which department ensures the solutions that go out of CSM hold their quality?" />
              <field name="transformation_framework" string=" Our Digital Transformation Framework is also called the 3E framework. Evaluate &amp; Envisage, _____, and Evolve. What’s the missing ‘E’?" />
            </group>
            
            
          </sheet>
        </form>
      </field>
    </record>
    
    <record model="ir.actions.act_window" id="onboarding_induction_feedback_answer_action_window">
      <field name="name">Onboarding feedback Answer</field>
      <field name="res_model">onboard_induction_answer_sheet</field>
      <field name="view_mode">tree,form</field>
      <field name="view_type">form</field>
      <!-- <field name="view_ids" eval="[(5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_onboarding_induction_feedback.kw_feedback_page_list')}),
