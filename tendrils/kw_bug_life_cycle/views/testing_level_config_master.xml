<odoo>
    <data>
        <!-- Tree View Of Testing level Config Master Master -->
        <record id="kw_testing_config_view_tree" model="ir.ui.view">
            <field name="name">Testing level Config Master</field>
            <field name="model">testing_level_config_master</field>
            <field name="arch" type="xml">
                <tree delete="false" class="bug_raise_tree">
                    <field name="name"/>
                    <field name="active" widget="boolean_toggle" invisible="1"/>
                </tree>
            </field>
        </record>
        
        <!-- Form View Of Testing level Config Master Master -->
        <record id="kw_testing_config_view_form" model="ir.ui.view">
            <field name="name">Testing level Config Master</field>
            <field name="model">testing_level_config_master</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="active" widget="boolean_toggle"/>
                            </group> 
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- Testing level Config  Master Action -->

        <record model="ir.actions.act_window" id="kw_testing_config_action_window">
            <field name="name">Testing level Config Master</field>
            <field name="res_model">testing_level_config_master</field>
            <field name="view_mode">tree,form</field> 
            <field name="view_type">form</field>
        </record>
    </data>
</odoo>