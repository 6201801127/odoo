<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="0">
        <record id="kw_bug_type_master1" model="bug_type_master">
            <field name="name">L1-Duplicate Data Error</field>
            <field name="code">L1</field>
        </record>
        <record id="kw_bug_type_master2" model="bug_type_master">
            <field name="name">L2-Submit/Save/Update/Delete/Confirmation Message Error</field>
            <field name="code">L2</field>
        </record>
        <record id="kw_bug_type_master3" model="bug_type_master">
            <field name="name">L3-Invalid Date Error</field>
            <field name="code">L3</field>
        </record>
        <record id="kw_bug_type_master4" model="bug_type_master">
            <field name="name">L4-Mail ID Error</field>
            <field name="code">L4</field>
        </record>
        <record id="kw_bug_type_master5" model="bug_type_master">
            <field name="name">L5-Max. Length/Data Type Error</field>
            <field name="code">L5</field>
        </record>
        <record id="kw_bug_type_master6" model="bug_type_master">
            <field name="name">L6-Dependency Data Error</field>
            <field name="code">L6</field>
        </record>
        <record id="kw_bug_type_master7" model="bug_type_master">
            <field name="name">L7-Functionality Error</field>
            <field name="code">L7</field>
        </record>
        <record id="kw_bug_type_master8" model="bug_type_master">
            <field name="name">S2-URL Manipulation/Cross-Site Scripting Error</field>
            <field name="code">S2</field>
        </record>
        <record id="kw_bug_type_master9" model="bug_type_master">
            <field name="name">S3-Session Check Error</field>
            <field name="code">S3</field>
        </record>
        <record id="kw_bug_type_master10" model="bug_type_master">
            <field name="name">B1-Look and Feel (Due to any Browser Compatibility)</field>
            <field name="code">B1</field>
        </record>
        <record id="kw_bug_type_master11" model="bug_type_master">
            <field name="name">B2-Button Or any Major Functionality Not Working (Due to any Browser Compatibility)</field>
            <field name="code">B2</field>
        </record>
        <record id="kw_bug_type_master12" model="bug_type_master">
            <field name="name">C1-Spelling Error/Tab Sequence/Default Focus</field>
            <field name="code">C1</field>
        </record>
        <record id="kw_bug_type_master13" model="bug_type_master">
            <field name="name">C2-Error Related To Print Page And Other Cosmetic Issue</field>
            <field name="code">C2</field>
        </record>
        <record id="kw_bug_type_master14" model="bug_type_master">
            <field name="name">L8-Mandatory Validation</field>
            <field name="code">L8</field>
        </record>
        <record id="kw_bug_type_master15" model="bug_type_master">
            <field name="name">S1-SQL Injection Error</field>
            <field name="code">S1</field>
        </record>
    </data>
    <data>
        <record id="raise_bug_code" model="ir.sequence">
            <field name="name">Raise Defect Code</field>
            <field name="code">self.kw_raise_defect</field>
            <field name="prefix">||</field>
            <field name="padding">4</field>
        </record>

        <record id="upload_test_case_code" model="ir.sequence">
            <field name="name">Upload Test Case Code</field>
            <field name="code">self.kw_test_case_upload</field>
            <field name="prefix">_</field>
            <field name="padding">4</field>
        </record>
    </data>
    <data>
        <!-- Severity Master data files -->
        <record id="kw_severity_master1" model="severity_master">
            <field name="name">Minor</field>
        </record>
        <record id="kw_severity_master2" model="severity_master">
            <field name="name">Major</field>
        </record>
        <record id="kw_severity_master3" model="severity_master">
            <field name="name">Critical</field>
        </record>
        <record id="kw_severity_master4" model="severity_master">
            <field name="name">Cosmetic</field>
        </record>
    </data>
    <data>
        <!-- Priority Master data files -->
        <record id="kw_priority_master1" model="priority_master">
            <field name="name">Low</field>
        </record>
        <record id="kw_priority_master2" model="priority_master">
            <field name="name">Medium</field>
        </record>
        <record id="kw_priority_master3" model="priority_master">
            <field name="name">High</field>
        </record>
        <record id="kw_priority_master4" model="priority_master">
            <field name="name">Showstopper</field>
        </record>
    </data>
    
    <!-- Testing Level Config data files -->
    <data>
        <record id="kw_testing_level_master1" model="testing_level_config_master">
            <field name="name">Component</field>
        </record>
        <record id="kw_testing_level_master2" model="testing_level_config_master">
            <field name="name">Integration</field>
        </record>
        <record id="kw_testing_level_master3" model="testing_level_config_master">
            <field name="name">System</field>
        </record>
        <record id="kw_testing_level_master4" model="testing_level_config_master">
            <field name="name">Regression</field>
        </record>
        <record id="kw_testing_level_master5" model="testing_level_config_master">
            <field name="name">Pre UAT</field>
        </record>
        <record id="kw_testing_level_master6" model="testing_level_config_master">
            <field name="name">UAT</field>
        </record>
        <record id="kw_testing_level_master7" model="testing_level_config_master">
            <field name="name">Piloting</field>
        </record>
        <record id="kw_testing_level_master8" model="testing_level_config_master">
            <field name="name">LIVE</field>
        </record>
        <record id="kw_testing_level_master9" model="testing_level_config_master">
            <field name="name">Maintenance</field>
        </record>
        <record id="kw_testing_level_master10" model="testing_level_config_master">
            <field name="name">Prototype</field>
        </record>
        <record id="kw_testing_level_master11" model="testing_level_config_master">
            <field name="name">Opportunity</field>
        </record>
        <record id="kw_testing_level_master12" model="testing_level_config_master">
            <field name="name">SIT</field>
        </record>
        <record id="kw_testing_level_master13" model="testing_level_config_master">
            <field name="name">Functional</field>
        </record>
    </data>
    <!--    Scheduler for sla configuration -->
    <data noupdate="1">
        <record id="sla_config_mail_cron " model="ir.cron">
            <field name="name"> Bug Life Cycle: SLA Configuration Mail Cron</field>
            <field name="model_id" ref="model_kw_raise_defect"/>
            <field name="state">code</field>
            <field name="code">model.sla_config_mail_cron()</field>
            <field name='interval_number'>3</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
        </record>
        
        <record id="daily_pm_mail_cron" model="ir.cron">
            <field name="name">Bug Life Cycle: Daily PM Mail Cron</field>
            <field name="model_id" ref="model_kw_raise_defect"/>
            <field name="state">code</field>
            <field name="code">model.daily_mail_to_pm_mail_cron()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 06:30:00')"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active">False</field>
            <field name="day_of_week" eval="'1-5'" /> <!-- Monday to Friday -->
        </record>
        
        <record id="developer_accept_remainder_mail_cron" model="ir.cron">
            <field name="name"> Bug Life Cycle: Developer Accept Mail Cron</field>
            <field name="model_id" ref="model_kw_raise_defect"/>
            <field name="state">code</field>
            <field name="code">model.developer_remainder_mail_cron()</field>
            <field name='interval_number'>1</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
        </record>
        
        <record id="daily_escalation_mail_cron" model="ir.cron">
            <field name="name">Bug Life Cycle: Daily Reminder/Escalation Mail Cron</field>
            <field name="model_id" ref="model_kw_raise_defect"/>
            <field name="state">code</field>
            <field name="code">model.daily_mail_to_developer_escalation_mail_cron()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 16:30:00')"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active">False</field>
            <field name="day_of_week" eval="'1-5'" /> <!-- Monday to Friday -->
        </record>
    </data>
    
    <template id="bug_report_backend" inherit_id="web.assets_backend" active="True">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/kw_bug_life_cycle/static/src/scss/bug_tree_view.scss"/>
        </xpath>
    </template>
    
    <data>
        <record id="kw_tc_exce_pass" model="kw_tc_execution_type">
            <field name="name">Passed</field>
            <field name="code">PASS</field>
        </record>
        <record id="kw_tc_exce_failed" model="kw_tc_execution_type">
            <field name="name">Failed</field>
            <field name="code">FAIL</field>
        </record>
        <record id="kw_tc_exce_ntexc" model="kw_tc_execution_type">
            <field name="name">Not Executed</field>
            <field name="code">NOTEXC</field>
        </record>
    </data>
</odoo>