<odoo>
  <data>
    <record model="ir.ui.view" id="payroll_data_view">
      <field name="name">Payroll Data</field>
      <field name="model">payroll.data</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="employee_id"/>
              <field name="year" attrs="{'required':[('month','!=',False)]}"/>
              <field name="month" attrs="{'required':[('year','!=',False)]}"/>
              <field name="page_no"/>
              <field name="page_size"/>
            </group>
            <footer>
              <button name="get_data" type="object" string="Get Data" class="btn-primary" groups=""/>
            </footer>
          </sheet>
        </form>
      </field>
    </record>

    <!-- <record model="ir.actions.act_window" id="payroll_data_action_window">
