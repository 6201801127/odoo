<odoo>
    <data>
      <record model="ir.ui.view" id="kw_attendance_api_log_view">
        <field name="name">Payroll Attendance API Log</field>
        <field name="model">kw_attendance_log</field>
        <field name="arch" type="xml">
          <form>
            <sheet>
              <group>
                <field name="create_date" string="Requested On"/>
               <field name="request_params"/>
               <field name="response_result"/>
              </group>
            </sheet>
          </form>
        </field>
      </record>

      <!-- kw_payroll_log tree view -->
      <record id="kw_payroll_attendance_api_log_tree" model="ir.ui.view">
          <field name="name">kw_payroll_log.view.tree</field>
          <field name="model">kw_attendance_log</field>
          <field name="arch" type="xml">
              <tree>
                  <!-- Add your fields here -->
                  <field name="create_date" string="Requested On"/>
                  <field name="request_params"/>
              </tree>
          </field>
      </record>

      <record model="ir.actions.act_window" id="kw_payroll_attendance_api_log_action_window">
        <field name="name">Payroll Attendance API Log</field>
        <field name="res_model">kw_attendance_log</field>
        <field name="view_mode">tree,form</field>
      </record>

      <!-- <menuitem name="Payroll Attendance API Log" id="payroll_attendance_log_menu" parent="hr_payroll.menu_hr_payroll_configuration" sequence="18" action="kw_payroll_attendance_api_log_action_window"/> -->
    </data>
  </odoo>