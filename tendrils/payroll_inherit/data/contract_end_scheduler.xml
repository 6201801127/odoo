<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
		<record id="csm_contract_end_scheduler" model="ir.cron">
			<field name="name">Scheduler for Contract End</field>
			<field name="model_id" ref="model_hr_contract"/>
			<field name="state">code</field>
			<field name="code">model.contract_end()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">months</field>
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">1</field>
			<field name="active" eval="False" />
		</record>
		<record id="resource_ctc_sync_scheduler" model="ir.cron">
			<field name="name">Scheduler for Resource CTC Sync</field>
			<field name="model_id" ref="model_bank_details_update"/>
			<field name="state">code</field>
			<field name="code">model.sync_resource_ctc()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">months</field>
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">1</field>
			<field name="active" eval="False" />
		</record>
	</data>
</odoo>