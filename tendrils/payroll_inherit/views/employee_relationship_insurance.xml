<odoo>
    <data>
        <record model="ir.ui.view" id="employee_relationship_insurance_list">
            <field name="name">Family insurance Form</field>
            <field name="model">employee_family_insurance_report</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="download_file" invisible="1"/>
                            <field name="finacial_year" invisible="1"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="download_excl_insurance_report" string="Download employee report" type="object" class="btn btn-primary"/>
                        <button string="Cancel" class="btn btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="employee_relationship_insurance_action" model="ir.actions.act_window">
            <field name="name">Health Insurance Excel Export</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">employee_family_insurance_report</field>
            <field name="view_mode">form</field>
            <field name="view_type">form</field>
            <field name="target">new</field>
            <field name="help" type="html">
                <p>
                    created
                </p>
            </field>
        </record>
    </data>
</odoo>
