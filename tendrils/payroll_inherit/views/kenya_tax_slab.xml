<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<!-- model.name tree view -->
		<record id="kenya_tax_slab_view_tree" model="ir.ui.view">
			<field name="name">Tax Slab Master</field>
			<field name="model">kenya_tax_slab</field>
			<field name="arch" type="xml">
				<tree editable="top">
					<field name="amount_from"/>
					<field name="amount_to"/>
					<field name="rate"/>
				</tree>
			</field>
		</record>
		

		<record id="kenya_tax_slab_action" model="ir.actions.act_window">
			<field name="name">Tax Slab Master</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">kenya_tax_slab</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No Records to show...
				</p>
			</field>
		</record>
	
		<menuitem name="Tax Slab Master" id="kenya_tax_slab_menu" parent="hr_payroll.menu_hr_payroll_configuration" action="kenya_tax_slab_action" sequence="100"/>

	</data>
</odoo>
