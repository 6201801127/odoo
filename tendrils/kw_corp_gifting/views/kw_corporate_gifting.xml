<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>      
		<record id="kw_corporate_gifting_tree" model="ir.ui.view">
			<field name="name">Corporate Gifting Tree</field>
			<field name="model">kw_corporate_gifting</field>
			<field name="arch" type="xml">
				<tree string="Corporate Gifting">
					<field name="occasion_id"/>
					<field name="year"/>
				</tree>
			</field>
		</record>

		<record id="kw_corporate_gifting_form" model="ir.ui.view">
			<field name="name">Corporate Gifting Form</field>
			<field name="model">kw_corporate_gifting</field>
			<field name="arch" type="xml">
				<form string="Job Portal">
					<sheet>
						<group>
							<group>
								<field name="occasion_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
							</group>
							<group>
								<field name="year"/>
								
							</group>
						</group>
						<notebook>
							<page name="guest" string="Guest Information">
								<field name="gifting_guest_lines">
									<tree editable="top">
										<field name="sbu" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
										<field name="guest_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
										<field name="gift_details" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
										<field name="designation" force_save="1"/>
										<field name="organisation" force_save="1"/>
										<field name="address" force_save="1"/>
										<field name="tel" force_save="1"/>
										<field name="state_id" force_save="1"/>
										<field name="delivery_at"/>
										<field name="location"/>
										<field name="category" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
										<field name="tag" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
										<field name="delivered_by" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
									</tree>
								</field>
							</page>
						</notebook>
					</sheet>
				</form>
			</field>
		</record>

		<record id="kw_corporate_gifting_action" model="ir.actions.act_window">
			<field name="type">ir.actions.act_window</field>
			<field name="name">Corporate Gifting</field>
			<field name="res_model">kw_corporate_gifting</field>
			<field name="view_mode">tree,form</field>
			<field name="view_type">form</field>
		</record>

		<!-- <menuitem name="Corporate Gifting" parent="menu_corporate_gifting" id="menu_corporate_gifting_form" action="kw_corporate_gifting_action" sequence="99"/>

 -->
   

	</data>
</odoo>
