		<record id="kw_occasion_master_tree" model="ir.ui.view">
			<field name="name">Occasion Master Tree</field>
			<field name="model">kw_occasion_master</field>
			<field name="arch" type="xml">
				<tree editable="top">
					<field name="sequence" widget="handle"/>
					<field name="name"/>
					<field name="code" groups="base.group_no_one"/>
					<field name="active" widget="boolean_toggle"/>
				</tree>
			</field>
		</record>

