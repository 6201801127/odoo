<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- explicit tree view definition Opening -->
        <record model="ir.ui.view" id="kw_leave_type_master_list">
            <field name="name">Leave Type Master</field>
            <field name="model">kw_leave_type_master</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="leave_code"/>

                    <field name= "for_probationary"   />
                    <field name= "employement_type_ids"  widget="many2many_tags" options="{'no_open': True, 'no_create': True, 'no_create_edit': True}" />


                    <field name="description"/>
                </tree>
            </field>
        </record>
        <!-- explicit tree view definition Ending -->

        <!-- actions views Opening-->
        <record model="ir.actions.act_window" id="kw_leave_type_master_window">
            <field name="name">Leave Type Master</field>
            <field name="res_model">kw_leave_type_master</field>
            <field name="view_mode">tree,form</field>
        </record>
        <!-- actions views Ending-->

        <!-- <menuitem name="Leave Type Master" id="kw_leave_type_master_menu" parent="hr_holidays.menu_hr_holidays_configuration"
         sequence="1"  action="kw_leave_type_master_window" /> -->
    </data>
</odoo>