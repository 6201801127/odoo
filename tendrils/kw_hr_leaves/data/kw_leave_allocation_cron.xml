<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="kw_leave_allocation_update_scheduler" model="ir.cron">
            <field name="name">Leave Allocation/Entitlement Scheduler</field>
            <field name="model_id" ref="model_hr_leave_allocation"/>
            <field name="state">code</field>
            <field name="code">model.leave_allocation_as_per_cycle()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">0</field>
            <!-- <field name="priority">8</field> -->
            <field name="active" eval="False" />
        </record>

        <record id="kw_compute_cf_encashment_scheduler" model="ir.cron">
            <field name="name">Leave Carry Forward &amp; Encashment Computation Scheduler</field>
