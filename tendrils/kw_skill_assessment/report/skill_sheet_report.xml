<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="kw_emp_skill_sheet_client_action" model="ir.actions.client">
        <field name="name">Skill Sheet Client Action</field>
        <field name="tag">skill_sheet_client</field>
        <field name="target">current</field>
    </record>

    <template id="kw_skill_sheet_assets_backend" name="kw_skill_sheet_assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script src="kw_skill_assessment/static/src/js/skill_sheet_report.js" type="text/javascript"/>
            <script type="text/javascript" src="kw_skill_assessment/static/src/js/xlsx.js"/>

        </xpath>
    </template>

</odoo>