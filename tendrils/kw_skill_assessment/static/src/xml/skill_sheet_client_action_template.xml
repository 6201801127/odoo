<templates id="skill_sheet_temp">
    <t t-name="SkillSheetHead">
        <div style="border: 1px solid ##b7b5b5;padding-top:5px;font-weight:bold;">
            <div>
                <div class="filter_view_tb"></div>
            </div>
            <br></br>
            <div>
                <div class="table_view"></div>
            </div>
        </div>
    </t>
    <t t-name="SkillSheetTable">
        <div class="container-fluid px-5 py-3">
            <div class="card">
                <div class="card-body" style="background-color: aliceblue;">
                    <div class="h-100 d-flex align-items-center justify-content-center">
                        <h2>
                            Skill Sheet Report
                        </h2>
                    </div>
                    <div class="h-100 d-flex align-items-center justify-content-center" style="font-size: smaller;">
                        <table class="table-bordered" style="border:1px solid black;text-align:center" id="skill_short_mark_table">
                            <tr>
                                <td style ="background-color: #ffb3b3; width:100px; height:30px; font-size: inherit;">0</td>
                                <td style ="background-color:  #ff8c1a; width:150px; font-size: inherit;">Fresher</td>
                            </tr>
                            <tr>
                                <td style ="background-color: #ffb3b3; font-size: inherit;"> 1-2</td>
                                <td style ="background-color: #80aaff; width:120px; font-size: inherit;">Average</td>
                            </tr>
                            <tr>
                                <td style ="background-color: #ffb3b3; font-size: inherit;"> 3-4</td>
                                <td style ="background-color: #d2ff4d; width:120px; font-size: inherit;">High</td>
                            </tr>
                            <tr>
                                <td style ="background-color: #ffb3b3; font-size: inherit;"> 5</td>
                                <td style ="background-color: #608000; width:120px; font-size: inherit;">Expert</td>
                            </tr>
                            <tr>
                                <td style ="background-color: #ffb3b3; font-size: inherit;"> NA</td>
                                <td style ="background-color: #c1c1b8; width:120px; font-size: inherit;">Not Applicable</td>
                            </tr>
                        </table>
                    </div>
                    <div class="d-flex justify-content-between p-3">

                        <div class="table-responsive" style="overflow: auto;font-size: smaller;">
                            <table class="table table-bordered fixed_headers m-0" id="skill_employee">
                                <thead class="report_theads" style="font-size: inherit;">
                                    <button style="font-size: inherit;" id="action_export" type="button" class="mt-2 btn btn-success action_export">Export</button>
                                    <tr class="rpt_heading" style="font-size: medium;">
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Employee Code</td>
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Location</td>
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Department</td>
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Section</td>
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Designation</td>
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Grade</td>
                                        <td rowspan="2" class="text-left wordbreak align-middle" style="background-color: #c1c1b8;color: black;">Employee Name</td>
                                        <!-- <t t-esc="m_data" /> -->
                                        <t t-foreach="m_data" t-as="val">
                                            <!-- <t t-esc="m_data[val]['count']" /> -->
                                            <th class="text-center wordbreak align-middle" t-att-colspan="m_data[val]['count']" t-esc="val"></th>
                                        </t>
                                    </tr>
                                    <tr class="rpt_heading">
                                        <t t-foreach="m_data" t-as="val">
                                            <t t-foreach="m_data[val]" t-as="dic_data">
                                                <t t-if="dic_data != 'count'">
                                                    <td style="width:15%;height:20px;">
                                                        <span t-esc="dic_data"/>
                                                    </td>
                                                </t>
                                            </t>
                                        </t>
                                    </tr>
                                </thead>
                                <tbody class="report_tbodys" style="font-size: small;font-style: normal;">
                                    <t t-foreach="template_body_data" t-as="val">
                                        <tr >
                                            <t t-foreach="val" t-as="data">
                                                <td class="text-left wordbreak" data-toggle="tooltip" flow="down" data-placement="bottom" data-container="body" style="white-space: nowrap;text-overflow: ellipsis;overflow: hidden;">
                                                    <span t-esc="val[data]"/>
                                                </td>
                                            </t>
                                        </tr>
                                    </t>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>
