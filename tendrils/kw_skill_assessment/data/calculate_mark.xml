<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
		<record id="calculate_mark_scheduler" model="ir.cron">
			<field name="name">Skill Assessment: Calculate Mark Scheduler</field>
			<field name="model_id" ref="model_kw_skill_answer_master"/>
			<field name="state">code</field>
			<field name="code">model._calculate_skill_mark()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">30</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">2</field>
			<field name="active" eval="True" />
		</record>
	</data>

</odoo>