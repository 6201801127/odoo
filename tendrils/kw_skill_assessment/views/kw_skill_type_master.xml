<odoo>
	<data>
		<!-- explicit tree view of kw skill type master : Opening -->
		<record model="ir.ui.view" id="kw_skill_type_master_list">
			<field name="name">Skill Category Master</field>
			<field name="model">kw_skill_type_master</field>
			<field name="arch" type="xml">
				<tree editable="top" delete="false">
					<field name="skill_type" string="Name"/>
					<field name="active" widget="boolean_toggle" groups="base.group_no_one"/>
				</tree>
			</field>
		</record>
		<!-- explicit tree view of kw skill type master : Ending -->

		<record id="kw_skill_type_master_action_window" model="ir.actions.act_window">
			<field name="name">Skill Category Master</field>
			<field name="res_model">kw_skill_type_master</field>
			<field name="view_mode">tree</field>
			<field name="view_type">tree</field>
			<field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('kw_skill_type_master_list')})]" />
			<field name="view_type">form</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					Add a skill category ...
				</p>
			</field>
		</record>
	</data>
</odoo>