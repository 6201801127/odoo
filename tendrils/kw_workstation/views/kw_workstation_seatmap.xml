							<t t-elif="infra_code == 'noida_office'">
								<div class="row">
									<div class="col-sm-9">
										<t t-call="kw_workstation.seat_map_noida_office_layout"/>
									</div>
									<div class="col-sm-3">
										<div class="alert alert-primary txt_floor_name" role="alert"/>
										<t t-raw="stats"/>
									</div>
								</div>
							</t>
							<t t-elif="infra_code == 'patna_office'">
								<div class="row">
									<div class="col-sm-9">
										<t t-call="kw_workstation.seat_map_patna_office_layout"/>
									</div>
									<div class="col-sm-3">
										<div class="alert alert-primary txt_floor_name" role="alert"/>
										<t t-raw="stats"/>
									</div>
								</div>
							</t>
							<t t-elif="infra_code == 'ranchi_office'">
								<div class="row">
									<div class="col-sm-9">
										<t t-call="kw_workstation.seat_map_ranchi_office_layout"/>
									</div>
									<div class="col-sm-3">
										<div class="alert alert-primary txt_floor_name" role="alert"/>
										<t t-raw="stats"/>
									</div>
								</div>
							</t>

							<t t-elif="infra_code == 'all' ">

								<div class="row">
									<div class="col-sm-9">
										<div class="row">

											<t t-if="branch_code == 'BBS' or branch_code == 'all'">
												<t t-call="kw_workstation.seat_map_office_layout_bbsr"/>
											</t>
											<t t-if="branch_code == 'DL' or branch_code == 'all'">
												<t t-call="kw_workstation.seat_map_office_layout_delhi"/>
											</t>
											<t t-if="branch_code == 'JP' or branch_code == 'all'">
												<t t-call="kw_workstation.seat_map_office_layout_jaipur_office_unit"/>
											</t>
											<t t-if="branch_code == 'PN' or branch_code == 'all'">
												<t t-call="kw_workstation.seat_map_office_layout_patna_office_unit"/>
											</t>
											<t t-if="branch_code == 'RNC' or branch_code == 'all'">
												<t t-call="kw_workstation.seat_map_office_layout_ranchi_office_unit"/>
