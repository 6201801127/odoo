										</div>
									</div>
									<div class="col-sm-3">
										<t t-raw="stats"/>
									</div>
								</div>
							</t>

							<t t-else="">

							</t>

							<div class="col-sm-6 ws-info">
								<table class="ws-table table table-bordered">
									<tr>
										<th style="background-color:#E6E6E6;">WS Type</th>
										<th style="background-color:#E6E6E6;">Total WS</th>
										<th style="background-color:#E6E6E6;">Assigned</th>
										<th style="background-color:#E6E6E6;">Not Assigned</th>
										<th style="background-color:#E6E6E6;">PPL Occupied</th>
									</tr>
									<tr>
										<td>Cabin</td>
										<td><span id="tot_ws_cabin"/></td>
										<td><span id="tot_ws_cabin_assign"/></td>
										<td><span id="tot_ws_cabin_notassign"/></td>
										<td><span id="tot_cab_occupied"/></td>
									</tr>
									<tr>
										<td>Sr. WS</td>
