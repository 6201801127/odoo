										<td><span id="tot_ws_sr_assign"/></td>
										<td><span id="tot_ws_sr_notassign"/></td>
										<td><span id="tot_sr_occupied"/></td>
									</tr>
									<tr>
										<td>Mid WS</td>
										<td><span id="tot_ws_mid"/></td>
										<td><span id="tot_ws_mid_assign"/></td>
										<td><span id="tot_ws_mid_notassign"/></td>
										<td><span id="tot_mid_occupied"/></td>
									</tr>
									<tr>
										<td>Running WS</td>
