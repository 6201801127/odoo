<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="infra_view" model="ir.actions.act_url">
			<field name="name">Infrastructure Seat Map View</field>
			<field name="url">/seat-map</field>
			<field name="target">new</field>
		</record>
		<template id="kw_workstation_seat_map" name="Seat Map">
			<t t-call="website.layout">
				<div class="container mt-3">
					<div class="card">
						<div class="card-body">
							<div class="d-flex row relative">
								<div class="col-sm-2 mt-1">
									<lable for="branchSelect">Select Branch</lable>
									<select class="browser-default custom-select seatmap-select" id="branchSelect" t-att-rel="branch_code">
										<option value="all">Choose Branch</option>
										<t t-foreach="branches" t-as="branch">
											<option t-att-value="branch.code"
													t-attf-id="branch#{branch.code}"
													t-att-selected="branch.code==branch_code"><t t-esc="branch.alias"/></option>
										</t>
									<option value="all" id="all" t-att-selected="'all'==infra_code">All</option>
									</select>
								</div>
								<div class="col-sm-2 mt-1">
									<lable for="branchUnitSelect">Select Unit</lable>
									<select class="browser-default custom-select seatmap-select" id="branchUnitSelect">
										<option value="all">Choose Unit</option>
										<t t-foreach="branch_unit" t-as="unit">
											<option t-att-value="unit.code"
												t-att-selected="unit.code==unit_code"
												t-att-id="unit.code"><t t-esc="unit.name"/></option>
										</t>
										<option value="all" id="all" t-att-selected="'all'==infra_code">All</option>
									</select>
								</div>
								<div class="col-sm-2 mt-1">
									<lable for="infraSelect">Select Infrastructure</lable>
									<select class="browser-default custom-select seatmap-select" id="infraSelect" t-att-rel="infra_code">
										<option value="all">Choose Infrastructure</option>
										<t t-foreach="infrastructure" t-as="infra">
											<option t-att-value="infra.code" t-att-id="infra.id"
											        t-att-selected="infra.code==infra_code"><t t-esc="infra.name"/></option>
										</t>
										<option value="all" id="all" t-att-selected="'all'==infra_code">All</option>
									</select>
								</div>

								<div class="col-sm-2 mt-1">
									<lable for="projectSelect">Select Project</lable>
									<select class="browser-default custom-select seatmap-select select2" id="projectSelect">
										<option value="0">Choose Project</option>
									</select>
								</div>

								<div class="col-sm-2 mt-1">
									<lable for="EmployeeSelect">Select Employee</lable>
									<select class="browser-default custom-select seatmap-select select2" id="EmployeeSelect">
										<option value="0">Choose Employee</option>
									</select>
								</div>
								<div class="col-sm-2 mt-1">
									<lable for="DesignationSelect">Select Designation</lable>
									<select class="browser-default custom-select seatmap-select select2" id="DesignationSelect">
										<option value="0">Choose Designation</option>
									</select>
								</div>
								<div class="col-sm-2 mt-1">
									<lable for="GradeSelect">Select Grade</lable>
									<select class="browser-default custom-select seatmap-select select2" id="GradeSelect">
										<option value="0">Choose Grade</option>
									</select>
								</div>

								<div class="col-sm-7 mt-2 ws-info offset-2">

								</div>

								<div class="clearfix"/>
							</div>
							<div class="clearfix"/>
							<t t-set="stats">
								<table class="ws-table table table-bordered">
									<tr>
										<th class="text-uppercase text-center" style="width:50%;background-color:#E6E6E6;" colspan="2">Workstation</th>
									</tr>
									<tr>
										<th class="text-uppercase font-weight-bold" style="width:60%">Total</th>
										<td class=" text-center font-weight-bold" style="width:40%"><span id="total_work_station"/></td>
									</tr>
									<tr>
										<th class="text-uppercase">Assigned</th>
										<td class=" text-center"><span id="tot_assigned"/></td>
									</tr>
									<tr>
										<th class="text-uppercase">Not Assigned</th>
										<td class=" text-center"><span id="tot_not_assigned"/></td>
									</tr>
									<tr>
										<th class="text-uppercase text-center" style="width:50%;background-color:#E6E6E6;" colspan="2">ASSIGNED WS - Break up</th>
									</tr>
									<tr>
										<th class="text-uppercase">WFO</th>
										<td class=" text-center"><span id="tot_wfo"/></td>
									</tr>
									<tr>
										<th class="text-uppercase">WFH</th>
										<td class=" text-center"><span id="tot_wfh"/></td>
									</tr>
									<tr>
										<th class="text-uppercase">Contractual</th>
										<td class=" text-center"><span id="tot_contract"/></td>
									</tr>
									<tr>
										<th class="text-uppercase">Reserved</th>
										<td class=" text-center"><span id="tot_reserved"/></td>
									</tr>
									<tr>
										<th class="text-uppercase font-weight-bold">Total Assigned WS</th>
										<td class=" text-center font-weight-bold"><span id="tot_assigned_ws"/></td>
									</tr>
								</table>

								<table class="table">
									<tr>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 ml-1 col-sm-12 btn-wfo light-ic pr-2 pl-2"><strong>WFO</strong> <span class="badge bg-secondary font-small" id="tot_wfo_ico"/></button>
										</td>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 mr-1 col-sm-12 btn-wfh pr-2 pl-2"><strong>WFH</strong> <span class="badge bg-secondary font-small" id="tot_wfh_ico"/></button>
										</td>
									</tr>
									<tr>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 ml-1 col-sm-12 btn-hybrid dark-ic pr-2 pl-2"><strong>Hybrid</strong> <span class="badge bg-secondary font-small" id="tot_hybrid_ico"/></button>
										</td>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 mr-1 col-sm-12 btn-reserved light-ic pr-2 pl-2"><strong>Reserved</strong> <span class="badge bg-secondary font-small" id="tot_reserved_ico"/></button>
										</td>
									</tr>
									<tr>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 ml-1 col-sm-12 btn-contractual light-ic pr-2 pl-2"><strong>Contractual</strong> <span class="badge bg-secondary font-small" id="tot_contract_ico"/></button>
										</td>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 mr-1 col-sm-12 btn-not-assigned light-ic pr-2 pl-2"><strong>Not Assigned</strong> <span class="badge bg-secondary font-small" id="tot_not_assigned_ico"/></button>
										</td>
									</tr>
									<tr>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 ml-1 col-sm-12 btn-project-sbu light-ic pr-2 pl-2"><strong>SBU User</strong></button>
										</td>
										<td class="text-center m-0 p-0">
											<button class="btn btn-sm1 mr-1 col-sm-12 btn-project-horizontal light-ic pr-2 pl-2"><strong>Horizontal User</strong></button>
										</td>
									</tr>
								</table>
							</t>
							<!-- SVG rendering start-->
							<t t-if="infra_code == 'sixth-c'">
								<div class="row">
									<div class="col-sm-9">
										<t t-call="kw_workstation.seat_map_sixth_floor_c_block"/>
									</div>
									<div class="col-sm-3">
										<div class="alert alert-primary txt_floor_name" role="alert"/>
										<t t-raw="stats"/>
									</div>
								</div>

							</t>
							<t t-if="infra_code == 'sixth-s'">
								<div class="row">
									<div class="col-sm-9">
