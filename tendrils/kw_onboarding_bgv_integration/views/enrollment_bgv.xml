<odoo>
    <data>
        <record id="inherit_kw_onboarding_bgv_form_view" model="ir.ui.view">
            <field name="name">inherit.kw_onboarding.form</field>
            <field name="model">kwonboard_enrollment</field>
            <field name="inherit_id" ref="kw_onboarding.kwonboard_enrollment_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/div" position="before">
                    <div class="oe_button_box" name="button_box">
                        <button name="sync_bgv_applicant_data" type="object" class="oe_stat_button" string="Sync BGV Data"
                                icon="fa-paper-plane" confirm="Are you sure to sync applicant verification Data?">

                        </button>
                    </div>

                </xpath>
            </field>
        </record>
        <record id="inherit_kw_onboarding_bgv_tree_view" model="ir.ui.view">
            <field name="name">inherit.kw_onboarding.tree</field>
            <field name="model">kwonboard_enrollment</field>
            <field name="inherit_id" ref="kw_onboarding.kwonboard_enrollment_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="inside">
                    <button name="sync_bgv_applicant_data" class="oe_highlight" type="object" icon="fa-rocket fa-lg"

                            aria-label="BGV" confirm="Are you sure to sync applicant verification Data?"/>

                </xpath>
            </field>
        </record>
    </data>
</odoo>