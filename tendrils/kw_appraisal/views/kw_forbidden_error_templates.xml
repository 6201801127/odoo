<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <!-- Forbidden error messages-->
        <template id="error" name="Tendrils | Forbidden Page">
            <t t-call="website.layout" name=''>
                <div id="wrap">
                    <div class="container">
                        <div class="jumbotron mt32 text-center">
                            <h1 class="mt32">403: Forbidden</h1>
                            <p>The page you were looking for could not be authorized.</p>
                        </div>
                    </div>
                </div>
            </t>
        </template>


        <template id="kw_survey_error_assests" inherit_id="kw_appraisal.error">
            <xpath expr="." position="inside">
                <link href="/kwantify_theme/static/src/mdb/css/mdb.min.css" rel="stylesheet"/>
                <link href="/kw_appraisal/static/src/css/front_style.css" rel="stylesheet"/>
                <script type="text/javascript" src="/kwantify_theme/static/src/mdb/js/mdb.min.js"></script>            
                <!-- <script type="text/javascript" src="/kw_appraisal/static/src/js/skill_test.js"></script> -->
            </xpath>
        </template>
    </data>
</odoo>