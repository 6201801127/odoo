<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="kw_survey_type" id="kw_feedback_assessment_survey_type">
            <field name="name">Assessment Feedback</field>
            <field name="code">assessment_feedback</field>
        </record>
        <record id="kw_feedback_assessment_survey_type_form" model="survey.survey">
            <field name="title">Assessment Feedback</field>
            <field name="stage_id" ref="survey.stage_permanent"/>
            <field name="auth_required" eval="False"/>
            <field name="users_can_go_back" eval="True"/>
            <field name="survey_type" ref="kw_feedback_assessment_survey_type"/>
            <field name="description">&lt;p&gt;Assessment evaluation form for participants.&lt;/p&gt;</field>
            <field name="thank_you_message">&lt;p&gt;Thanks for submitting feedback.&lt;/p&gt;</field>
        </record>
        <record id="assessment_feedback_form_page1" model="survey.page">
            <field name="title">Core Skills</field>
            <field name="survey_id" ref="kw_feedback_assessment_survey_type_form"/>
            <field name="sequence">1</field>
            <field name="description">&lt;p&gt;&lt;/p&gt;</field>
        </record>

        <record id="kw_assessment_feedback_page1_question1" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page1"/>
            <field name="sequence">1</field>
            <field name="question">Database Concept/ Testing Concept</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page1_question2" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page1"/>
            <field name="sequence">2</field>
            <field name="question">Domain Knowledge (Functionality of Module)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>
        
        <record id="kw_assessment_feedback_page1_question3" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page1"/>
            <field name="sequence">3</field>
            <field name="question">Skills (Programming/Designing/Testing)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page1_question4" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page1"/>
            <field name="sequence">4</field>
            <field name="question">Implementing Security Measures/ Defect Finding</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page1_question5" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page1"/>
            <field name="sequence">5</field>
            <field name="question">Coding /Testing Standards (Proper Exception Handling, Proper Commenting for code, Page History, Proper Variable Declaration/ Testing case writing standard)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>
        
        <record id="assessment_feedback_form_page2" model="survey.page">
            <field name="title">Quality &amp; Productivity</field>
            <field name="survey_id" ref="kw_feedback_assessment_survey_type_form"/>
            <field name="sequence">2</field>
            <field name="description">&lt;p&gt;&lt;/p&gt;</field>
        </record>

        <record id="kw_assessment_feedback_page2_question1" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page2"/>
            <field name="sequence">1</field>
            <field name="question">Quality and Timely completion of Task (Reliability in meeting deadlines with accuracy and thoroughness)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page2_question2" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page2"/>
            <field name="sequence">2</field>
            <field name="question">Creativity and Innovativeness (Response to changing needs of the requirement and proactive in contribution of new ideas)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>
        
        <record id="kw_assessment_feedback_page2_question3" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page2"/>
            <field name="sequence">3</field>
            <field name="question">Planning and Work Execution (Capacity to organize, plan, prioritize, manage tasks effectively, capacity to shoulder workload with speed, accuracy consistency)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="assessment_feedback_form_page3" model="survey.page">
            <field name="title">Soft Skills</field>
            <field name="survey_id" ref="kw_feedback_assessment_survey_type_form"/>
            <field name="sequence">3</field>
            <field name="description">&lt;p&gt;&lt;/p&gt;</field>
        </record>

        <record id="kw_assessment_feedback_page3_question1" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page3"/>
            <field name="sequence">1</field>
            <field name="question">Task Reporting</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page3_question2" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page3"/>
            <field name="sequence">2</field>
            <field name="question">Adaptibility</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>
        
        <record id="kw_assessment_feedback_page3_question3" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page3"/>
            <field name="sequence">3</field>
            <field name="question">Communication (Listening, conveying and recieving messages both from internal employees and Clients)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page3_question4" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page3"/>
            <field name="sequence">4</field>
            <field name="question">Time Management and Regularity (Daily attendance, Punctuality, Leave Requests)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>

        <record id="kw_assessment_feedback_page3_question5" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page3"/>
            <field name="sequence">5</field>
            <field name="question">Attitude/ Team Behaviour</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>
        <record id="kw_assessment_feedback_page3_question6" model="survey.question">
            <field name="page_id" ref="assessment_feedback_form_page3"/>
            <field name="sequence">6</field>
            <field name="question">Iniative (Self starting ability to proceed without being told every detail)</field>
            <field name="type">numerical_box</field>
            <field name="validation_required">True</field>
            <field name="validation_min_float_value">0</field>
            <field name="validation_max_float_value">5</field>
            <field name="constr_mandatory">True</field>
        </record>
    </data>
</odoo>
