<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="hr_employee_assessment_feedback_tree_view" model="ir.ui.view">
            <field name="name">Employee Assessment feedback inherit</field>
            <field name="model">hr.employee</field>
            <field name="priority" eval="56"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="display_name" string='Name'/>
                    <field name="job_id" string='Job Position'/>
                    <field name="department_id"/>
                    <field name="parent_id" string='Reporting Authority'/>
                    <field name="work_email"/>
                    <field name="mobile_phone" string='Mobile'/>
                </tree>
            </field>
        </record>
    </data>
</odoo>