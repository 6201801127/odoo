<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <template id="kw_feedback_thanks_page" name="Thank You">
            <t t-call="website.layout">
                <div class="wrap">
                    <div class="container">
                        <section class="service-request mt32">
                            <div class="container card">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-12 mx-auto">
                                            <div class="section__content text-center">
                                                <div class="swal2-icon swal2-success swal2-animate-success-icon" style="display: flex;" data-oe-model="ir.ui.view" data-oe-id="1641" data-oe-field="arch" data-oe-xpath="/t[1]/t[1]/div[1]/div[1]/div[1]/div[1]">
                                                    <span class="swal2-success-line-tip">
                                                    </span>
                                                    <span class="swal2-success-line-long">
                                                    </span>
                                                    <div class="swal2-success-ring">
                                                    </div>
                                                </div>
                                                <h2 class="display-6 text-center font-weight-bold mt-6 mb-4">Your feedback submitted successfully!</h2>
                                                <a class="btn btn-primary badge" style="background-color: #f7941d !important;" t-attf-href="/web#action=kw_assessment_feedback.kw_feedback_final_config_action&amp;model=kw_feedback_final_config&amp;view_type=kanban">
                                                    <i class="fa fa-arrow-circle-left " />
 &amp;nbsp;
                                                Back to feedback
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </t>
        </template>

        <template id="kw_feedback_no_open_template" name="Feedback not open">
            <t t-call="website.layout">
                <div class="wrap">
                    <div class="container">
                        <div class="jumbotron mt32 text-center">
                            <h1>
								Not Available
                            </h1>
                            <pre class="text-justify">
								This Feedback is not open.
								May be your assessment period is not started or this assessment has been closed.
								Thank you for your interest !!
                            </pre>
                        </div>
                    </div>
                </div>
            </t>
        </template>

        <template id="kw_feedback_no_open_template_assets" inherit_id="kw_assessment_feedback.kw_feedback_no_open_template">
            <xpath expr="." position="inside">
                <link href="/kwantify_theme/static/src/mdb/css/mdb.min.css" rel="stylesheet"/>
                <link href="/kw_assessment_feedback/static/src/css/front_style.css" rel="stylesheet"/>
                <script type="text/javascript" src="/kwantify_theme/static/src/mdb/js/mdb.min.js">
                </script>
            </xpath>
        </template>

        <template id="kw_assessment_feedback_thanks_page_assets" inherit_id="kw_assessment_feedback.kw_feedback_thanks_page">
            <xpath expr="." position="inside">
                <link href="/kw_assessment_feedback/static/src/css/front_style.css" rel="stylesheet"/>
                <link href="/kw_assessment_feedback/static/src/css/success.css" rel="stylesheet"/>
                <link href="/kwantify_theme/static/src/mdb/css/mdb.min.css?v=12" rel="stylesheet"/>
                <script type="text/javascript" src="/kwantify_theme/static/src/mdb/js/mdb.min.js?v=12"/>
            </xpath>
        </template>

    </data>
</odoo>
