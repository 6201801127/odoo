<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="kw_employee_inherits_employee_skill_tree" model="ir.ui.view">
            <field name="name">project inherits employee tree</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="kw_employee.kw_resource_skill_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='emp_display_name']" position="after">
                    <field name="job_id"/>
                </xpath>
                <xpath expr="//field[@name='skill_id']" position="after">
                    <field name="tagged_project"/>
                    <field name="project_manager_name"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
