<odoo>
    <data noupdate="1">
        <record id="kwantify_project_sync_odoo" model="ir.cron">
            <field name="name">Project : Sync Projects from V5</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doal">0</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_crm_lead"/>
            <field name="code">model.syncKwantifyProjectData()</field>
            <field name="state">code</field>
        </record>
        <record id="kwantify_opportunity_sync_scheduler" model="ir.cron">
            <field name="name">Project : Sync Opportunities from V5</field>
            <!-- <field name="active" eval="True"/> -->
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doal">0</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_crm_lead"/>
            <field name="code">model.sync_v5_opportunies()</field>
            <field name="state">code</field>
        </record>
        <record id="api_fetch_from_v5_sales_data_cron" model="ir.cron">
            <field name="name">Sales || GET V5 Portlet Data</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_sync_portlet_data"/>
            <field name="code">model.FetchV5Data()</field>
            <field name="state">code</field>
        </record>
        <record id="api_send_from_tendrils_sales_data_cron" model="ir.cron">
            <field name="name">Sales || SEND Tendrils Portlet Data</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_sync_portlet_data"/>
            <field name="code">model.action_button_sync_record_opp_debt()</field>
            <field name="state">code</field>
        </record>
        <record id="automation_for_update_recieved_v5_data" model="base.automation">
            <field name="name">Update Recieved V5 data</field>
            <field name="model_id" ref="model_kw_portlet_recieved_log"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[records.update_recieved_data_in_model()]]></field>
            <field name="trigger">on_time</field>
            <field name="trg_date_range">1</field>
            <field name="trg_date_range_type">minutes</field>
            <field name="active" eval="True"/>
        </record>
        <record id="automation_for_send_tendrils_data" model="base.automation">
            <field name="name">Send updated Tendrils data</field>
            <field name="model_id" ref="model_kw_sync_portlet_data"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[records.action_button_sync_record_opp_debt(record.id)]]></field>
            <field name="trigger">on_time</field>
            <field name="trg_date_range">1</field>
            <field name="trg_date_range_type">minutes</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>