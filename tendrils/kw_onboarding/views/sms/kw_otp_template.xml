<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="onboarding_otp_template" model="send_sms">
            <field name="name">Onboarding_OTP</field>
            <field name="gateway_id" model="gateway_setup" search="[('name','=','Bulksms_Gateway')]"/>
            <field name="model_id" ref="kw_onboarding.model_kwonboard_enrollment"/>
            <field name="sms_to">${object.mobile}</field>
            <field name="sms_html">Hello ${object.name}, your otp is ${object.otp_number}.</field>
       </record>
    </data>
</odoo>