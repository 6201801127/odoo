        <record id="kw_card_master_tree1_inherit" model="ir.ui.view">
            <field name="name">kw_card_master_tree1_inherit</field>
            <field name="model">kw_card_master</field>
            <field name="inherit_id" ref="kw_employee.kw_card_master_tree1"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='employee_id']" position="after">
                    <field name="enrollment_id" string="Onboarding Tagged"
