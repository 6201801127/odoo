<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <data noupdate="1">
        <record id="kw_tour_settlement_reminder_scheduler" model="ir.cron">
            <field name="name">Tour: Settlement Reminder and Return Ticket Booking Reminder Scheduler</field>
            <field name="model_id" ref="model_kw_tour"/>
            <field name="state">code</field>
            <field name="code">model.remind_settlements()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <!-- <field name="active" eval="True" /> -->
        </record>

        <record id="kw_tour_pending_action_reminder_scheduler" model="ir.cron">
            <field name="name">Tour: Tour and Settlement Pending Action Reminder Scheduler</field>
            <field name="model_id" ref="model_kw_tour"/>
            <field name="state">code</field>
            <field name="code">model.remind_pending_actions()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <!-- <field name="active" eval="True" /> -->
        </record>

