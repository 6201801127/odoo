<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="kw_tour_project_budget_sync" model="ir.cron">
            <field name="name">Tour Budget: Sync Tour Project Budget</field>
            <field name="model_id" ref="model_kw_tour_project_budget"/>
            <field name="code">model._CreateUpdateProjectBudget()</field>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <field name="active" eval="True"/>
        </record>

        <record id="kw_tour_project_budget_pm_reminder_scheduler" model="ir.cron">
            <field name="name">Tour Project Budget: Reminder to PM for threshold limit crossed</field>
            <field name="model_id" ref="model_kw_tour_project_budget"/>
            <field name="state">code</field>
            <field name="code">model.remind_pm_budget_limit()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <field name="active" eval="True"/>
        </record>

        <record id="kw_tour_treasury_budget_sync" model="ir.cron">
            <field name="name">Tour Budget: Sync Tour Treasury Budget</field>
            <field name="model_id" ref="model_kw_tour_treasury_budget"/>
            <field name="code">model.create_update_treasury_budget()</field>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">5</field>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
