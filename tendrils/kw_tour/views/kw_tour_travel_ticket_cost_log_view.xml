<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_kw_tour_travel_ticket_cost_log_form" model="ir.ui.view">
            <field name="name">view.kw_tour_travel_ticket_cost_log.form</field>
            <field name="model">kw_tour_travel_ticket_cost_log</field>
            <field name="type">form</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <form string="kw_tour_travel_ticket_cost_log">
                <script>
                    $(document).ready(function(){
                        $(".modal-title").show();
                    });
                </script>
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="booking_date"/>
                                <field name="travel_mode_id"/>
                                <field name="currency_id"/>
                                <field name="cost"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_kkw_tour_travel_ticket_cost_log_tree" model="ir.ui.view">
            <field name="name">view.kw_tour_travel_ticket_cost_log.tree</field>
            <field name="model">kw_tour_travel_ticket_cost_log</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16"/>
            <field name="active" eval="True"/>
            <field name="arch" type="xml">
                <tree import="false" create="false" delete="false">
                    <field name="booking_date"/>
                    <field name="travel_mode_id"/>
                    <field name="currency_id"/>
                    <field name="cost"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>