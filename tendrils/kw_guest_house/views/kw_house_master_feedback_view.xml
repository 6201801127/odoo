<odoo>
  <data>


    <record model="ir.ui.view" id="kw_house_master_feedback_list">
      <field name="name">kw_house_master_feedback_list</field>
      <field name="model">kw_house_master_feedback</field>
      <field name="arch" type="xml">
        <tree>
          <field name="booking_id"/>
          <field name="requested_by_id"/>
          <field name="guest_house"/>
          <field name="country"/>
          <field name="state"/>
          <field name="guest_type"/>
          <field name="check_in_date_time"/>
          <field name="check_out_date_time"/>
          <field name="room_type"/>
          <field name="booking_type" attrs="{'invisible':[('room_type', '!=', 'double')]}"/>
          <field name="room_no"/>
          <field name="rating" string="Rating Out Of 5"/>
          <field name="feedback"/>
        </tree>
      </field>
    </record>


    <record model="ir.ui.view" id="kw_house_master_feedback_search">
      <field name="name">kw_house_master_feedback_search</field>
      <field name="model">kw_house_master_feedback</field>
      <field name="arch" type="xml">
        <search>
          <field name="requested_by_id"/>
          <filter string="My Feedbacks" name="filter_my_records" domain="[('create_uid', '=', uid)]"/>
        </search>
      </field>
    </record>


    <record model="ir.actions.act_window" id="kw_house_master_feedback_action_window">
      <field name="name">Feedback</field>
      <field name="res_model">kw_house_master_feedback</field>
      <field name="view_mode">tree</field>
      <field name="view_type">form</field>
      <field name="context">{'feedback':1, 'search_default_filter_my_records':1, 'create': False, 'delete': False, 'copy': False}</field>
    </record>


  </data>
</odoo>