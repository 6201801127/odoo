<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="v5_accounting_purchase_vouchers_details" model="ir.config_parameter">
            <field name="key">v5_accounting_purchase_vouchers_details</field>
            <field name="value">https://kwportalservice.csmpl.com/AccountSynService.svc/PurchaseVoucherDtls</field>
        </record>
        <record id="v5_accounting_other_vouchers_details" model="ir.config_parameter">
            <field name="key">v5_accounting_other_vouchers_details</field>
            <field name="value">https://kwportalservice.csmpl.com/AccountSynService.svc/OtherVoucherDtls</field>
        </record>
		<record id="v5_accounting_customer_details" model="ir.config_parameter">
            <field name="key">v5_accounting_customer_details</field>
            <field name="value">https://kwportalservice.csmpl.com/OdooSynSVC.svc/GetKwCustomerdetails</field>
        </record>

        <record id="other_voucher_data_sync_scheduler" model="ir.cron">
			<field name="name">Other Voucher Details Scheduler</field>
			<field name="model_id" ref="model_kw_accounting_sync_data"/>
			<field name="state">code</field>
			<field name="code">model.create_other_voucher_details()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<!-- <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 06:15:00')"/>  -->
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">2</field>
			<field name="active">False</field>
		</record>
		<record id="purchase_voucher_data_sync_scheduler" model="ir.cron">
			<field name="name">Purchase Voucher Details Scheduler</field>
			<field name="model_id" ref="model_kw_accounting_sync_data"/>
			<field name="state">code</field>
			<field name="code">model.create_purchase_voucher_details()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<!-- <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 06:15:00')"/>  -->
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">2</field>
			<field name="active">False</field>
		</record>

		<record id="customer_details_data_sync_scheduler" model="ir.cron">
			<field name="name">Customer Details Scheduler</field>
			<field name="model_id" ref="model_kw_accounting_sync_data"/>
			<field name="state">code</field>
			<field name="code">model.create_client_details()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<!-- <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 06:15:00')"/>  -->
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">2</field>
			<field name="active">False</field>
		</record>

    </data>
</odoo>
