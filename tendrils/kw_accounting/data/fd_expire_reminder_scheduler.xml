<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
		<record id="fd_expire_reminder_mail_scheduler" model="ir.cron">
			<field name="name">FD Expire Reminder Mail Scheduler</field>
			<field name="model_id" ref="model_fd_tracker"/>
			<field name="state">code</field>
			<field name="code">model.fd_expire_reminder_mail()</field>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 06:15:00')"/> 
			<field name="numbercall">-1</field>
			<field name="doall" eval="False"/>
			<field name="priority">2</field>
			<field name="active">False</field>
		</record>
	</data>
</odoo>
