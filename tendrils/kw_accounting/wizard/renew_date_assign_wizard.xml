<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="renew_date_assign_wizard_form_view" model="ir.ui.view">
        <field name="name">Renew Date Assign Wizard</field>
        <field name="model">renew_date_assign_wizard</field>
        <field name="arch" type="xml">
            <form string="Renew">

                <sheet>
                    <group>
                        <field name="mr_dt" required='1'/>
                    </group>
                </sheet>
                <footer>
                    <button string="Done" name="action_assign_renew_date"
                            type="object" default_focus="1" class="btn-success"/>
                    <button string="Cancel" class="btn-danger" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>
