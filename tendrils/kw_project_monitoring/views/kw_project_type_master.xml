
<odoo>
    <data>

        <record id="view_kw_project_type_master_tree" model="ir.ui.view">
            <field name="name">kw_project_type_master_tree</field>
            <field name="model">kw_project_type_master</field>
            <field name="arch" type="xml">
                <tree string="Project Type Master">
                    <field name="project_type"/>
                </tree>
            </field>
        </record>

        <record id="view_kw_project_type_master_form" model="ir.ui.view">
            <field name="name">kw_project_type_master_form</field>
            <field name="model">kw_project_type_master</field>
            <field name="arch" type="xml">
                <form string="Project Type Master">
                    <sheet>
                        <group>
                            <group>
                                <field name="project_type"/>
                            </group>
                              
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_kw_project_type_master" model="ir.actions.act_window">
            <field name="name">Project Type Master</field>
            <field name="res_model">kw_project_type_master</field>
            <field name="view_mode">tree,form</field>
            
        </record>

        <!--Project Module Master-->

        <record id="view_kw_project_module_master_tree" model="ir.ui.view">
            <field name="name">kw_project_task_master_tree</field>
            <field name="model">kw_project_module_master</field>
            <field name="arch" type="xml">
                <tree string="Project Module Master">
                    <field name="project_id"/>
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="view_kw_project_module_master_form" model="ir.ui.view">
            <field name="name">kw_project_module_master_form</field>
            <field name="model">kw_project_module_master</field>
            <field name="arch" type="xml">
                <form string="Project Module Master">
                    <sheet>
                        <group>
                            <group>
                                <field name="project_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
                                <field name="name"/>
                                <field name="description"/>
                            </group>
                            <group>
                                <field name="existing_modules"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_kw_project_module_master" model="ir.actions.act_window">
            <field name="name">Project Module Master</field>
            <field name="res_model">kw_project_module_master</field>
            <field name="view_mode">tree,form</field>
            
        </record>

        <record id="view_project_risk_category_tree" model="ir.ui.view">
            <field name="name">project_risk_category_tree</field>
            <field name="model">project_risk_category</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <record id="action_project_risk_category" model="ir.actions.act_window">
            <field name="name">Project Risk Category Master</field>
            <field name="res_model">project_risk_category</field>
            <field name="view_mode">tree</field>
            
        </record>

        <record id="view_project_architecture_type_master_tree" model="ir.ui.view">
            <field name="name">project_architecture_type_master_tree</field>
            <field name="model">project_architecture_type_master</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="architecture_name"/>
                    <field name="description"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>


        <record id="action_project_architecture_type_master" model="ir.actions.act_window">
            <field name="name">Project Architecture Type</field>
            <field name="res_model">project_architecture_type_master</field>
            <field name="view_mode">tree</field>
            
        </record>
        
    </data>
</odoo>
