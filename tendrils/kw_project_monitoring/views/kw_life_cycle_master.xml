<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_kw_life_cycle_master_tree" model="ir.ui.view">
            <field name="name">kw_life_cycle_master_tree</field>
            <field name="model">kw_life_cycle_master</field>
            <field name="arch" type="xml">
                <tree string="KW Life Cycle Master">
                    <field name="sl_no"/>
                    <field name="project_type" options="{'no_open': True,'no_create':True}"/>
                    <field name="life_cycle_name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="view_kw_life_cycle_master_form" model="ir.ui.view">
            <field name="name">kw_life_cycle_master_form</field>
            <field name="model">kw_life_cycle_master</field>
            <field name="arch" type="xml">
                <form string="KW Life Cycle Master">
                    <sheet>
                        <group>
                            <group>
                                <field name="project_type" options="{'no_open': True,'no_create':True}"/>
                                <field name="life_cycle_name"/>
                            </group>
                            <group>
                                <field name="active"/>
                                <field name="description"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Define the action -->
        <record id="action_kw_life_cycle_master" model="ir.actions.act_window">
            <field name="name">Life Cycle Master</field>
            <field name="res_model">kw_life_cycle_master</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Create a new life cycle master.
                </p>
            </field>
        </record>
<!-- Life Cycle Phase Tree View -->
        <record id="view_kw_life_cycle_phase_master_tree" model="ir.ui.view">
            <field name="name">kw_life_cycle_phase_master_tree</field>
            <field name="model">kw_life_cycle_phase_master</field>
            <field name="arch" type="xml">
                <tree string="Life Cycle Phase Master">
                    <field name="life_cycle_id" />
                    <field name="phase_name" />
                    <field name="phase_order"/>
                    <field name="phase_type"/>
                    <field name="description"/>
                    
                </tree>
            </field>
        </record>

    <!-- Life Cycle Phase Form View -->

        <record id="view_kw_life_cycle_phase_master_form" model="ir.ui.view">
            <field name="name">kw_life_cycle_phase_master_form</field>
            <field name="model">kw_life_cycle_phase_master</field>
            <field name="arch" type="xml">
                <form string="KW Life Cycle Master">
                    <sheet>
                        <group>
                            <group>
                                <field name="life_cycle_id" options="{'no_open': True,'no_create':True}"/>
                                <field name="phase_name" />
                                <field name="phase_order"/>
                   
                            </group>
                            <group>
                                <field name="phase_type" widget="radio" options="{'horizontal': true}"/>
                                <field name="description"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    <!-- Life Cycle Phase Action Window -->

        <record id="action_kw_life_cycle_phase_master" model="ir.actions.act_window">
            <field name="name">Life Cycle Phase Master</field>
            <field name="res_model">kw_life_cycle_phase_master</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Create a new life cycle phase.
                </p>
            </field>
        </record>

