    <record id="unadjusted_use_case_w1" model="kw_unadjusted_use_case_weight">
        <field name="name">Simple</field>
        <field name="description">A simple user interface and touches only a single database entity; its success scenario has 3 steps or less; its implementation involves less than 5 classes.</field>
        <field name="weight">5</field>
    </record>
    <record id="unadjusted_use_case_w2" model="kw_unadjusted_use_case_weight">
        <field name="name">Average</field>
        <field name="description">More interface design and touches 2 or more database entities; between 4 to 7 steps; its implementation involves between 5 to 10 classes.</field>
        <field name="weight">10</field>
    </record>
    <record id="unadjusted_use_case_w3" model="kw_unadjusted_use_case_weight">
        <field name="name">Complex</field>
        <field name="description">Involves a complex user interface or processing and touches 3 or more database entities; over seven steps; its implementation involves more than 10 classes.</field>
        <field name="weight">15</field>
    </record>

    <record id="unadjusted_actor_w1" model="kw_unadjusted_actor_weight">
