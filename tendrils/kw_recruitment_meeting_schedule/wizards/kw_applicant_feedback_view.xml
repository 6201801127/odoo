<?xml version="1.0"?>
<odoo>
    <data>
        <!-- <act_window id="kw_training_notify_session_act_window" name="Send Notification Mail" src_model="kw_training_schedule" res_model="kw_training_notify_session" view_mode="form" target="new" multi="True"/> -->

        <record id="kw_recruitment_meeting_applicant_feedback_wizard_form" model="ir.ui.view">
            <field name="name">Print Applicant Feedback</field>
            <field name="model">kw_applicant_feedback</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="meeting_id" readonly="1" options="{'no_edit': True, 'no_create':True, 'no_open':True}"/>
                        <field name="applicant_id"  options="{'no_edit': True,'no_create':True,'no_open':True}"/>
                    </group>
                    <footer>
                        <button name="print_applicant_feedback_report" string="Download" type="object" class="oe_highlight" />
                        Or      
                        <button string="Cancel" class="oe_highlight" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>