<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <record id="kw_recruitment_candidate_interview_report_tree" model="ir.ui.view">
            <field name="name">Candidate Interview report tree</field>
            <field name="model">survey.user_input</field>
            <field name="arch" type="xml">
                <tree string="Candidate Interview report" create="false" delete="false" import="false" edit="false">
                    <field name="applicant_id" string="Applicant"/>
                    <field name="position" string="Position"/>
                    <field name="department_id"/>
                    <field name="qualification" />
                    <field name="experience"/>
                    <field name="partner_id" string="Evaluators"/>
                    <field name="date"/>
                    <field name="remark" string="Status"/>
                    <field name="score"/>
                </tree>
            </field>
        </record>
        <record id="action_kw_recruitment_candidate_interview_report" model="ir.actions.act_window">
		  <field name="name">Candidate Interview Report</field>
		  <field name="res_model">survey.user_input</field>
		  <field name="view_mode">tree</field>
		  <field name="context">{}</field>
		  <field name="domain">[]</field>
		  <field name="view_ids" eval="[(5, 0, 0),
		(0, 0, {'view_mode': 'tree', 'view_id': ref('kw_recruitment_candidate_interview_report_tree')})]"/>
	    </record>
        
    </data>
</odoo>