<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="cpd_auto_reject_applications_cron" model="ir.cron">
            <field name="name">CPD || Auto reject applications</field>
            <field name="model_id" ref="kw_cpd.model_kw_cpd_applications"/>
            <field name="state">code</field>
            <field name="code">model.cpd_auto_reject_applications()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="priority">99</field>
        </record>
    </data>


    <template id="kw_cpd_backend" inherit_id="web.assets_backend" active="True">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/kw_cpd/static/src/scss/kw_cpd.scss"/>
        </xpath>
    </template>
    
</odoo>