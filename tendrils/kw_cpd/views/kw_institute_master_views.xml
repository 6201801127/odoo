<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="kw_institute_master_tree" model="ir.ui.view">
            <field name="name">kw_institute_master_tree</field>
            <field name="model">kw_institute_master</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="active" widget="boolean_toggle"/>
                </tree>
            </field>
        </record>


        <record id="kw_institute_master_act_window" model="ir.actions.act_window">
            <field name="name">Institute Master</field>
            <field name="res_model">kw_institute_master</field>
            <field name="view_mode">tree</field>
            <field name="view_type">form</field>
        </record>


        <menuitem name="CPD" id="main_menu_kw_cpd" web_icon="kw_cpd,static/src/img/cpd_img.png"/>
        <menuitem name="Configuration" id="menu_kw_cpd_configuration" parent="main_menu_kw_cpd" sequence='100' groups='kw_cpd.group_kw_cpd_lnk_manager'/>
        <menuitem name="Institute Master" id="menu_kw_institute_master" parent="menu_kw_cpd_configuration" action="kw_institute_master_act_window" sequence='1'/>


    </data>
</odoo>
