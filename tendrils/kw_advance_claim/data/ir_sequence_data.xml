<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Advance salary -->
        <record id="seq_kw_apply_salary_advance" model="ir.sequence">
            <field name="name">Advance Salary</field>
            <field name="code">kw_apply_salary_advance</field>
            <field name="prefix">SA/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- sequence for petty cash -->
        <record id="seq_kw_apply_petty_cash" model="ir.sequence">
            <field name="name">Petty Cash</field>
            <field name="code">kw_apply_petty_cash</field>
            <field name="prefix">PC/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- sequence for claim settlement -->
        <record id="seq_kw_apply_claim" model="ir.sequence">
            <field name="name">Claim Settlement</field>
            <field name="code">kw_apply_claim</field>
            <field name="prefix">CS/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- sequence for claim bill ine -->
        <record id="seq_kw_claim_bil_line" model="ir.sequence">
            <field name="name">Claim Bill Line</field>
            <field name="code">kw_claim_bill_line</field>
            <field name="prefix">CB/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
