<odoo>
    <data noupdate="1">
        <record id="seq_pip_code" model="ir.sequence">
			<field name="name">PIP</field>
			<field name="code">PIP</field>
            <field name="prefix">PIP/</field>
			 <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
			<field name="company_id" eval="False"/>
		</record>
        <record id="sequence_pip_process_date_range1" model="ir.sequence.date_range">
            <field name="date_from">2022-04-01</field>
            <field name="date_to">2023-03-31</field>
            <field name="prefix">22-23</field>
            <!-- <field name="number_next_actual" eval="1"/> -->
            <field name="sequence_id" ref="seq_pip_code"/>
        </record>
        <record id="sequence_pip_process_date_range2" model="ir.sequence.date_range">
