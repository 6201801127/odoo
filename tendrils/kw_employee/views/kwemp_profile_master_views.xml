<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>

		<!-- search view  -->
		<record id="kwmaster_course_name_master_search_view" model="ir.ui.view">
			<field name="name">kwmaster_course_name_search_view</field>
			<field name="model">kwmaster_course_name</field>
			<field name="type">search</field>
			<field name="mode">primary</field>
			<field name="priority" eval="16"/>
			<field name="arch" type="xml">
				<search string="Courses">
					<field name="name" string="Couse Name"/>
					<field name="course_type"/>
					<filter string="Active" domain="[('active', '=',True)]" name="enable_courses"/>
					<filter string="Inactive" domain="[('active', '=',False)]" name="disable_courses"/>
				</search>
			</field>
		</record>


		<!-- explicit Course Action window definition -->
		<record model="ir.actions.act_window" id="kwemp_master_action_window">
			<field name="name">Courses</field>
			<field name="res_model">kwmaster_course_name</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="search_view_id" ref="kwmaster_course_name_master_search_view"/>
		</record>


		<!-- explicit Course Action Menu Item definition -->
		<menuitem name="Courses"
		          id="kwemp_master_course_menu"
		          parent="kwemp_education_submenu"
		          sequence="7"
		          action="kwemp_master_action_window"
		          groups="hr.group_hr_manager"/>

		<!-- explicit Course form view definition -->
		<record id="kwmasters_form_view" model="ir.ui.view">
			<field name="name">Course Form</field>
			<field name="model">kwmaster_course_name</field>
			<field name="arch" type="xml">
				<form string="Masters Course Form">
					<sheet>
						<group string="Course Form">
							<group name="group_left">
								<field name="course_type"/>
								<field name="name" string="Name"/>
								<field name="code" required="True"/>
								<!-- <field name="stream_ids" string = "Stream Details" />
								<field name="institute_ids" string = "Institute Details" /> -->
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<!-- explicit Course tree view definition -->
		<record id="kwmasters_course_tree_view" model="ir.ui.view">
			<field name="name">Course Lists</field>
			<field name="model">kwmaster_course_name</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
					<field name="code" groups="base.group_no_one"/>
					<field name="course_type"/>
					<field name="stream_ids"/>
					<field name="institute_ids"/>
					<field name="kw_id" groups="base.group_no_one"/>
					<field name="active" widget="boolean_toggle" groups="base.group_no_one"/>
				</tree>
			</field>
		</record>

		<!-- search view  -->
		<record id="kwmaster_stream_name_master_search_view" model="ir.ui.view">
			<field name="name">kwmaster_stream_name_search_view</field>
			<field name="model">kwmaster_stream_name</field>
			<field name="type">search</field>
			<field name="mode">primary</field>
			<field name="priority" eval="16"/>
			<field name="arch" type="xml">
				<search string="Stream">
					<field name="name" string="Name"/>
					<field name="course_id"/>
					<filter string="Active" domain="[('active', '=',True)]" name="enable_grade"/>
					<filter string="Inactive" domain="[('active', '=',False)]" name="disable_grade"/>
				</search>
			</field>
		</record>

		<!--START :  explicit Stream Action window definition -->
		<record id="kwmaster_stream_name_window_action" model="ir.actions.act_window">
			<field name="name">Streams</field>
			<field name="res_model">kwmaster_stream_name</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="search_view_id" ref="kwmaster_stream_name_master_search_view"/>
		</record>
		<!--END :  explicit Stream Action window definition -->

		<!-- explicit Stream Action Menu Item definition -->
		<menuitem name="Streams"
                  id="kwmaster_stream_name_menu"
                  parent="kwemp_education_submenu"
		          action="kwmaster_stream_name_window_action"
                  sequence="8"
                  groups="hr.group_hr_manager"/>

		<!--START : explicit Course tree view definition -->
		<record id="kwmasters_stream_tree_view" model="ir.ui.view">
			<field name="name">Stream Lists</field>
			<field name="model">kwmaster_stream_name</field>
			<field name="arch" type="xml">
				<tree editable='bottom'>
					<field name="name"/>
					<field name="course_id" options="{'no_create': True, 'no_open': True, 'no_edit': True}"/>
					<field name="kw_id" groups="base.group_no_one"/>
					<field name="active" widget="boolean_toggle" groups="base.group_no_one"/>
				</tree>
			</field>
		</record>
		<!--END : explicit Course tree view definition -->

		<!--START :  explicit Stream form view definition -->
		<record id="kwmasters_stream_form_view" model="ir.ui.view">
			<field name="name">Add Stream Details</field>
			<field name="model">kwmaster_stream_name</field>
			<field name="arch" type="xml">
				<form string="Add Stream Details">
					<group>
						<field name="course_id" string="Select Course"
						       options="{'no_open': True,'no_create':True,'no_edit':True}"/>
						<field name="name" string="Name"/>
						<!-- <field name="specialization_ids"/> -->
					</group>
				</form>
			</field>
		</record>

		<!--END :  explicit Stream form view definition -->

		<!-- search view -->
		<record id="kwmaster_institute_name_master_search_view" model="ir.ui.view">
			<field name="name">kwmaster_institute_name_search_view</field>
			<field name="model">kwmaster_institute_name</field>
			<field name="type">search</field>
			<field name="mode">primary</field>
			<field name="priority" eval="16"/>
			<field name="arch" type="xml">
				<search string="Institute Name">
					<field name="name" string="Name"/>
					<field name="inst_course_ids"/>
					<filter string="Active" domain="[('active', '=',True)]" name="enable_institute"/>
					<filter string="Inactive" domain="[('active', '=',False)]" name="disable_institute"/>
				</search>
			</field>
		</record>


		<!-- START :   explicit Institute Action Window definition -->
		<record id="kwmaster_institute_name_window_action" model="ir.actions.act_window">
			<field name="name">Institutes</field>
			<field name="res_model">kwmaster_institute_name</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="search_view_id" ref="kwmaster_institute_name_master_search_view"/>
		</record>
		<!-- END :   explicit Institute Action Window definition -->

		<!-- START :   explicit Institute Action Menu Item definition -->
		<menuitem name="Institutes"
                  id="kwmaster_institute_name_menu"
                  parent="kwemp_education_submenu"
		          action="kwmaster_institute_name_window_action"
                  sequence="9"
                  groups="hr.group_hr_manager"/>
		<!-- END :   explicit Institute Action Menu Item definition -->

		<!--START :  explicit Institute tree view definition -->
		<record id="kwmasters_institute_view_tree" model="ir.ui.view">
			<field name="name">Institute Details</field>
			<field name="model">kwmaster_institute_name</field>
			<field name="arch" type="xml">
				<tree default_order='name'>                    <!-- create="true" delete="true" editable="top" -->
					<field name="name"/>
					<!-- <field name="description"/> -->
					<!-- <field name="course_ids"/> -->
					<field name="inst_course_ids" widget="many2many_tags" options="{'no_create': True, 'no_open': True, 'no_edit': True}"/>
					<field name="kw_id" groups="base.group_no_one"/>
					<field name="active" widget="boolean_toggle" groups="base.group_no_one"/>
				</tree>
			</field>
		</record>
		<!--END :  explicit Institute tree view definition -->

		<!-- START :   explicit Institute form view definition -->
		<record id="kwmaster_institute_name_form_view" model="ir.ui.view">
			<field name="name">Board / University / Institute Details</field>
			<field name="model">kwmaster_institute_name</field>
			<field name="arch" type="xml">
				<form string="Add Board / University / Institute Details">
					<group>
						<!-- <field name="course_ids" string="Select Course"/> -->
						<field name="name" string="Name"/>
						<field name="inst_course_ids" widget="many2many_tags"/>
						<!-- <field name="specialization_ids"/> -->
					</group>
				</form>
			</field>
		</record>
		<!-- END :   explicit Institute form view definition -->

		<!-- search view -->
		<record id="kwmaster_specializations_master_search_view" model="ir.ui.view">
			<field name="name">kwmaster_specializations_search_view</field>
			<field name="model">kwmaster_specializations</field>
			<field name="type">search</field>
			<field name="mode">primary</field>
			<field name="priority" eval="16"/>
			<field name="arch" type="xml">
				<search string="Specialization">
					<field name="name" string="Specialization"/>
					<field name="stream_id"/>
					<filter string="Active" domain="[('active', '=',True)]" name="enable_specialization"/>
					<filter string="Inactive" domain="[('active', '=',False)]" name="disable_specialization"/>
				</search>
			</field>
		</record>

		<!-- START :   explicit Specialization Action Window definition -->
		<record id="kwmaster_specialization_action_window" model="ir.actions.act_window">
			<field name="name">Specializations</field>
			<field name="res_model">kwmaster_specializations</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="search_view_id" ref="kwmaster_specializations_master_search_view"/>
		</record>
		<!-- END :   explicit Specialization Action Window definition -->

		<menuitem name="Specialization"
		          id="kwmaster_specialization_menu"
		          parent="kwemp_education_submenu"
		          action="kwmaster_specialization_action_window"
		          sequence="10"
		          groups="hr.group_hr_manager"/>

		<!-- START :  specilization form view -->
		<record id="kwmaster_specialization_view_form" model="ir.ui.view">
			<field name="name">Specialization Details</field>
			<field name="model">kwmaster_specializations</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name="stream_id" string="Select Stream" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
							<field name="name"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<!-- END :  specilization form view -->

		<!-- START :   explicit Specialization tree view definition -->
		<record id="kwmaster_specializations_view_tree" model="ir.ui.view">
			<field name="name">Specialization Details Tree</field>
			<field name="model">kwmaster_specializations</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
					<field name="stream_id"/>
					<field name="kw_id" groups="base.group_no_one"/>
					<field name="active" widget="boolean_toggle" groups="base.group_no_one"/>
				</tree>
			</field>
		</record>
		<!-- END :   explicit Specialization tree view definition -->


		<!-- START : explicit Reference mode definition -->
		<record model="ir.actions.act_window" id="kwemp_reference_mode_master_action_window">
			<field name="name">Reference Mode</field>
			<field name="res_model">kwemp_reference_mode_master</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

		<!--  Reference mode master Tree View  -->
		<record id="kwemp_reference_mode_master_view_tree" model="ir.ui.view">
			<field name="name">Reference Mode Tree</field>
			<field name="model">kwemp_reference_mode_master</field>
			<field name="arch" type="xml">
				<tree create="true" delete="true" editable="top">
					<field name="name"/>
					<field name="kw_id" groups="base.group_no_one"/>
					<field name="active" widget="boolean_toggle"/>
				</tree>
			</field>
		</record>
		<!-- <menuitem name="Reference Mode" id="kwemp_reference_mode_master_menu" parent="kw_master_menu" sequence="11" action="kwemp_reference_mode_master_action_window"  groups="hr.group_hr_manager"/> -->
	</data>
</odoo>
