<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Tree view for "model" -->
        <record id="view_kw_daily_matching_report_tree" model="ir.ui.view">
            <field name="name">view.kw_daily_matching_report.tree</field>
            <field name="model">kw_daily_matching_report</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree string="kw_daily_matching_report_tree" create="false" delete="false" edit="false" import="false">
                    <field name="item" />
                    <field name="count" />
                </tree>
            </field>
        </record>
        
        <!-- Search view for "model" -->
        <record id="view_kw_daily_matching_report_search" model="ir.ui.view">
            <field name="name">view.kw_daily_matching_report.search</field>
            <field name="model">kw_daily_matching_report</field>
            <field name="type">search</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <search string="kw_daily_matching_report_search">
                    <field name="item" />
                </search>
            </field>
        </record>
        
        <!-- Main window action for "model" -->
        <record id="action_kw_daily_matching_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Daily Attendance Compare With Kwantify Report</field>
            <field name="res_model">kw_daily_matching_report</field>
            <field name="view_mode">tree</field>
            <field name="target">current</field>
            <field name="domain">[]</field>
            <field name="context">\{}</field>
            <field name="search_view_id" ref="view_kw_daily_matching_report_search" />
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No record found
                </p>
            </field>
        </record>
    </data>
</odoo>
