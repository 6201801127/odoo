<odoo>
    <data noupdate="1">

        <record id="schedule_kw_daily_employee_attendance_create_each_day" model="ir.cron">
            <field name="name">Daily Attendance Creation </field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doall">1</field>
            <field name="numbercall">-1</field>
            <!-- <field name="nextcall" >2019-11-26 12:59:59</field> -->

            <field name="model_id" ref="model_kw_manual_attendance_hr_wizard"/>
            <field name="code">model.auto_attendance_status_update()</field>
            <field name="state">code</field>          
        </record>


        <record id="schedule_employee_attendance_recompute_each_day" model="ir.cron">
            <field name="name">Daily Attendance Re-computation </field>
            <!-- <field name="active" eval="True" /> -->
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doall">1</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_daily_employee_attendance"/>
            <field name="code">model.recompute_employee_daily_attendance()</field>
            <field name="state">code</field>  
            <field name="active">False</field>          
        </record>
        <!-- Daily Attendance Leave,Tour Status Re-computation -->
        <record id="schedule_employee_attendance_leave_tour_recompute_each_day" model="ir.cron">
            <field name="name">Daily Attendance Leave,Tour Status Re-computation </field>            
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doall">1</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_daily_employee_attendance"/>
            <field name="code">model.update_daily_attendance_leave_tour_status()</field>
            <field name="state">code</field>  
            <field name="active">False</field>          
        </record>

        <record id="schedule_employee_attendance_sync_with_kwantify" model="ir.cron">
            <field name="name">Daily Attendance info Sync with Kwantify </field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="doall">1</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_hr_attendance"/>
            <field name="code">model.sync_attendance_info_kwantify()</field>
            <field name="state">code</field>  
            <field name="active">False</field>          
        </record>

        <!-- Daily Attendance Absentee statements and Pending approvals -->
        <record id="schedule_employee_absentee_statement_pending_approval" model="ir.cron">
            <field name="name">Daily Attendance Absentee Statements and LE Pending approvals</field>            
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doall">0</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_kw_daily_employee_attendance"/>
            <field name="code">model.attendance_reminder_mail_absentee_late_entry()</field>
            <field name="state">code</field>  
            <field name="active">False</field>          
        </record>
        
    </data>
</odoo>