<?xml version="1.0"?>
<odoo>
  <data>
        <record model="ir.ui.view" id="kw_attendance_grace_time_log_tree">
            <field name="name">kw_attendance_grace_time_log.tree</field>
            <field name="model">kw_attendance_grace_time_log</field>           
            <field name="arch" type="xml"> 
                <tree>                  
                    <field name="shift_id"/>
                    <field name="grace_time" widget="float_time" />
                    <field name="effective_from"/>
                    <field name="effective_to"/>      
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_kw_attendance_grace_time_log_window">
            <field name="name">Grace Time Change Log</field>
            <field name="res_model">kw_attendance_grace_time_log</field> 
            <field name="view_mode">tree</field>
            <field name="context">{"create": False,"edit": False}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No record found
                </p>
            </field>
        </record>

     </data>
</odoo>