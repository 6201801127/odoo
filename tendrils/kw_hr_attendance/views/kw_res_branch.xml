<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="res_branch_form" model="ir.ui.view">
            <field name="name">res.branch.form</field>
            <field name="model">kw_res_branch</field>
            <field name="inherit_id" ref="kw_branch_master.kw_res_branch_form" />
            <field name="arch" type="xml">
                <field name="fax" position="after">
                    <field name="default_shift_id" />
                </field>
            </field>
        </record>

        <record id="res_branch_tree" model="ir.ui.view">
            <field name="name">res.branch.tree</field>
            <field name="model">kw_res_branch</field>
            <field name="inherit_id" ref="kw_branch_master.kw_res_branch_tree" />
            <field name="arch" type="xml">
                <field name="telephone_no" position="after">
                    <field name="default_shift_id" />
                </field>
            </field>
        </record>
    </data>
</odoo>
