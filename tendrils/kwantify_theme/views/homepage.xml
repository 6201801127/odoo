        <template id="tendrils_homepage_template" name="Tendrils Homepage" inherit_id="website.homepage">
            <xpath expr="//div[@id='wrap']" position="inside">


               
                    <a href="/" class="navbar-brand logo tendrils_mob">
                        <img class="tendrils_mob_logo" src="/kwantify_theme/static/src/img/tendrils.png" role="img" alt="Tendrils Logo" title="Tendrils logo" />
                    </a>
                

                <section class="kw_homepage">
                    <div class="container-fluid">
                    <div class="row kw_home">
                        <div class="col-xl-5 col-lg-5 col-md-12 col-12 order-2 order-xl-1 order-lg-1">
                            <img src="/kwantify_theme/static/src/img/Mask Group 3.png" class="img-fluid kw_home_banner" role="img" alt="CSM Logo"
                            title="Tendrils"/>
                        </div>
                        <div class="col-xl-7 col-lg-7 col-md-12 col-12 kw_welcome order-1 order-xl-2 order-lg-2">
                            <h1 class="heading">Welcome to <strong>Tendrils</strong></h1>
                            <p class="content">This <b>Enterprise Management Portal </b> ensures smooth functioning of our departments, 
                                makes our everyday tasks easier and integrates information in a seamless manner.</p>

                                <p class="kw_signin">Get started by signing in!</p>
                                <a class="btn signinButton" t-attf-href="/web/login"
                                    t-if="user_id._is_public()"> Sign In <i class="icon-right-arrow"/> </a>
                        </div>
                    </div>
                    </div>
                </section>
                <div class="oe_structure"/>
            </xpath>
        </template>
        
