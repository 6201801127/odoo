<?xml version="1.0" encoding="UTF-8"?>
<!-- Container -->
<templates id="partner_dashboard_template" xml:space="preserve">
    <t t-name="Partner_Dashboard">
        <div class="o_control_panel">
            <ol class="breadcrumb" role="navigation" style="width:30%;">
                <li class="breadcrumb-item active">
                    <p class="font-weight-bold h2 mb-4 mt-2">Partner Dashboard</p>
                </li>
            </ol>
        </div>
        <div class="p-5 container-fluid">
            <div class="row mb-4">
                <div class="col-12">
                    <div class="card partner-card">
                    
                        <!-- <div class="card-body text-center partner-primary-card-body">
                            <span class="rcm-h1 font-weight-bold text-center">Dashboard </span>
                        </div> -->
                        <div class="card-body text-center partner-secondary-card-body">
                            <div id="render_partner_type_container"></div>
                    </div>
                    </div>
                </div>
            </div>
            <div class="row mt-5">
                <div class="col-6">
                    <div class="card partner-card mb-4">
                        <div class="card-body text-center partner-secondary-card-body">
                                <div id="partner_industries_container_graph"></div>
                        </div>
                    </div>
                </div>
               <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center partner-secondary-card-body">
                            <div id="partner_technology_container_graph"></div>
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center partner-secondary-card-body">
                            <div id="partner_geographics_container_graph"></div>
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="card rcm-card mb-4">
                        <div class="card-body text-center partner-secondary-card-body">
                             <div id="partner_services_container_graph"></div>
                        </div>
                    </div>
                </div>
            </div>
            
        </div>
    </t>


</templates>
