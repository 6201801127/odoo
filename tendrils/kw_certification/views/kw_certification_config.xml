<odoo>
  <data>
    <!-- list view of certification Master -->

    <record model="ir.ui.view" id="kw_certification_master_list">
      <field name="name">kw_certification_master_list</field>
      <field name="model">certification_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
            <field name="name"/>
            <field name="oem"/>
            <field name="code"/>
            <field name="certification_fees" invisible="1"/>
            <field name="active" widget="boolean_toggle"/>
        </tree>
      </field>
    </record>
    <!--  form view  -->
    <record id="kw_certification_master_form" model="ir.ui.view">
      <field name="name">kw_certification_master_form</field>
      <field name="model">certification_master</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="certification_fees" invisible="1"/>
                        <field name="active" widget="boolean_toggle"/>
                    </group>
             
                </group>
            </sheet>
        </form>
      </field>
    </record>
    <!--  action  -->
    <record model="ir.actions.act_window" id="kw_certification_master_action_window">
      <field name="name">Certification Master</field>
      <field name="res_model">certification_master</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree</field>
    </record>

        <!-- list view of certification Technology Master -->

    <record model="ir.ui.view" id="kw_certification_master_technology_list">
      <field name="name">kw_certification_technology_list</field>
      <field name="model">kw_certification_technology_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
            <field name="name"/>
            <field name="code"/>
            <field name="active" widget="boolean_toggle"/>
        </tree>
      </field>
    </record>
    <!--  form view  -->
    <record id="kw_certification_technology_master_form" model="ir.ui.view">
      <field name="name">kw_certification_technology_form</field>
      <field name="model">kw_certification_technology_master</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="active" widget="boolean_toggle"/>
                    </group>
             
                </group>
            </sheet>
        </form>
      </field>
    </record>
    <!--  action  -->
    <record model="ir.actions.act_window" id="kw_certification_master_technology_action_window">
      <field name="name">Certification Technology Master</field>
      <field name="res_model">kw_certification_technology_master</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree</field>
    </record>

       <!-- list view of certification Budget Master -->

    <record model="ir.ui.view" id="kw_certification_budget_master_list">
      <field name="name">kw_certification_budget_list</field>
      <field name="model">kw_certification_budget_master</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="certificate_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
          <field name="budget"/>
          <field name="active" widget="boolean_toggle"/>
        </tree>
      </field>
    </record>
    <!--  form view  -->
    <record id="kw_certification_budget_master_form" model="ir.ui.view">
      <field name="name">kw_certification_budget_form</field>
      <field name="model">kw_certification_budget_master</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <group>
                        <field name="certificate_id" options="{'no_open': True,'no_create':True,'no_edit':True}"/>
                        <field name="budget"/>
                        <field name="active" widget="boolean_toggle"/>
                    </group>
             
                </group>
            </sheet>
        </form>
      </field>
    </record>
    <!--  action  -->
    <record model="ir.actions.act_window" id="kw_certification_master_budget_action_window">
      <field name="name">Certification Budget Master</field>
      <field name="res_model">kw_certification_budget_master</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree</field>
    </record>

  </data>
</odoo>