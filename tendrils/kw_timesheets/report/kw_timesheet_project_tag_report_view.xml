<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!-- Tree view for "model" -->
        <record id="kw_timesheet_project_tag_report_tree" model="ir.ui.view">
            <field name="name">view.kw_timesheet_project_tag_report.tree</field>
            <field name="model">kw_timesheet_project_tag_report</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree create="false" edit="false" delete="false" import="false" >
                    <field name="project_name" />
                    <field name="resource_count"/>
                    <field name="estimated_effort" />
                    <field name="actual_timesheet_effort" />
                    <field name="deviation_percentage" />
                    <button name="projec_tag_employee_action" type="object" string="View Details" class="oe_highlight" />
                </tree>
            </field>
        </record>

        
        <!-- Main window action for "model" -->
        <record id="kw_timesheet_project_tag_report_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Timesheet Project Taging Report</field>
            <field name="res_model">kw_timesheet_project_tag_report</field>
            <field name="view_mode">tree</field>
            <field name="target">current</field>
            <field name="domain">[]</field>
        </record>

        <!-- <menuitem name="Timesheet Project Tag Report"
                  action="kw_timesheet_project_tag_report_act_window"
                  id="menu_kw_timesheets_project_tag_report"
                  parent="hr_timesheet.menu_timesheets_reports"
                  groups='hr_timesheet.group_timesheet_manager'
                 /> -->
       
    </data>
</odoo>
