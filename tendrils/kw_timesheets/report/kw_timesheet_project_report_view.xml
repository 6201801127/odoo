<odoo>
    <data>
        <record id="view_kw_timesheet_project_report_tree" model="ir.ui.view">
            <field name="name">Timesheet Project Report View</field>
            <field name="model">kw_timesheet_project_report_view</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree create="false" edit="false" delete="false" import="false">
                    <field name="project_id" invisible="1"/>
                    <field name="from_date"/>
                    <field name="to_date"/>
                    <field name="project_name" />
                    <field name="tagged_resource"/>
                    <field name="no_of_filled" />
                    <button name="action_view_timesheet_details" string="View Details" type="object" class="btn btn-primary" attrs="{'invisible':[('no_of_filled','=',0)]}"/>
                    <field name="no_of_not_filled"/>
                    <button name="action_view_not_filled_employees" string="View Resources" type="object" class="btn btn-primary" attrs="{'invisible':[('no_of_not_filled','=',0)]}"/>
                </tree>
            </field>
        </record>
        
        <record id="kw_timesheet_project_repport_view_view_search" model="ir.ui.view">
            <field name="name">view.kw_timesheet_project_report_view.search</field>
            <field name="model">kw_timesheet_project_report_view</field>
            <field name="arch" type="xml">
                <search string="Project Fill Report">
                    <field name="project_id" string="Project"/>
                </search>
            </field>
        </record>
        
    </data>
</odoo>