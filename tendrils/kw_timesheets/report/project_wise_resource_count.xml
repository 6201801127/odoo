<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>

        <record id="timesheet_kw_project_resource_count_report_action" model="ir.actions.act_window">
			<field name="name">Project wise Report</field>
			<field name="res_model">kw_project_resource_count_report</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
            <field name="domain">['|','|','|','|','|','|',('project_manager_id.user_id', '=', uid),('reviewer.user_id', '=', uid),('sbu_id.user_id', '=', uid),('sbu_id.sbu_master_id.representative_id.parent_id.user_id', '=', uid),('sbu_id.sbu_master_id.employee_id.parent_id.user_id', '=', uid),('sbu_id.sbu_master_id.employee_id.parent_id.parent_id.user_id', '=', uid),('sbu_id.sbu_master_id.employee_id.user_id', '=', uid)]</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No records found...
				</p>
			</field>
		</record>

        <menuitem id="timesheet_kw_project_resource_count_report_menu"
				  name="Project wise Report"
				  action="timesheet_kw_project_resource_count_report_action"
                  parent="hr_timesheet.menu_timesheets_reports"
				  sequence="99"
		/>

		<record id="timesheet_project_wise_resource_action" model="ir.actions.act_window">
			<field name="name">Project Wise Report-(Total)</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">project_wise_resource</field>
			<field name="view_mode">tree,pivot</field>
			<field name="view_type">form</field>
			<field name="domain">['|','|','|','|','|','|',('project_manager_id.user_id', '=', uid),('project_sbu_id.user_id', '=', uid),('project_sbu_id.sbu_master_id.representative_id.parent_id.user_id', '=', uid),('project_sbu_id.sbu_master_id.employee_id.parent_id.user_id', '=', uid),('project_sbu_id.sbu_master_id.employee_id.parent_id.parent_id.user_id', '=', uid),('project_sbu_id.sbu_master_id.employee_id.user_id', '=', uid),('project_name.reviewer_id.user_id', '=', uid)]</field>
			<field name="context">{'search_default_project_name':1,'search_default_sbu_id':1,'search_default_employee_id':1}</field>
		</record>


        <menuitem id="timesheet_project_wise_resource_menu"
				  name="Project Wise Report-(Total)"
				  action="timesheet_project_wise_resource_action"
				  sequence="99"
				  parent="hr_timesheet.menu_timesheets_reports"/>


		<record id="resource_wise_timesheet_project_action" model="ir.actions.act_window">
			<field name="name">Resource Wise Report</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">resource_wise_project</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="domain">['|','|','|','|',('emp_sbu_id.user_id', '=', uid),('emp_sbu_id.sbu_master_id.representative_id.parent_id.user_id', '=', uid),('emp_sbu_id.sbu_master_id.employee_id.parent_id.user_id', '=', uid),('emp_sbu_id.sbu_master_id.employee_id.parent_id.parent_id.user_id', '=', uid),('emp_sbu_id.sbu_master_id.employee_id.user_id', '=', uid)]</field>
		</record>


        <menuitem id="project_wise_resource_timesheet_menu"
				  name="Resource Wise Report"
				  action="resource_wise_timesheet_project_action"
				  parent="hr_timesheet.menu_timesheets_reports"
		/>
		<record id="sbu_project_timesheet_resource_action" model="ir.actions.act_window">
            <field name="name">SBU Resource Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sbu_wise_resource</field>
            <field name="view_mode">tree</field>
            <field name="domain">['|','|','|','|',('sbu_id.representative_id.user_id', '=', uid),('sbu_id.representative_id.parent_id.user_id', '=', uid),('sbu_id.employee_id.parent_id.user_id', '=', uid),('sbu_id.employee_id.parent_id.parent_id.user_id', '=', uid),('sbu_id.employee_id.user_id', '=', uid)]</field>
        </record>

        <menuitem id="sbu_project_timesheet_resource__menu" name="SBU Resource Report" parent="hr_timesheet.menu_timesheets_reports" 
                    action="sbu_project_timesheet_resource_action" sequence="499"
					active='true'/>

		<record id="timesheet_sbu_project_report_action" model="ir.actions.act_window">
			<field name="name">SBU Project Report</field>
			<field name="res_model">project_date_reminder_cron</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="domain">['|','|','|','|',('sbu_id.representative_id.user_id', '=', uid),('sbu_id.representative_id.parent_id.user_id', '=', uid),('sbu_id.employee_id.parent_id.user_id', '=', uid),('sbu_id.employee_id.parent_id.parent_id.user_id', '=', uid),('sbu_id.employee_id.user_id', '=', uid)]</field>
			<field name="search_view_id" ref="kw_resource_management.project_date_reminder_cron_tree_search_view"/>
			<field name="view_id" ref="kw_resource_management.project_date_reminder_cron_tree_view"/>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					No records found...
				</p>
			</field>
		</record>
		<menuitem id="timesheet_sbu_project_report_menu"
					name="SBU Project Report"
					action="timesheet_sbu_project_report_action"
					parent="hr_timesheet.menu_timesheets_reports"
					sequence="500"
		/>

    </data>
</odoo>
