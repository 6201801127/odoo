<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="kw_project_inherit_view" model="ir.ui.view">
            <field name="name">Inherit_project</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project" />
            <field name="arch" type="xml">
                <xpath expr="//sheet/notebook/page/group/field[@name='subtask_project_id']" position="after">

                    <field name="prject_category_id" options="{'no_create': True, 'no_create_edit':True, 'no_open':True}" widget='many2many_tags'/>
                    
                </xpath>
            </field>
        </record>
    </data>
</odoo>