<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="kw_timehseet_payroll_inherit">
			<field name="name">Paroll_inherit</field>
			<field name="model">kw_payroll_monthly_attendance_report</field>
			<field name="inherit_id" ref="payroll_inherit.view_kw_payroll_monthly_attendance_report_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='num_leave_lwop']" position="after">
                    <field name="required_effort" />
                    <field name="num_actual_effort" />
                    <field name="total_effort" />
				</xpath>
				
			</field>
		</record>
    </data>
</odoo>