<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <record id="hr_employee_timesheet_form_inherit" model="ir.ui.view">
            <field name="name">hr.employee.timesheet.form</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr_timesheet.hr_employee_view_form_inherit_timesheet"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='timesheet']" position="inside">
                    <field name="enable_timesheet" />
                </xpath>
            </field>
        </record>

    </data>
</odoo>