        <!-- start : scheduler for timesheet one day update and validate -->
        <record id="kw_timesheet_one_day_update_reminder_scheduler" model="ir.cron">
            <field name="name">Timesheet : One Day Update Scheduler for Employees</field>
            <field name="model_id" ref="model_account_analytic_line"/>
            <field name="code">model.remind_one_day_timesheeet_to_employees()</field>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="priority">3</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="doall">0</field>
            <field name="numbercall">-1</field>
