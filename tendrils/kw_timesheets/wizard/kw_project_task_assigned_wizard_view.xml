<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="project_task_assign_report_wizard_form" model="ir.ui.view">
			<field name="name">project_task_assign_report_wizard form</field>
			<field name="model">project_task_assign_report_wizard</field>
			<field name="arch" type="xml">
				<form>
					<div>
                        <group>
                            <group>
                                <field name="to_date"/>
                            </group>
                        </group>
					</div>
					<footer>
						<button string='Filter' name="project_task_assign_filter" type="object"
						        class="btn font-weight-bold text-white" style="background-color: #108fbb;"/>
						<button string="Cancel" class="btn-secondary" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>

		<record id="project_task_assign_report_wizard_actions" model="ir.actions.act_window">
			<field name="type">ir.actions.act_window</field>
			<field name="name">Project Task assign Filter</field>
			<field name="res_model">project_task_assign_report_wizard</field>
			<field name="view_mode">form</field>
			<field name="view_type">form</field>
			<field name="target">new</field>
		</record>

		<menuitem id="menu_kw_project_task_assign_report_wizard"
		          name="Project Task Assign Report"
		          parent="hr_timesheet.menu_timesheets_reports"
		          action="project_task_assign_report_wizard_actions"/>

 		<!-- <record id="kw_timesheets.menu_kw_project_task_assign_report_wizard" model="ir.ui.menu">
                <field name="groups_id" eval="[(3,ref('kw_timesheets.group_kw_timesheets_report_manager')),(3,ref('hr_timesheet.group_timesheet_manager'))]"/>
            </record> -->

	</data>
</odoo>
