<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="bsscl_tour_da_view_form" model="ir.ui.view">
            <field name="name">BSSCL: Tour_da view form</field>
            <field name="model">bsscl.tour_da</field>
            <field name="arch" type="xml">
                <form string="Tour_da Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="percentage"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="bsscl_tour_da_view_tree_id" model="ir.ui.view">
            <field name="name">BSSCL: Tour_da view tree</field>
            <field name="model">bsscl.tour_da</field>
            <field name="arch" type="xml">
                <tree string="Tour_da Tree">
                    <field name="name"/>
                    <field name="percentage"/>
                </tree>
            </field>
        </record>

        <record id="bsscl_tour_da_action_id" model="ir.actions.act_window">
            <field name="name">Tour Da / यात्रा महंगाई भत्ता </field>
            <field name="res_model">bsscl.tour_da</field>
            <field name="view_mode">tree,form</field>
        </record>
    
    </data>
    

</odoo>
