<odoo>
  <data>

    <record id="professional_tax_view_tree" model="ir.ui.view">
      <field name="name">professional.tax.view.tree</field>
      <field name="model">hr_payslip_prof_tax</field>
      <field name="type">tree</field>
      <field name="arch" type="xml">
        <tree string="Professional Tax" editable="top">
          <field name="state_id"/>
          <field name="gross_from"/>
          <field name="gross_to"/>
          <field name="prof_monthly_deduct"/>
          <field name="prof_last_month_deduct"/>
        </tree>
      </field>
    </record>

    <record id="professional_tax_configuration_action_window" model="ir.actions.act_window">
        <field name="name">Professional Tax</field>
        <field name="res_model">hr_payslip_prof_tax</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem action="professional_tax_configuration_action_window" id="professional_tax_configuration" parent="hr_payroll.menu_hr_payroll_configuration" sequence="49"/>

  </data>
</odoo>