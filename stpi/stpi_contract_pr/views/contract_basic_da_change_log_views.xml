<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_contract_basic_da_change_log_tree" model="ir.ui.view">
        <field name="name">view.contract_basic_da_change_log.tree</field>
        <field name="model">contract_basic_da_change_log</field>
        <field name="arch" type="xml">
            <tree>
                <field name="date"/>
                <field name="wage"/>
                <field name="da"/>
                <field name="basic_da"/>
                <field name="related_year_int"/>
                <field name="related_month_str"/>
            </tree>
        </field>
    </record>

    <record id="view_contract_basic_da_change_log_form" model="ir.ui.view">
        <field name="name">view.contract_basic_da_change_log.form</field>
        <field name="model">contract_basic_da_change_log</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="date"/>
                            <field name="wage"/>
                            <field name="da"/>
                        </group>
                        <group>
                            <field name="basic_da"/>
                            <field name="related_year_int"/>
                            <field name="related_month_str"/>
                        </group>
                    </group>

                </sheet>
            </form>
        </field>
    </record>

</odoo>