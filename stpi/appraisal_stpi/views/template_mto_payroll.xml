<odoo>
    <data>
        <record model="ir.ui.view" id="pay_level_form_inherit">
            <field name="name">hr.payslip.paylevel.inherit</field>
            <field name="model">hr.payslip.paylevel</field>
            <field name="inherit_id" ref="stpi_contract_pr.pay_level_form_view_id"/>
            <field name="arch" type="xml">
                <xpath expr = "//group/group[2]" position="after">
                    <group string="Appraisal">
                        <field name = "template_id" invisible="1"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>

