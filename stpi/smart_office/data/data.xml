        <!-- Start : Userwise stages -->

        <record id="userwise_corres_create_self" model="correspondence.userwise.stage">
            <field name="name">Correspondence Created by Self</field>
            <field name="code">corres_create_self</field>
        </record>

        <record id="userwise_corres_forward_by_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence Forwarded by Someone</field>
            <field name="code">corres_forward_by_someone</field>
        </record>

        <record id="userwise_corres_forward_to_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence Forwarded to Someone</field>
            <field name="code">corres_forward_to_someone</field>
        </record>

        <record id="userwise_corres_cc_by_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence CC by Someone</field>
            <field name="code">corres_cc_by_someone</field>
        </record>

        <record id="userwise_corres_transferred_by_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence Transferred by Someone</field>
            <field name="code">corres_transferred_by_someone</field>
        </record>

        <record id="userwise_corres_transferred_to_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence Transferred to Someone</field>
            <field name="code">corres_transferred_to_someone</field>
        </record>

        <record id="userwise_corres_pulled_from_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence Pulled from Someone's Inbox</field>
            <field name="code">corres_pulled_from_someone</field>
        </record>

        <record id="userwise_corres_pulled_to_someone" model="correspondence.userwise.stage">
            <field name="name">Correspondence Pulled to Someone's Inbox</field>
            <field name="code">corres_pulled_to_someone</field>
        </record>

