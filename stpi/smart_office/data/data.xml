        <!--&lt;!&ndash;<field name="implied_ids" eval="[(4, ref('group_dms_user'))]" />&ndash;&gt;-->
        <!--&lt;!&ndash;<field name="category_id" ref="category_dms_security" />&ndash;&gt;-->
        <!--<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />-->
        <!--</record>-->

        <record id="access_group_01_demo" model="muk_security.access_groups">
            <field name="name">Manager Group (Read)</field>
            <field name="perm_unlink" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="groups" eval="[(6, 0, [ref('base.group_system')])]"/>
        </record>

        <record id="smart_office_directory_root" model="muk_dms.directory">
            <field name="name">Root</field>
            <field name="is_root_directory" eval="True"/>
            <field name="root_storage" ref="smart_office.smart_office_storage_access"/>
            <!-- <field name="alias_id" ref="mail_alias_equipment"/> -->
            <!-- <field name="groups" eval="[(6, 0, [ref('smart_office.access_group_01_demo')])]"/> -->
            <field name="category" ref="smart_office.category_01_demo"/>
            <field name="tags" eval="[(6, 0, [ref('smart_office.tag_07_demo')])]"/>
        </record>

        <record id="smart_office_directory" model="muk_dms.directory">
            <field name="name">Incoming Files</field>
            <field name="is_root_directory" eval="True"/>
            <field name="root_storage" ref="smart_office.smart_office_storage_access"/>
            <!-- <field name="alias_id" ref="mail_alias_equipment"/> -->
            <!-- <field name="groups" eval="[(6, 0, [ref('smart_office.access_group_01_demo')])]"/> -->
            <field name="category" ref="smart_office.category_01_demo"/>
            <field name="tags" eval="[(6, 0, [ref('smart_office.tag_07_demo')])]"/>
        </record>

        <record id="corres_stage_created" model="smart_office.correspondence.stage">
            <field name="name">Correspondence Created</field>
            <field name="code">corres_create</field>
        </record>

        <record id="corres_stage_forwarded" model="smart_office.correspondence.stage">
            <field name="name">Correspondence Forwarded</field>
            <field name="code">corres_forward</field>
        </record>

        <record id="corres_stage_cc_forwarded" model="smart_office.correspondence.stage">
            <field name="name">CC</field>
            <field name="code">cc</field>
        </record>

        <record id="corres_stage_attach_file_from_create_file" model="smart_office.correspondence.stage">
            <field name="name">Attached to File</field>
            <field name="code">attach_from_create_file</field>
        </record>

        <!-- <record id="corres_stage_attach_file_from_existing_file" model="smart_office.correspondence.stage">
            <field name="name">Attach File from Existing File</field>
            <field name="code">attach_from_existing_file</field>
        </record> -->

        <record id="corres_stage_file_removed" model="smart_office.correspondence.stage">
            <field name="name">Removed From File</field>
            <field name="code">file_removed</field>
        </record>

        <record id="corres_stage_transferred" model="smart_office.correspondence.stage">
            <field name="name">Correspondence Transferred</field>
            <field name="code">corres_transfer</field>
        </record>

        <record id="corres_stage_pull_inbox" model="smart_office.correspondence.stage">
            <field name="name">Pulled To Inbox</field>
            <field name="code">corres_pull_inbox</field>
        </record>

        <!-- <record id="corres_stage_send_back" model="smart_office.correspondence.stage">
            <field name="name">Send Back</field>
            <field name="code">corres_send_back</field>
        </record> -->

