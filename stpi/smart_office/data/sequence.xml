<odoo>
    <data>
        <record id="sequence_letter_name" model="ir.sequence">
            <field name="name">Letter</field>
            <field name="code">muk.dms.letter</field>
            <field name="prefix">Letter/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="sequence_smart_office_noting" model="ir.sequence">
            <field name="name">Sequence Number Of Noting</field>
            <field name="code">smart_office_noting_sequence</field>
            <field name="implementation">no_gap</field>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>