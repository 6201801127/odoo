<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_smart_office_file_tracking_tree" model="ir.ui.view">
            <field name="name">view.smart_office.file.tracking.tree</field>
            <field name="model">smart_office.file.tracking</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree limit="10" default_order="action_time desc">
                    <field name="action_time" />
                    <field name="action_by_user_id" />
                    <field name="action_stage_id" />
                    <field name="previous_owner_user_id"/>
                    <field name="action_to_user_id" />
                    <field name="remark" />
                </tree>
            </field>
        </record>

        <record id="view_smart_office_file_tracking_form" model="ir.ui.view">
            <field name="name">view.smart_office.file.tracking.form</field>
            <field name="model">smart_office.file.tracking</field>
            <field name="type">form</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="action_date" />
                                <field name="action_by_user_id" />
                                <field name="action_stage_id" />
                            </group>
                            <group>
                                <field name="action_to_user_id" />
                            </group>
                            <field name="remark" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_smart_office_file_tracking_search" model="ir.ui.view">
            <field name="name">view.smart_office.file.tracking.search</field>
            <field name="model">smart_office.file.tracking</field>
            <field name="type">search</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <search>
                    <field name="action_by_user_id" />
                    <field name="action_stage_id" />
                    <field name="action_to_user_id"/>
                </search>
            </field>
        </record>

